   <xsd:element name="ComplianceDocumentPackage">
      <xsd:complexType>
         <xsd:sequence>
            <xsd:element name="DocID" minOccurs="0">
               <xsd:complexType>
                  <xsd:attribute name="doc" type="comp:ComplianceDocumentTagNR" fixed="NRCCLTOE"/>
                  <xsd:attribute name="docType" type="comp:ComplianceDocumentTypeNR" fixed="CERTIFICATE OF COMPLIANCE NR"/>
                  <xsd:attribute name="docTitle" type="comp:ComplianceDocumentTitleNR" fixed="Outdoor Lighting"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Payload" type="comp:Payload"/>
            <xsd:element name="DocumentData">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="Header" type="comp:HeaderNRCC"/>
                     <xsd:element ref="nRCCLTO01E"/>
                     <xsd:element name="DeclarationsBlock" type="comp:DeclarationsNRCC" maxOccurs="unbounded"/>
                     <xsd:element name="Section_MandatoryMeasureBlock" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">
                              <d:b>
                                 <d:i>Outdoor Lighting Mandatory Measures:</d:i>
                              </d:b>
                           </xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If NRCC-ELC-E is included in scope And 
A05_RequestedMMB == true
this Mandatory Measures Noteblock is required 
Else it is not required and nothing is captured or displayed
End If</xsd:documentation>
                           <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
                        </xsd:annotation>
                        <xsd:complexType>
                           <xsd:sequence>
                              <xsd:element name="MM_BeforeNote1" minOccurs="0">
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">
                                       <d:b>
                                          <d:i>2019 Nonresidential Energy Standards Compliance (Title 24, Part 6)</d:i>
                                       </d:b>
                                    </xsd:documentation>
                                 </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="MM_BeginNote1" minOccurs="0">
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">
                                       <d:i>The following mandatory measures have been identified as applicable to the project and must be met in order to demonstrate compliance with Title 24, Part 6</d:i>
                                    </xsd:documentation>
                                 </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="MM110_9_MeasureApplies" type="comp:MeasureApplies">
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">
                                       <d:b>
                                          <d:t24refnolink/>110.9 OUTDOOR LIGHTING CONTROLS AND COMPONENTS</d:b>
                                    </xsd:documentation>
                                    <xsd:documentation source="AdditionalRequirements">ALL LIGHTING CONTROL DEVICES AND SYSTEMS, AND ALL LIGHT SOURCES SHALL MEET THE APPLICABLE REQUIREMENTS OF 
                                       <d:t24refnolink/>110.9.</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">Result = true for all projects that include outdoor lighting requiring the NRCC-LTO and FieldText and AdditionalRequirements static text is displayed in the report</xsd:documentation>
                                 </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="MM130_0_MeasureApplies" type="comp:MeasureApplies">
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">
                                       <d:b>
                                          <d:t24refnolink/>130.0 GENERAL LUMINAIRE REQUIREMENTS</d:b>
                                    </xsd:documentation>
                                    <xsd:documentation source="AdditionalRequirements">ALL LUMINAIRES SHALL BE FACTORY-LABELLED PER 
                                       <d:t24refnolink/>130.0(c).  
                                       <d:line1/>ENERGY MANAGEMENT CONTROL SYSTEMS (EMCS) SHALL MEET REQUIREMENTS OF 
                                       <d:t24refnolink/>130.0(e).</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">Result = true for all projects that include outdoor lighting requiring the NRCC-LTO and FieldText and AdditionalRequirements static text is displayed in the report</xsd:documentation>
                                 </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="MM130_2b_MeasureApplies" type="comp:MeasureApplies">
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">
                                       <d:b>
                                          <d:t24refnolink/>130.2(b) LUMINAIRE CUTOFF REQUIREMENTS</d:b>
                                    </xsd:documentation>
                                    <xsd:documentation source="AdditionalRequirements">ALL OUTDOOR LUMINAIRES WITH INITIAL LUMINAIRE LUMENS 
                                       <d:gte/>6,200 SHALL COMPLY WITH THE BACKLIGHT, UPLIGHT AND GLARE (BUG) REQUIREMENTS OF TITLE 24, PART 11, SECTION 5.106.8.</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">If  any row has F09_LuminairePowerCutoffCompliance == CutoffGTE6200Lumens 
result = true and FieldText and AdditionalRequirements static text is displayed in the report
Else
result = false and nothing is displayed in the report
End If</xsd:documentation>
                                 </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="MM130_2c_MeasureApplies" type="comp:MeasureApplies">
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">
                                       <d:b>
                                          <d:t24refnolink/>130.2(c) CONTROLS FOR OUTDOOR LIGHTING</d:b>
                                    </xsd:documentation>
                                    <xsd:documentation source="AdditionalRequirements">ALL OUTDOOR LIGHTING SHALL BE INDEPENDENTLY CONTROLLED FROM OTHER ELECTRICAL LOADS AND SHALL HAVE THE FOLLOWING FEATURES:</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">If MM130_2c1_MeasureApplies Or MM130_2c2_MeasureApplies Or MM130_2c3_MeasureApplies == true  
this static text is displayed once in the report as a heading for these measures
Else
this static text is not displayed in the report
End If</xsd:documentation>
                                 </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="MM130_2c1_MeasureApplies" type="comp:MeasureApplies">
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText"/>
                                    <xsd:documentation source="AdditionalRequirements">1. AUTOMATICALLY TURNS OFF OUTDOOR LIGHTING WHEN DAYLIGHT IS AVAILABLE</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">If  any row has H02_ShutOffControlsOutdoors == AstronomicalTimer Or Photocontrol 
result = true and FieldText and AdditionalRequirements static text is displayed in the report
Else
result = false and nothing is displayed in the report
End If</xsd:documentation>
                                 </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="MM130_2c2_MeasureApplies" type="comp:MeasureApplies">
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText"/>
                                    <xsd:documentation source="AdditionalRequirements">2. AUTOMATIC SCHEDULING CONTROLS 
                                       <d:l t="A">
                                          <d:list>CAPABLE OF REDUCING LIGHTING POWER AT LEAST 50% AND NO MORE THAN 90% AND SEPARATELY CAPABLE OF TURNING LIGHTING OFF DURING UNOCCUPIED PERIODS</d:list>
                                          <d:list>THAT ALLOW SCHEDULING OF AT LEAST TWO NIGHTTIME PERIODS WITH INDEPENDENT LIGHTING LEVELS (MAY INCLUDE OVERRIDE FOR NO MORE THAN 2 HOURS)</d:list>
                                          <d:list>ACCEPTANCE TESTS SHALL VERIFY SCHEDULED OCCUPIED AND UNOCCUPIED</d:list>
                                          <d:list>AUTOMATIC SCHEDULING CONTROLS SHALL BE INSTALLED FOR ALL OUTDOOR LIGHTING. </d:list>
                                       </d:l>
                                    </xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">If  any row has H03_AutoScheduleControlsOutdoors == AutoSchedule Or EMCS
result = true and FieldText and AdditionalRequirements static text is displayed in the report
Else
result = false and nothing is displayed in the report
End If</xsd:documentation>
                                 </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="MM130_2c3_MeasureApplies" type="comp:MeasureApplies">
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText"/>
                                    <xsd:documentation source="AdditionalRequirements">3. MOTION SENSING CONTROLS 
                                       <d:l t="A">
                                          <d:list>CAPABLE OF REDUCING LIGHTING POWER AT LEAST 50% AND NO MORE THAN 90% AND SEPARATELY CAPABLE OF TURNING LIGHTING OFF DURING UNOCCUPIED PERIODS</d:list>
                                          <d:list> CAPABLE OF DIMMING OR TURNING OFF LIGHTING NO LONGER THAN 15 MINUTES AFTER AREA IS VACATED AND TURNING LIGHTING ON WHEN AREA BECOMES OCCUPIED </d:list>
                                          <d:list> SINGLE SENSORS CAN CONTROL NO MORE THAN 1,500 WATTS OF LIGHTING POWER</d:list>
                                          <d:list>SHALL BE INSTALLED FOR THE FOLLOWING AND MAY BE INSTALLED FOR OTHER OUTDOOR LIGHTING AND IN COMBINATION WITH OTHER OUTDOOR LIGHTING CONTROLS: </d:list>
                                       </d:l>
                                       <d:line2/>
                                       <d:tab1/>(i) OUTDOOR LUMINAIRES OTHER THAN BUILDING FACADE, ORNAMENTAL HARDSCAPE, OUTDOOR DINING OR OUTDOOR SALES FRONTAGE LIGHTING, WHERE THE BOTTOM OF THE LUMINAIRE IS MOUNTED 24 FEET OR LESS ABOVE GRADE 
                                       <d:line1/>(ii)WALL MOUNTED LUMINAIRES INSTALLED FOR BUILDING FACADE, ORNAMENTAL HARDSCAPE, OUTDOOR DINING LIGHTING THAT HAVE A BILATERALLY SYMMETRIC DISTRIBUTION (AS DESCRIBED IN THE IES HANDBOOK) MOUNTED 24 FEET OR LESS ABOVE GRADE</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">If  any row has H04_MotionSensorCompliance == MotionSensorCompliant
result = true and FieldText and AdditionalRequirements static text is displayed in the report
Else
result = false and nothing is displayed in the report
End If</xsd:documentation>
                                 </xsd:annotation>
                              </xsd:element>
                           </xsd:sequence>
                        </xsd:complexType>
                     </xsd:element>
                     <xsd:element name="Section_DwellUnitMandatoryMeasureBlock" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">
                              <d:b>
                                 <d:i>Dwelling Unit and Guest Room Outdoor Lighting Mandatory Measures:</d:i>
                              </d:b>
                           </xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If NRCC-LTIOE is included in scope And 
A05_RequestedMMB == true And 
If A06_WholeBuildingOccupancy == HighRiseResidential Or HotelMotel 
this Mandatory Measures Noteblock is required 
 
Else it is not required and nothing is captured or displayed
End If</xsd:documentation>
                           <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
                        </xsd:annotation>
                        <xsd:complexType>
                           <xsd:sequence>
                              <xsd:element name="DuMM_BeforeNote1" minOccurs="0">
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">
                                       <d:b>
                                          <d:i>2019 Nonresidential Energy Standards Compliance (Title 24, Part 6, Ch.1)</d:i>
                                       </d:b>
                                    </xsd:documentation>
                                 </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="DuMM_BeginNote1" minOccurs="0">
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">
                                       <d:i>The following mandatory measures have been identified as applicable to the project and must be met in order to demonstrate compliance with Title 24, Part 6</d:i>
                                    </xsd:documentation>
                                 </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="DUMM130_0b_MeasureApplies" type="comp:MeasureApplies">
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">
                                       <d:b>
                                          <d:t24refnolink/>130.0(b) AREAS REQUIRING RESIDENTIAL LIGHTING STANDARDS </d:b>
                                    </xsd:documentation>
                                    <xsd:documentation source="AdditionalRequirements">LIGHTING FOR HIGH-RISE RESIDENTIAL DWELLING UNITS, HOTEL/MOTEL GUEST ROOMS, DORMITORY AND SENIOR HOUSING DWELLING ACCOMMODATIONS, AND FIRE STATION DWELLING AREAS SHALL MEET REQUIREMENTS OF 
                                       <d:t24refnolink/>150.0(k). THIS INCLUDES OUTDOOR LIGHTING ATTACHED TO HIGH-RISE RESIDENTIAL OR HOTEL/MOTEL BUILDINGS AND SEPARATELY CONTROLLED FROM INSIDE A DWELLING UNIT OR GUEST ROOM.</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">If A06_WholeBuildingOccupancy == HighRiseResidential Or 
A06_WholeBuildingOccupancy == HotelMotel 
If result = true  FieldText and AdditionalRequirements static text is displayed in the report
Else result = false nothing is displayed in the report
End if</xsd:documentation>
                                 </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="DUMM150_0k1A_MeasureApplies" type="comp:MeasureApplies">
                                 <xsd:annotation>
