                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Thermal Performance</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Hframed04b_Status == ExistingAlteration And Hframed10_ConstructionSpecificationMethod == UFactor 
 If Hframed04a_TopLevelOccupancy == OtherOccupancy Then 
 If Hframed07_PartitionFramingMaterial == WoodFraming Or Wood0_625Gypsum result = 0.110; 
 Else If Hframed07_PartitionFramingMaterial == Metal result = 0.217; 
End If 
End If
 
Else If Hframed04b_Status == New Then 
If Hframed06_WallType == DemisingWall 
If Hframed07_PartitionFramingMaterial == WoodFraming Or Wood0_625Gypsum result = 0.099; 
Else If Hframed07_PartitionFramingMaterial == Metal result = 0.151;
End if  
 
Else If Hframed06_WallType == ExteriorWall 
Switch (Hframed04b_TopLevelOccupancy) 
case  Nonresidential: 
If Hframed07_PartitionFramingMaterial == WoodFraming Or Wood0_625Gypsum 
If A03_ClimateZoneNumber == 1 result = 0.095; 
Else  If A03_ClimateZoneNumber == 2, 4, 9, 10, 12, 13, 14, Or 16 result = 0.059; 
Else If A03_ClimateZoneNumber == 3, 6, Or 7 result = 0.110; 
Else If A03_ClimateZoneNumber == 5 Or 8 result = 0.102; 
Else If A03_ClimateZoneNumber == 11 result = 0.045;
Else If A03_ClimateZoneNumber == 11 result = 0.042; 
End If       
Else If Hframed07_PartitionFramingMaterial == Metal  
If A03_ClimateZoneNumber == 1, 6 Or 7 result = 0.069; 
Else If A03_ClimateZoneNumber == 2, 4, 5, Or  InRange(8, 16) result = 0.062; 
Else If A03_ClimateZoneNumber == 3 result = 0.082; 
End If
End If</xsd:documentation>
