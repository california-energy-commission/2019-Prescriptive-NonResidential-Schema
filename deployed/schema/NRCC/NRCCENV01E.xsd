                                                      <xsd:documentation source="FieldText">Net Area<d:sup>2</d:sup> ft<d:sup>2</d:sup>
                                                      </xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Hspandrel01_UseInAreaWeightedAveUfactor == true, user input
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Hspandrel11_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Net Area<d:sup>2</d:sup> ft<d:sup>2</d:sup>
                                                      </xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Hspandrel08_ConstructionSpecificationMethod == RValue  Or Hspandrel01_UseInAreaWeightedAveUfactor == false result = BlankGrayedOut stored in NotApplicableMessage;
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
