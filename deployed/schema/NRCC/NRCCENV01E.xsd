                                                <xsd:element name="Kskyperf10c_VT_Limit" type="com:VT_Limit">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Product Performance</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Kskyperf06_TopLevelOccupancy == Nonresidential
Switch (Kskyperf05_FenestrationProductType) 
case SkylightGlassCurb:  result = 0.49; case SkylightGlassDeck:  result = 0.49;  case SkylightPlasticCurb:  result = 0.64;  case SkylightTubular: result = 0.38; 
End Switch
 
Else If Kskyperf06_TopLevelOccupancy == ResidentialNR 
Switch (Kskyperf05_FenestrationProductType) 
case SkylightGlassCurb:  result = 0.49; case SkylightGlassDeck:  result = 0.49;  case SkylightPlasticCurb: result = 0.64
End Switch
End If;</xsd:documentation>
