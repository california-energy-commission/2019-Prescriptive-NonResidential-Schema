                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Frame Material &amp;amp; Thickness</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">If Hconcrete04_UfactorSpecSource == JA4Tables, this row is grayed out; 
Else If  Hconcrete04_UfactorSpecSource == JA4Other Or ApprovedSoftware user enters text
End If</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
