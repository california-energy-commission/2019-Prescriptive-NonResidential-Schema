                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Fspan11_ContinuousInsulationNR">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Continuous Insulation per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan08_UfactorSpecSource == JA4Other Or ApprovedSoftware this row is grayed out; 
 
Else If Fspan08_UfactorSpecSource == JA4Tables user selects from list: 
R0ci display term None, 
R4ci display term R-4, 
R6ci display term R-6, 
R8ci display term R-8, 
R10ci display term R-10, 
R12ci display term R-12, 
R15ci display term R-15, 
R20ci display term R-20, 
R25ci display term R-25, 
R30ci display term R-30,</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="env:ContinuousInsulationNR">
                                                         <xsd:enumeration value="R0ci"/>
                                                         <xsd:enumeration value="R10ci"/>
                                                         <xsd:enumeration value="R12ci"/>
                                                         <xsd:enumeration value="R15ci"/>
                                                         <xsd:enumeration value="R20ci"/>
                                                         <xsd:enumeration value="R25ci"/>
                                                         <xsd:enumeration value="R30ci"/>
                                                         <xsd:enumeration value="R4ci"/>
                                                         <xsd:enumeration value="R6ci"/>
                                                         <xsd:enumeration value="R8ci"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                                <xsd:element name="Fspan11_ContinuousInsulationOther" type="env:ContinuousInsulationOther">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Continuous Insulation per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan08_UfactorSpecSource == JA4Tables, this row is grayed out; 
 
Else If Fspan08_UfactorSpecSource == JA4Other Or ApprovedSoftware user enters text</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fspan11_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText"/>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan04_Status == ExistingAlteration And  Fspan05_RoofInsulationException  != NoExceptionApplies result = BlankGrayedOut</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Fspan12_ConstructionSpecificationMethod">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Thermal Performance Unit</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan04_Status == ExistingAlteration And Fspan01_UseInAreaWeightedAveUfactor  == false list RValue display term R-value, UFactor display term U-factor ; 
 
Else result = UFactor and cannot be changed by user</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="env:ConstructionSpecificationMethod">
                                                         <xsd:enumeration value="RValue"/>
                                                         <xsd:enumeration value="UFactor"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                                <xsd:element name="Fspan12_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText"/>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan04_Status == ExistingAlteration And  Fspan05_RoofInsulationException  != NoExceptionApplies result = BlankGrayedOut</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Fspan13_InsulationUFactorLimit" type="env:InsulationUFactorLimit">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Thermal Performance</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan04_Status == New 
 
Switch (Fspan06_TopLevelOccupancy) 
case  Nonresidential: 
If A03_ClimateZoneNumber InRange (1, 5) Or InRange(9,16) result = 0.034; 
Else If A03_ClimateZoneNumber InRange (6, 8) ) result = 0.049; 
End If 
 
case ResidentialNR:  
If A03_ClimateZoneNumber == 1, 2, 4 Or  InRange (8, 16) result = 0.028; 
Else If A03_ClimateZoneNumber == 3, 5 Or 6 result = 0.034; 
Else result = 0.039; 
End If 
 
case RelocableSchoolBuildingAllZones:  result = 0.034; 
End Switch 
 
Else If Fspan04_Status == ExistingAlteration And Fspan12_ConstructionSpecificationMethod == UFactor 
 
If Fspan06_TopLevelOccupancy == Nonresidential Or RelocableSchoolBuildingAllZones
If A03_ClimateZoneNumber == 1 Or InRange(3,9)  result =  0.082; 
Else If A03_ClimateZoneNumber == 2 Or InRange(10,16) result = 0.055; 
End if
 
Else If Fspan06_TopLevelOccupancy == ResidentialNR result = 0.055;
End If 
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fspan13_InsulationRValueLimit" type="env:InsulationRValueLimit">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Thermal Performance</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan04_Status == ExistingAlteration And Fspan12_ConstructionSpecificationMethod == RValue 
 
If Fspan06_TopLevelOccupancy) == Nonresidential Or RelocableSchoolBuildingAllZones 
If A03_ClimateZoneNumber == 1 Or InRange(3,9) result =  R8; 
Else If A03_ClimateZoneNumber == 2 Or InRange(10,16) result = R14;
End If  
 
Else If Fspan06_TopLevelOccupancy == ResidentialNR result = R14;
End If 
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fspan13_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText"/>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan04_Status == ExistingAlteration And  Fspan05_RoofInsulationException  != NoExceptionApplies result = BlankGrayedOut</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="Fspan14UF">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">U-factor per Design</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:choice>
                                                      <xsd:element name="Fspan14_NotApplicableMessage">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText"/>
                                                            <xsd:documentation source="CalculationsAndRules">If Fspan04_Status == ExistingAlteration And  Fspan05_RoofInsulationException  != NoExceptionApplies Or If Fspan12_ConstructionSpecificationMethod == RValue result = BlankGrayedOut stored in NotApplicableMessage and all fields in Fspan14 are grayed out;</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="comp:NotApplicableMessage">
                                                               <xsd:enumeration value="BlankGrayedOut"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                      <xsd:element name="Fspan14JA4">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">per JA4</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">If Fspan08_UfactorSpecSource == JA4Tables, result is stored in one of these choices from Table JA4.2.6</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:complexType>
                                                            <xsd:sequence>
                                                               <xsd:choice maxOccurs="1">
                                                                  <xsd:element name="Fspan14_UfactorJA426FireProof" type="comp:UfactorJA426FireProof">
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText"/>
                                                                        <xsd:documentation source="CalculationsAndRules">If Fspan09_FireProofing == FireProofing 
 
If Fspan10_ConcreteThickness == None 
Switch (Fspan11_ContinuousInsulationNR) 
case R0: result = 0.348;  
case R4:  result = 0.145;  
case R6: result = 0.113;   
case R8: result = 0.092;   
case R10: result = 0.078 ;  
case R12: result = 0.067 ;  
case R15: result = 0.056 ;  
case R20: result = 0.044 ;  
case R25: result = 0.036  ;  
case R30: result =  0.030; 
End Switch
 
Else If Fspan10_ConcreteThickness == 2In 
Switch (Fspan11_ContinuousInsulationNR) 
case R0: result = 0.324;  
case R4:  result = 0.141;  
case R6: result = 0.110;   
case R8: result = 0.090;   
case R10: result = 0.076 ;  
case R12: result = 0.066 ;  
case R15: result = 0.055 ;  
case R20: result = 0.043 ;  
case R25: result = 0.036  ;  
case R30: result =  0.030; 
End Switch
 
Else If Fspan10_ConcreteThickness == 4In 
Switch (Fspan11_ContinuousInsulationNR) 
case R0: result = 0.302;  
case R4:  result = 0.137;  
case R6: result = 0.107;   
case R8: result = 0.088;   
case R10: result = 0.075 ;  
case R12: result = 0.065;  
case R15: result = 0.055 ;  
case R20: result = 0.043 ;  
case R25: result = 0.035  ;  
case R30: result =  0.030; 
End Switch
 
 Else If Fspan10_ConcreteThickness == 6In 
Switch (Fspan11_ContinuousInsulationNR) 
case R0: result = 0.283;  
case R4:  result = 0.133;  
case R6: result = 0.105;   
case R8: result = 0.087;   
case R10: result = 0.074 ;  
case R12: result = 0.064;  
case R15: result = 0.054 ;  
case R20: result = 0.042 ;  
case R25: result = 0.035  ;  
case R30: result =  0.030;
End Switch
End If
End If</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Fspan14_UfactorJA426NoFireProof" type="comp:UfactorJA426NoFireProof">
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText"/>
                                                                        <xsd:documentation source="CalculationsAndRules">If Fspan09_FireProofing == NoFireProofing 
 
If Fspan10_ConcreteThickness == None 
Switch (Fspan11_ContinuousInsulationNR) 
case R0: result = 0.503;  
case R4:  result = 0.167;  
case R6: result = 0.125;   
case R8: result = 0.100;   
case R10: result = 0.083 ;  
case R12: result = 0.071;  
case R15: result = 0.059;  
case R20: result = 0.045;  
case R25: result = 0.037;  
case R30: result =  0.031;
End Switch 
 
Else If Fspan10_ConcreteThickness == 2In 
Switch (Fspan11_ContinuousInsulationNR) 
case R0: result = 0.452;  
case R4:  result = 0.161;  
case R6: result = 0.122;   
case R8: result = 0.098;   
case R10: result = 0.082;  
case R12: result = 0.070;  
case R15: result = 0.058;  
case R20: result = 0.045;  
case R25: result = 0.037  ;  
case R30: result =  0.031;
End Switch  
 
Else If Fspan10_ConcreteThickness == 4In 
Switch (Fspan11_ContinuousInsulationNR) 
case R0: result = 0.412;  
case R4:  result = 0.156;  
case R6: result = 0.119;   
case R8: result = 0.096;   
case R10: result = 0.080;  
case R12: result = 0.069;  
case R15: result = 0.057;  
case R20: result = 0.045;  
case R25: result = 0.036;  
case R30: result =  0.031; 
End Switch  
 
Else If Fspan10_ConcreteThickness == 6In 
Switch (Fspan11_ContinuousInsulationNR) 
case R0: result = 0.377;  
case R4:  result = 0.150;  
case R6: result = 0.116;   
case R8: result = 0.094;   
case R10: result = 0.079;  
case R12: result = 0.068;  
case R15: result = 0.057;  
case R20: result = 0.044;  
case R25: result = 0.036;  
case R30: result =  0.031;
End Switch 
End If
End If</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                               </xsd:choice>
                                                            </xsd:sequence>
                                                         </xsd:complexType>
                                                      </xsd:element>
                                                      <xsd:element name="Fspan14_UfactorJA4Other" type="comp:UfactorJA4Other">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">per Software/ Other</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">If Fspan08_UfactorSpecSource == JA4Other Or  ApprovedSoftware, user input value</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                   </xsd:choice>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Fspan15_RoofNetArea" type="comp:RoofNetArea">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Net Area<d:sup>3</d:sup> ft<d:sup>2</d:sup>
                                                      </xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan01_UseInAreaWeightedAveUfactor == true, user input</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fspan15_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Net Area<d:sup>3</d:sup> ft<d:sup>2</d:sup>
                                                      </xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan04_Status == ExistingAlteration And  Fspan05_RoofInsulationException  != NoExceptionApplies Or If Fspan12_ConstructionSpecificationMethod == RValue Or If Fspan01_UseInAreaWeightedAveUfactor == false result = BlankGrayedOut stored in NotApplicableMessage;</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="FspanFootnote1" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                                 <xsd:documentation source="AdditionalRequirements">
                                    <d:i><d:sup>1</d:sup>FOOTNOTES: If any individual assembly is non-compliant, assemblies may show compliance using an area-weighted calculation.  Metal building roofs may not be combined with other roof types.  The area-weighted compliance option is not available for alterations demonstrating compliance with R-values in 
                                       <d:t24table/>Table 141.0-C.</d:i>
                                 </xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FspanFootnote2" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                                 <xsd:documentation source="AdditionalRequirements">
                                    <d:i><d:sup>2</d:sup> If "R-value" is shown in cell 13 as the Thermal Performance Unit, the R-value shown here is for continuous insulation per 
                                       <d:t24table/>Table 141.0-C.</d:i>
                                 </xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FspanFootnote3" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                                 <xsd:documentation source="AdditionalRequirements">
                                    <d:i><d:sup>3</d:sup> Roof area minus any fenestration/ skylight area </d:i>
                                 </xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Fmetalp" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>Metal Panel Assemblies</d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if F01Metal_RoofTypeApplies == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Fmetalp01_UseInAreaWeightedAveUfactor" type="comp:UseInAreaWeightedAveUfactor">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Include Metal Panel Roof Assemblies in Area-Weighted Average U-factor Calculation<d:sup>1</d:sup>
                                 </xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If checked result = true; Else result = false.  
 
 If result == true   D03_ExceptionalConditionMessageNRCCENVE += RoofAreaWeightedAveInF</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="TableFmetalp">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Fmetalp02_RoofAssemblyName" type="env:RoofAssemblyName">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Tag/Plan Detail ID</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Fmetalp03_RoofDescription" type="env:RoofDescription">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Name/Description</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Fmetalp04_Status">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Status</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from list: New display term New, ExistingAlteration display term Altered</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="com:Status">
                                                      <xsd:enumeration value="ExistingAlteration"/>
                                                      <xsd:enumeration value="New"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Fmetalp05_RoofInsulationException" type="comp:RoofInsulationException">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Exception to Roof Insulation Requirements in 
                                                         <d:t24ref h="141.0(b)2Biii"/> (Alts. Only)</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fmetalp04_Status == New result = BlankGrayedOut stored in NotApplicableMessage; 
 
Else default result = NoExceptionApplies but user can select from list: 
 
InsulationReducesFlashingHeightLT8In display term Adding required insulation will reduce base flashing height to less that 8 inches at penthouse or parapet walls &amp;amp; conditions i-v in exception c to 
                                                         <d:t24ref h="141.0(b)2Biii"/> have been met.,  
 
MechanicalEquipmentOnRoof display term Mechanical equipment will not be lifted as part of the roof replacement, insulation added may be limited to the maximum insulation thickness that will allow a height of 8 inches from the roof membrane surface to the top of the flashing 
 
NoExceptionApplies display term None of these exceptions apply 
 
R7OrUfactorLT0_089 display term Existing roof is insulated with at least R-7 or has a U-factor lower than 0.089</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fmetalp05_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Exception to Roof Insulation Requirements in 
                                                         <d:t24ref h="141.0(b)2Biii"/> (Alts. Only)</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fmetalp04_Status == New result = BlankGrayedOut stored in NotApplicableMessage; Else default result = NoExceptionApplies</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="Fmetalp06_TopLevelOccupancy">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Occupancy Type</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from list: Nonresidential, ResidentialNR,    RelocableSchoolBuildingAllZones</xsd:documentation>
                                                   <xsd:appinfo source="displayterm">
                                                      <dtyp:displayterm value="ResidentialNR">High-rise Res/Hotel/ Motel</dtyp:displayterm>
                                                      <dtyp:displayterm value="Nonresidential">Nonresidential/ Relocatable 1 CZ</dtyp:displayterm>
                                                      <dtyp:displayterm value="RelocableSchoolBuildingAllZones">Relocatable all CZ</dtyp:displayterm>
                                                   </xsd:appinfo>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="com:TopLevelOccupancy">
                                                      <xsd:enumeration value="Nonresidential"/>
                                                      <xsd:enumeration value="RelocableSchoolBuildingAllZones"/>
                                                      <xsd:enumeration value="ResidentialNR"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="TableFmetalpperf">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                                 <xsd:documentation source="CalculationsAndRules">If Fmetalp04_Status == New Or If Fmetalp04_Status == ExistingAlteration And  Fmetalp05_RoofInsulationException  == NoExceptionApplies, this table is required</xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Fmetalp07_RoofAssemblyName" type="env:RoofAssemblyName">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Tag/Plan Detail ID</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Autofilled from Fmetalp02_RoofAssemblyName</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Fmetalp08_UfactorSpecSource" type="comp:UfactorSpecSource">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">How Design U-factor was determined</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">User selects from list: 
JA4Tables display term JA4 Tables, 
JA4Other display term Other per JA4.1.2.1, 
ApprovedSoftware display term Approved Software.  
 
If result == JA4Other  Or ApprovedSoftware D03_ExceptionalConditionMessageNRCCENVE += AdditionalDocJA4OtherSoftware</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fmetalp08_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">How Design U-factor was determined</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fmetalp04_Status == ExistingAlteration And  Fmetalp05_RoofInsulationException  != NoExceptionApplies result = BlankGrayedOut</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Fmetalp09_MetalPanelThickness" type="env:MetalPanelThickness">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Panel Thickness (in.)</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fmetalp08_UfactorSpecSource == JA4Other Or ApprovedSoftware this row is grayed out; 
 
Else If Fmetalp08_UfactorSpecSource == JA4Tables user selects from list: 
2In display term 2 in. 
2_5In display term 2.5 in. 
3In display term 3 in. 
4In display term 4 in. 
5In display term 5 in., 6In display term 6 in.</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fmetalp09_PanelThicknessOther" type="env:PanelThicknessOther">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Panel Thickness (in.)</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fmetalp08_UfactorSpecSource == JA4Tables this row is grayed out; 
 
Else If  Fmetalp08_UfactorSpecSource == JA4Other Or ApprovedSoftware user enters text</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fmetalp09_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Panel Thickness (in.)</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fmetalp04_Status == ExistingAlteration And  Fmetalp05_RoofInsulationException  != NoExceptionApplies result = BlankGrayedOut</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Fmetalp10_ConstructionSpecificationMethod">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Thermal Performance Unit</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">Result = UFactor display term U-factor and cannot be changed by user</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="env:ConstructionSpecificationMethod">
                                                         <xsd:enumeration value="UFactor"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                                <xsd:element name="Fmetalp10_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Thermal Performance Unit</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fmetalp04_Status == ExistingAlteration And  Fmetalp05_RoofInsulationException  != NoExceptionApplies result = BlankGrayedOut stored in NotApplicableMessage;</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Fmetalp11_InsulationUFactorLimit" type="env:InsulationUFactorLimit">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Thermal Performance</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fmetalp04_Status == New 
 
Switch (Fmetalp06_TopLevelOccupancy) 
case  Nonresidential: 
If A03_ClimateZoneNumber InRange (1, 5) Or InRange(9,16) result = 0.034; 
Else If A03_ClimateZoneNumber InRange (6, 8) ) result = 0.049; 
 
case ResidentialNR:
 If A03_ClimateZoneNumber == 1, 2, 4 Or  InRange (8, 16) result = 0.028; 
Else If A03_ClimateZoneNumber == 3, 5 Or 6 result = 0.034; 
Else result = 0.039; 
 
case RelocableSchoolBuildingAllZones:  result = 0.034; 
End Switch
 
Else If Fmetalp04_Status == ExistingAlteration  
 
If Fmetalp06_TopLevelOccupancy == Nonresidential Or RelocableSchoolBuildingAllZones
If A03_ClimateZoneNumber == 1 Or InRange(3,9)  result =  0.082; 
Else If A03_ClimateZoneNumber == 2 Or InRange(10,16) result = 0.055; 
End if 
 
Else If Fmetalp06_TopLevelOccupancy == ResidentialNR result = 0.055;
End if 
End if</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fmetalp11_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Thermal Performance</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fmetalp04_Status == ExistingAlteration And  Fmetalp05_RoofInsulationException  != NoExceptionApplies  result = BlankGrayedOut stored in NotApplicableMessage;</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="Fmetalp12Label" minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">U-factor per Design</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Fmetalp12_UfactorJA429" type="comp:UfactorJA429">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">per JA4</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fmetalp09_MetalPanelThickness == 2In result = 0.079;  
Else If Fmetalp09_MetalPanelThickness == 2_5In result = 0.064;  
Else If Fmetalp09_MetalPanelThickness == 3In  result = 0.054; 
Else If Fmetalp09_MetalPanelThickness == 4In result = 0.041;   
Else If Fmetalp09_MetalPanelThickness == 5In result = 0.033 ;  
Else If Fmetalp09_MetalPanelThickness == 6In  result =  0.028;
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fmetalp12_UfactorJA4Other" type="comp:UfactorJA4Other">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">per Software/ Other</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fmetalp08_UfactorSpecSource == JA4Other Or  ApprovedSoftware, user input value</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fmetalp12_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">U-factor per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fmetalp04_Status == ExistingAlteration And  Fmetalp05_RoofInsulationException  != NoExceptionApplies result = BlankGrayedOut stored in NotApplicableMessage;</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Fmetalp13_RoofNetArea" type="comp:RoofNetArea">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Net Area<d:sup>3</d:sup> ft<d:sup>2</d:sup>
                                                      </xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fmetalp01_UseInAreaWeightedAveUfactor == true, user input</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fmetalp13_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Net Area<d:sup>3</d:sup> ft<d:sup>2</d:sup>
                                                      </xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fmetalp04_Status == ExistingAlteration And  Fmetalp05_RoofInsulationException  != NoExceptionApplies Or If Fmetalp01_UseInAreaWeightedAveUfactor == false result = BlankGrayedOut stored in NotApplicableMessage;</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="FmetalpFootnote1" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                                 <xsd:documentation source="AdditionalRequirements">
                                    <d:i><d:sup>1</d:sup>FOOTNOTES: If any individual assembly is non-compliant, assemblies may show compliance using an area-weighted calculation.  Metal building roofs may not be combined with other roof types.  The area-weighted compliance option is not available for alterations demonstrating compliance with R-values in 
                                       <d:t24table/>Table 141.0-C.</d:i>
                                 </xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FmetalpFootnote2" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                                 <xsd:documentation source="AdditionalRequirements">
                                    <d:i><d:sup>2</d:sup>Roof area minus any fenestration/ skylight area </d:i>
                                 </xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Fmetalb" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>Metal Building Roof Assemblies</d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if F01MetalBldg_RoofTypeApplies == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Fmetalb01_UseInAreaWeightedAveUfactor" type="comp:UseInAreaWeightedAveUfactor">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Calculate Area-Weighted Average U-factor for Metal Building Roof<d:sup>1</d:sup>
                                 </xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If checked result = true; Else result = false.   
 
If result == true   D03_ExceptionalConditionMessageNRCCENVE += RoofAreaWeightedAveInF</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="TableFmetalb">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Fmetalb02_RoofAssemblyName" type="env:RoofAssemblyName">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Tag/Plan Detail ID</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Fmetalb03_RoofDescription" type="env:RoofDescription">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Name/Description</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Fmetalb04_Status">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Status</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from list: New display term New, ExistingAlteration display term Altered</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="com:Status">
                                                      <xsd:enumeration value="ExistingAlteration"/>
                                                      <xsd:enumeration value="New"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Fmetalb05_RoofInsulationException" type="comp:RoofInsulationException">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Exception to Roof Insulation Requirements in 
                                                         <d:t24ref h="141.0(b)2Biii"/> (Alts. Only)</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fmetalb04_Status == New result = BlankGrayedOut stored in NotApplicableMessage; 
 
Else default result = NoExceptionApplies but user can select from list: 
 
InsulationReducesFlashingHeightLT8In display term Adding required insulation will reduce base flashing height to less that 8 inches at penthouse or parapet walls &amp;amp; conditions i-v in exception c to 
                                                         <d:t24ref h="141.0(b)2Biii"/> have been met.,  
 
MechanicalEquipmentOnRoof display term Mechanical equipment will not be lifted as part of the roof replacement, insulation added may be limited to the maximum insulation thickness that will allow a height of 8 inches from the roof membrane surface to the top of the flashing, 
 
NoExceptionApplies display term None of these exceptions apply, 
 
R7OrUfactorLT0_089 display term Existing roof is insulated with at least R-7 or has a U-factor lower than 0.089</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fmetalb05_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Exception to Roof Insulation Requirements in 
                                                         <d:t24ref h="141.0(b)2Biii"/> (Alts. Only)</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fmetalb04_Status == New result = BlankGrayedOut stored in NotApplicableMessage; Else default result = NoExceptionApplies</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="Fmetalb06_TopLevelOccupancy">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Occupancy Type</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from list: Nonresidential, ResidentialNR,    RelocableSchoolBuildingAllZones</xsd:documentation>
                                                   <xsd:appinfo source="displayterm">
                                                      <dtyp:displayterm value="ResidentialNR">High-rise Res/Hotel/ Motel</dtyp:displayterm>
                                                      <dtyp:displayterm value="Nonresidential">Nonresidential/ Relocatable 1 CZ</dtyp:displayterm>
                                                      <dtyp:displayterm value="RelocableSchoolBuildingAllZones">Relocatable all CZ</dtyp:displayterm>
                                                   </xsd:appinfo>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="com:TopLevelOccupancy">
                                                      <xsd:enumeration value="Nonresidential"/>
                                                      <xsd:enumeration value="RelocableSchoolBuildingAllZones"/>
                                                      <xsd:enumeration value="ResidentialNR"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="TableFmetalbperf">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Fmetalb07_RoofAssemblyName" type="env:RoofAssemblyName">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Tag/Plan Detail ID</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Autofilled from Fmetalb02_RoofAssemblyName</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Fmetalb08_UfactorSpecSource" type="comp:UfactorSpecSource">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">How Design U-factor was determined</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fmetalb12_ConstructionSpecificationMethod == RValue result = BlankGrayedOut stored in NotApplicableMessage; 
 
Else User selects from list: 
JA4Tables display term JA4 Tables 
JA4Other display term Other per JA4.1.2.1 
ApprovedSoftware display term Approved Software.  
 
If result == JA4Other  Or ApprovedSoftware D03_ExceptionalConditionMessageNRCCENVE += AdditionalDocJA4OtherSoftware</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fmetalb08_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">How Design U-factor was determined</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fmetalb04_Status == ExistingAlteration And  Fmetalb05_RoofInsulationException  != NoExceptionApplies Or If Fmetalb12_ConstructionSpecificationMethod == RValue result = BlankGrayedOut stored in NotApplicableMessage</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Fmetalb09_MetalBldgRoofType" type="env:MetalBldgRoofType">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Insulation System</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fmetalb08_UfactorSpecSource == JA4Other Or ApprovedSoftware this row is grayed out; 
 
Else If Fmetalb08_UfactorSpecSource == JA4Tables user selects from list:  
 
ScrewDownRoof display term Screw Down Roofs 
StandingSeam1Layer display term Standing Seam - 1 Layer of Insulation 
StandingSeam2Layer display term Standing Seam - 2 Layers of Insulation
FilledThermalBlocks display term Filled Cavity with thermal blocks</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fmetalb09_MetalBldgRoofTypeOther" type="env:MetalBldgRoofTypeOther">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Insulation System</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fmetalb08_UfactorSpecSource == JA4Tables this row is grayed out; 
 
Else If Fmetalb_UfactorSpecSource == JA4Other Or ApprovedSoftware user enters text</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fmetalb09_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Insulation System</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fmetalb04_Status == ExistingAlteration And  Fmetalb05_RoofInsulationException  != NoExceptionApplies Or If Fmetalb12_ConstructionSpecificationMethod == RValue result = BlankGrayedOut stored in NotApplicableMessage and both rows for F09 are grayed out;
 ;</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Fmetalb10_CavityInsulationNR">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Cavity Insulation per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fmetalb08_UfactorSpecSource == JA4Other Or ApprovedSoftware this row is grayed out; 
 
Else If Fmetalb08_UfactorSpecSource == JA4Tables user selects from list as follows:  
 
If Fmetalb09_MetalBuildingRoofType == ScrewDownRoof,  list  (display terms add dash e.g. R-10)
R10, 
R11, 
R13, 
R16, 
R19 ;     
 
Else  If Fmetalb09_MetalBuildingRoofType == StandingSeam1Layer,  list  (except for R0, display terms add dash e.g. R-10)
R0 (display term None), 
R10, 
R11, 
R13, 
R16, 
R19; 
 
Else  If Fmetalb09_MetalBuildingRoofType == StandingSeam2Layer, list 
R10_R10 display term R-10 + R-10, 
R10_R11 display term R-10 + R-11, 
R10_R13 display term R-10 + R-13, 
R10_R19 display term R-10 + R-19, 
R11_R11 display term R-11 + R-11, 
R11_R13 display term R-11 + R-13, 
R11_R19 display term R-11 + R-19, 
R13_R13 display term R-13 + R-13, 
R13_R19 display term R-13 + R-19, 
R16_R19 display term R-16 + R-19, 
R19_R19 display term R-19 + R-19; 
 
Else  If Fmetalb09_MetalBuildingRoofType == FilledThermalBlocks list 
R10_R19 display term R-10 + R-19.</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="env:CavityInsulationNR">
                                                         <xsd:enumeration value="R10"/>
                                                         <xsd:enumeration value="R10_R10"/>
                                                         <xsd:enumeration value="R10_R11"/>
                                                         <xsd:enumeration value="R10_R13"/>
                                                         <xsd:enumeration value="R10_R19"/>
                                                         <xsd:enumeration value="R11"/>
                                                         <xsd:enumeration value="R11_R11"/>
                                                         <xsd:enumeration value="R11_R13"/>
                                                         <xsd:enumeration value="R11_R19"/>
                                                         <xsd:enumeration value="R13"/>
                                                         <xsd:enumeration value="R13_R13"/>
                                                         <xsd:enumeration value="R13_R19"/>
                                                         <xsd:enumeration value="R16"/>
                                                         <xsd:enumeration value="R16_R19"/>
                                                         <xsd:enumeration value="R19"/>
                                                         <xsd:enumeration value="R19_R19"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                                <xsd:element name="Fmetalb10_CavityInsulationOther" type="env:CavityInsulationOther">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Cavity Insulation per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fmetalb08_UfactorSpecSource == JA4Tables, this row is grayed out; 
 
Else If Fmetalb08_UfactorSpecSource == JA4Other Or ApprovedSoftware user enters text</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fmetalb10_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Cavity Insulation per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fmetalb04_Status == ExistingAlteration And  Fmetalb05_RoofInsulationException  != NoExceptionApplies Or If Fmetalb12_ConstructionSpecificationMethod == RValue result = BlankGrayedOut stored in NotApplicableMessage and both rows for F10 are grayed out;</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Fmetalb11_ContinuousInsulationNR">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Continuous Insulation per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fmetalb08_UfactorSpecSource == JA4Other Or ApprovedSoftware this row is grayed out; 
 
Else If Fmetalb08_UfactorSpecSource == JA4Tables user selects from list: 
R0ci display term None, 
R6ci display term R-6, 
R9ci display term R-9, 
R13ci display term R-13, 
R15ci display term R-15, 
R19ci display term R-19, 
R22ci display term R-22, 
R25ci display term R-25, 
R32ci display term R-32, 
R38ci display term R-38</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="env:ContinuousInsulationNR">
                                                         <xsd:enumeration value="R0ci"/>
                                                         <xsd:enumeration value="R13ci"/>
                                                         <xsd:enumeration value="R15ci"/>
                                                         <xsd:enumeration value="R19ci"/>
                                                         <xsd:enumeration value="R22ci"/>
                                                         <xsd:enumeration value="R25ci"/>
                                                         <xsd:enumeration value="R32ci"/>
                                                         <xsd:enumeration value="R38ci"/>
                                                         <xsd:enumeration value="R6ci"/>
                                                         <xsd:enumeration value="R9ci"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                                <xsd:element name="Fmetalb11_ContinuousInsulationOther" type="env:ContinuousInsulationOther">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Continuous Insulation per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fmetalb08_UfactorSpecSource == JA4Tables, this row is grayed out; 
 
Else If Fmetalb08_UfactorSpecSource == JA4Other Or ApprovedSoftware user enters text</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fmetalb11_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Continuous Insulation per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fmetalb04_Status == ExistingAlteration And  Fmetalb05_RoofInsulationException  != NoExceptionApplies result = BlankGrayedOut</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
