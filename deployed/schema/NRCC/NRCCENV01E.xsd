                        <xsd:annotation>
                           <xsd:documentation source="FieldText"/>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: If B02NewWall_ComponentApplies, B02AddWall_ComponentApplies And B02AltWall_ComponentApplies == false result = BlankGrayedOut stored in NotApplicableMessage and field is grayed out</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
