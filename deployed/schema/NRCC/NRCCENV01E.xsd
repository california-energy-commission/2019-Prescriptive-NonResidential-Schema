End Switch
Else Hspandrel06_SpandrelCurtainFinish  == DoubleNoLowE 
Switch (Hspandrel07_CavityInsulationNR) 
case R0:  0.727;  case R4: 0.611;  case R7:  0.400; case R10: 0.361;   case R15: 0.341;   case R20: 0.323;    case R25: 0.313;   case R30: 0.307;  
End Switch
Else Hspandrel06_SpandrelCurtainFinish  == DoubleLowE Or TriplePaneGlass  
Switch (Hspandrel07_CavityInsulationNR) 
case R0:  0.567;  case R4: 0.494;  case R7:  0.380; case R10: 0.351;   case R15: 0.335;   case R20: 0.320;    case R25: 0.311;   case R30: 0.306;  
End Switch
End if
 
Else If Hspandrel05_SpandrelCurtainType == AluminumThermalBreakCW 
If Hspandrel06_SpandrelCurtainFinish  == SinglePaneGlass Or StoneMetalConcrete 
Switch (Hspandrel07_CavityInsulationNR) 
case R0:  1.110;  case R4: 0.862;  case R7:  0.339; case R10: 0.282;   case R15: 0.256;   case R20: 0.234;    case R25: 0.222;   case R30: 0.215;  
