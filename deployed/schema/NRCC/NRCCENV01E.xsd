                                                <xsd:element name="Kskyperf10a_UFactorLimit" type="com:UFactorLimit">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Product Performance</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Kskyperf06_TopLevelOccupancy == Nonresidential  
Switch (Kskyperf05_FenestrationProductType) 
case SkylightGlassCurb:  result = 0.58; 
case SkylightGlassDeck:  result = 0.46;  
case SkylightPlasticCurb:  case SkylightTubular: result = 0.88; 
End Switch
 
Else If Kskyperf06_TopLevelOccupancy == ResidentialNR 
Switch (Kskyperf05_FenestrationProductType) 
case SkylightGlassCurb:  result = 0.58; 
case SkylightGlassDeck:  result = 0.46;  
case SkylightPlasticCurb: result = 0.88; 
End Switch
 
Else If Kskyperf06_TopLevelOccupancy == RelocableSchoolBuildingAllZones  
Switch (Kskyperf05_FenestrationProductType) 
case SkylightGlassCurb:  result = 0.99; 
case SkylightGlassDeck:  result = 0.57;  
case SkylightPlasticCurb: result = 0.87;
End Switch
End If</xsd:documentation>
