                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Span Deck &amp;amp; Concrete</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input: If checked result = true; Else result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
