                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Kskyperf10b_SHGC_Limit" type="com:SHGC_Limit">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Product Performance</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Kskyperf06_TopLevelOccupancy == Nonresidential  
Switch (Kskyperf05_FenestrationProductType) 
case SkylightGlassCurb: SkylightGlassDeck: result = 0.25; 
End Switch
 
Else If Kskyperf06_TopLevelOccupancy == ResidentialNR 
Switch (Kskyperf05_FenestrationProductType) 
case SkylightGlassCurb:  case SkylightGlassDeck:  result = 0.25;  
End Switch
 
Else If Kskyperf06_TopLevelOccupancy == RelocableSchoolBuildingAllZones  
Switch (Kskyperf05_FenestrationProductType)  
case SkylightGlassCurb:  case SkylightGlassDeck: 
If  Ksky04_TotalSkylightArea /  Ksky02_ExteriorSurfaceGrossArea LTE 0.02 result = 0.46; 
Else If Ksky04_TotalSkylightArea  /  Ksky02_ExteriorSurfaceGrossArea == InRange(0.021, 0.05) result = 0.36;
End If
case SkylightPlasticCurb
If  Ksky04_TotalSkylightArea /  Ksky02_ExteriorSurfaceGrossArea LTE 0.02 result = 0.69; 
Else If Ksky04_TotalSkylightArea  /  Ksky02_ExteriorSurfaceGrossArea == InRange(0.021, 0.05) result = 0.57;
End If
 
End Switch
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Kskyperf10b_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Product Performance</xsd:documentation>
