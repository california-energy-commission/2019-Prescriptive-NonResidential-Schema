                  </xsd:element>
                  <xsd:element name="Imass" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>Raised Mass Floors</d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required If I01Mass_FloorTypeApplies == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Imass01_UseInAreaWeightedAveUfactor" type="comp:UseInAreaWeightedAveUfactor">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Calculate Area-Weighted Average U-factor for Raised Mass Floors<d:sup>1</d:sup>
                                 </xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If checked result = true; Else result = false.  If result == true   D03_ExceptionalConditionMessageNRCCENVE += FloorAreaWeightedAveInI</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="TableImass">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Imass02_FloorAssemblyName" type="env:FloorAssemblyName">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Tag/Plan Detail ID</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Imass03_FloorDescription" type="env:FloorDescription">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Name/Description</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Imass04a_TopLevelOccupancy">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Occupancy &amp;amp; Status</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from list: Nonresidential display term Nonresidential/ Relocatable 1 CZ: New, ResidentialNR display term  High-rise Res/ Hotel/ Motel: New, RelocableSchoolBuildingAllZones display term Relocatable all CZ: New,  AlteredOccupancy display term High-rise Res/ Hotel/ Motel: Altered.</xsd:documentation>
                                                   <xsd:appinfo source="displayterm">
                                                      <dtyp:displayterm value="ResidentialNR">High-rise Res/ Hotel/ Motel: New</dtyp:displayterm>
                                                      <dtyp:displayterm value="Nonresidential">Nonresidential/ Relocatable 1 CZ: New</dtyp:displayterm>
                                                      <dtyp:displayterm value="RelocableSchoolBuildingAllZones">Relocatable all CZ: New</dtyp:displayterm>
                                                      <dtyp:displayterm value="AlteredOccupancy">High-rise Res/ Hotel/ Motel: Altered</dtyp:displayterm>
                                                   </xsd:appinfo>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="com:TopLevelOccupancy">
                                                      <xsd:enumeration value="AlteredOccupancy"/>
                                                      <xsd:enumeration value="Nonresidential"/>
                                                      <xsd:enumeration value="RelocableSchoolBuildingAllZones"/>
                                                      <xsd:enumeration value="ResidentialNR"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:element name="Imass04b_Status">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Occupancy &amp;amp; Status</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculatedf field: If Imass04a_TopLevelOccupancy == AlteredOccupancy result = ExistingAlteration; 
Else result = New
End If</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="com:Status">
                                                      <xsd:enumeration value="ExistingAlteration"/>
                                                      <xsd:enumeration value="New"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Imass05_UfactorSpecSource" type="comp:UfactorSpecSource">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">How Design U-factor was determined</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Imass08_ConstructionSpecificationMethod == RValue result = BlankGrayedOut stored in NotApplicableMessage; 
Else User selects from list: JA4Tables display term JA4 Tables, JA4Other display term Other per JA4.1.2.1, ApprovedSoftware display term Approved Software. 
End If
If result == JA4Other Or ApprovedSoftware   D03_ExceptionalConditionMessageNRCCENVE += AdditionalDocJA4OtherSoftware
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Imass05_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">How Design U-factor was determined</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If  Imass08_ConstructionSpecificationMethod == RValue result = BlankGrayedOut stored in NotApplicableMessage;
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Imass06_InsulationPlacement">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Insulation Location</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Imass05_UfactorSpecSource == JA4Tables, User selects from list ContinuousBelowDeck display term Continuous underneath, ContinuousAboveDeck display term Continuous above deck, no sleepers, ContinuousAboveDeckWithSleepers display term Continuous above deck with sleepers
End If</xsd:documentation>
                                                      <xsd:appinfo source="displayterm">
                                                         <dtyp:displayterm value="ContinuousBelowDeck">Continuous underneath</dtyp:displayterm>
                                                         <dtyp:displayterm value="ContinuousAboveDeck">Continuous above deck, no sleepers</dtyp:displayterm>
                                                         <dtyp:displayterm value="ContinuousAboveDeckWithSleepers">Continuous above deck with sleepers</dtyp:displayterm>
                                                      </xsd:appinfo>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="env:InsulationPlacement">
                                                         <xsd:enumeration value="ContinuousAboveDeck"/>
                                                         <xsd:enumeration value="ContinuousAboveDeckWithSleepers"/>
                                                         <xsd:enumeration value="ContinuousBelowDeck"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                                <xsd:element name="Imass06_InsulationOther" type="env:InsulationOther">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Insulation Location</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Imass05_UfactorSpecSource == JA4Other Or ApprovedSoftware user enters text.
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Imass06_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Insulation Location</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If  Imass08_ConstructionSpecificationMethod == RValue result = BlankGrayedOut and both rows from 06 are grayed out
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Imass07_ContinuousInsulationNR">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Continuous Insulation per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Imass05_UfactorSpecSource == JA4Other Or ApprovedSoftware this row is grayed out; 
Else If Imass05_UfactorSpecSource == JA4Tables user selects from list: R0ci display term None, R2ci display term R-2, R4ci display term R-4, R6ci display term R-6, R8ci display term R-8, R10ci display term R-10, R12ci display term R-12, R15ci display term R-15, R20ci display term R-20, R25ci display term R-25, R30ci display term R-30
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="env:ContinuousInsulationNR">
                                                         <xsd:enumeration value="R0ci"/>
                                                         <xsd:enumeration value="R10ci"/>
                                                         <xsd:enumeration value="R12ci"/>
                                                         <xsd:enumeration value="R15ci"/>
                                                         <xsd:enumeration value="R20ci"/>
                                                         <xsd:enumeration value="R25ci"/>
                                                         <xsd:enumeration value="R2ci"/>
                                                         <xsd:enumeration value="R30ci"/>
                                                         <xsd:enumeration value="R4ci"/>
                                                         <xsd:enumeration value="R6ci"/>
                                                         <xsd:enumeration value="R8ci"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                                <xsd:element name="Imass07_ContinuousInsulationOther" type="env:ContinuousInsulationOther">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Continuous Insulation per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Imass05_UfactorSpecSource == JA4Tables, this row is grayed out
Else If Imass05_UfactorSpecSource ==  JA4Other Or ApprovedSoftware 
Or If Imass08_ConstructionSpecificationMethod == RValue
user enters text
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="Imass08_ConstructionSpecificationMethod">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Thermal Performance Unit</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">If Imass04b_Status == ExistingAlteration  And Imass01_UseInAreaWeightedAveUfactor  == false list RValue, UFactor ; 
Else result = UFactor and cannot be changed by user
End If</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="env:ConstructionSpecificationMethod">
                                                      <xsd:enumeration value="RValue"/>
                                                      <xsd:enumeration value="UFactor"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Imass09_InsulationUFactorLimit" type="env:InsulationUFactorLimit">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Thermal Performance<d:sup>2</d:sup>
                                                      </xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">Calculated field: 
If Imass08_ConstructionSpecificationMethod == UFactor 
Switch ( Imass04a_TopLevelOccupancy) 
case AlteredOccupancy:
If Imass04b_Status == ExistingAlteration result = 0.111; 
End If
 
case  Nonresidential: 
If A03_ClimateZoneNumber == 1, 2, Or InRange(11, 15) result = 0.092; 
Else  If A03_ClimateZoneNumber == InRange (3, 10) result = 0.269; 
Else If A03_ClimateZoneNumber == 16 result = 0.058;   
End If
 
case ResidentialNR: 
If A03_ClimateZoneNumber ==  1, 2,Or 14 result = 0.045; 
Else If A03_ClimateZoneNumber == InRange(3, 5) Or InRange(11, 13) Or 15  result = 0.058; 
Else If A03_ClimateZoneNumber == 6 Or 10  result = 0.069;  
Else If A03_ClimateZoneNumber == 16 result = 0.037; 
Else If A03_ClimateZoneNumber == InRange(7,9) result = 0.092
End if
 
case RelocableSchoolBuildingAllZones: result = 0.048;
End Switch
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Imass09_InsulationRValueLimit">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Thermal Performance<d:sup>2</d:sup>
                                                      </xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If  Imass04b_Status == ExistingAlteration And  Imass08_ConstructionSpecificationMethod == RValue 
If  Imass04a_TopLevelOccupancy == AlteredOccupancy  result = R6; 
End If
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="env:InsulationRValueLimit">
                                                         <xsd:enumeration value="R6"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="Imass10Label" minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">U-factor per Design</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Imass10_UfactorJA446" type="comp:UfactorJA446">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">per JA4</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">Calculated field:  
If Imass05_UfactorSpecSource != JA4Tables this row is grayed out; 
Else 
Switch ( Imass07_ContinuousInsulationNR) 
case R0ci:  
If Imass06_InsulationPlacement == ContinuousBelowDeck result =  0.269;  
Else Imass06_InsulationPlacement == ContinuousAboveDeck  result =  0.234;   
Else Imass06_InsulationPlacement == ContinuousAboveDeckWithSleepers  result = 0.229;   
End If
 
case R2ci:  
If Imass06_InsulationPlacement == ContinuousBelowDeck result =  0.183;  
Else Imass06_InsulationPlacement == ContinuousAboveDeck  result =  0.159;   
Else Imass06_InsulationPlacement == ContinuousAboveDeckWithSleepers  result = 0.157;   
End If
 
case R4ci: If Imass06_InsulationPlacement == ContinuousBelowDeck result =  0.138;  
Else Imass06_InsulationPlacement == ContinuousAboveDeck  result =  0.121;   
Else Imass06_InsulationPlacement == ContinuousAboveDeckWithSleepers  result = 0.120;    
End If
 
case R6ci: 
If Imass06_InsulationPlacement == ContinuousBelowDeck result =  0.111;  
Else Imass06_InsulationPlacement == ContinuousAboveDeck  result =  0.097;   
Else Imass06_InsulationPlacement == ContinuousAboveDeckWithSleepers  result = 0.097;   
End If
 
case R8ci: 
If Imass06_InsulationPlacement == ContinuousBelowDeck result =  0.092;  
Else Imass06_InsulationPlacement == ContinuousAboveDeck  result =  0.081;   
Else Imass06_InsulationPlacement == ContinuousAboveDeckWithSleepers  result = 0.081;  
End If
 
case R10ci: 
If Imass06_InsulationPlacement == ContinuousBelowDeck result =  0.079;  
Else Imass06_InsulationPlacement == ContinuousAboveDeck  result =  0.070;   
Else Imass06_InsulationPlacement == ContinuousAboveDeckWithSleepers  result = 0.070;  
End If
 
case R12ci: 
If Imass06_InsulationPlacement == ContinuousBelowDeck result =  0.069;  
Else Imass06_InsulationPlacement == ContinuousAboveDeck  result =  0.061;   
Else Imass06_InsulationPlacement == ContinuousAboveDeckWithSleepers  result = 0.061;  
End If
 
case R15ci: 
If Imass06_InsulationPlacement == ContinuousBelowDeck result =  0.058;  
Else Imass06_InsulationPlacement == ContinuousAboveDeck  result =  0.052;   
Else Imass06_InsulationPlacement == ContinuousAboveDeckWithSleepers  result = 0.052;   
End If
 
case R20ci: 
If Imass06_InsulationPlacement == ContinuousBelowDeck result =  0.045;  
Else Imass06_InsulationPlacement == ContinuousAboveDeck  result =  0.041;   
Else Imass06_InsulationPlacement == ContinuousAboveDeckWithSleepers  result = 0.041; 
End If
 
case R25ci: 
If Imass06_InsulationPlacement == ContinuousBelowDeck result =  0.037;  
Else Imass06_InsulationPlacement == ContinuousAboveDeck  result =  0.034;   
Else Imass06_InsulationPlacement == ContinuousAboveDeckWithSleepers  result = 0.034; 
End If
 
case R30ci: 
If Imass06_InsulationPlacement == ContinuousBelowDeck result =  0.031;  
Else Imass06_InsulationPlacement == ContinuousAboveDeck  result =  0.029;   
Else Imass06_InsulationPlacement == ContinuousAboveDeckWithSleepers  result = 0.029;
End If
End Switch
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Imass10_UfactorJA4Other" type="comp:UfactorJA4Other">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">per Software/ Other</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Imass05_UfactorSpecSource == JA4Tables this row is grayed out;  
Else user enter value
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Imass10_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">U-factor per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If  Imass08_ConstructionSpecificationMethod == RValue result = BlankGrayedOut and both rows from 10 are grayed out
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Imass11_FloorArea" type="com:FloorArea">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Area ft<d:sup>2</d:sup>
                                                      </xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Imass11_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Area ft<d:sup>2</d:sup>
                                                      </xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If  Imass08_ConstructionSpecificationMethod == RValue Or If Imass01_UseInAreaWeightedAveUfactor  == false
result = BlankGrayedOut
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="ImassFootnote1" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                                 <xsd:documentation source="AdditionalRequirements">
                                    <d:i><d:sup>1</d:sup>FOOTNOTES: If any individual assembly is non-compliant, assemblies may show compliance using an area-weighted calculation. Raised Mass floors may not be combined with other floor types. The area-weighted compliance option is not available for alterations demonstrating compliance with R-values in 
                                       <d:t24ref h="141.0(b)1C2"/> for high-rise residential and hotel/ motel occupancies.</d:i>
                                 </xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="ImassFootnote2" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                                 <xsd:documentation source="AdditionalRequirements">
                                    <d:i><d:sup>2</d:sup> If "R-value" is shown in cell 08 as the Thermal Performance Unit, the R-value shown here is for continuous insulation per 
                                       <d:t24ref h="141.0(b)1C2"/>.</d:i>
                                 </xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Iheated" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>Heated Slab-on-grade Floors (new floors only)</d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required If I01Heated_FloorTypeApplies == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="TableIheated">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Iheated01_FloorAssemblyName" type="env:FloorAssemblyName">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Tag/Plan Detail ID</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Iheated02_FloorDescription" type="env:FloorDescription">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Name/Description</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Iheated03_InsulationPlacement">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Insulation Location</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from list: OutsideEdge display term Outside edge, BetweenSlab display term Between slab &amp;amp; outside foundation wall</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="env:InsulationPlacement">
                                                      <xsd:enumeration value="BetweenSlab"/>
                                                      <xsd:enumeration value="OutsideEdge"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:element name="Iheated04Label" minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Insulation Orientation</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Iheated04a" minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Vertical</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">List: Vertical in top cell</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Iheated04b" minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Horizontal</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">List: Horizontal in bottom cell</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Iheated05a_SlabInsulationRValueRequired">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Min. R-value required</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated field: This value is assigned for Vertical orientation: 
If A03_ClimateZoneNumber == InRange(1, 15) result = R5; 
Else If  A03_ClimateZoneNumber == 16 result = R10
End If</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="env:SlabInsulationRValueRequired">
                                                      <xsd:enumeration value="R10"/>
                                                      <xsd:enumeration value="R5"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Iheated05b_SlabInsulationRValueRequired">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Min. R-value required</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">Calculated field: This value is assigned for Horizontal orientation: 
If A03_ClimateZoneNumber == 16 And Iheated03_InsulationPlacement == BetweenSlab result = R7; 
Else If A03_ClimateZoneNumber == InRange(1, 15)  Or   Iheated03_InsulationPlacement == OutsideEdge   result = BlankGrayedOut stored in NotApplicablemessage;
End if</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="env:SlabInsulationRValueRequired">
                                                         <xsd:enumeration value="R7"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                                <xsd:element name="Iheated05b_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Min. R-value required</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">Calculated field: This value is assigned for Horizontal orientation: 
If A03_ClimateZoneNumber == 16 And Iheated03_InsulationPlacement == BetweenSlab result = R7; 
Else If A03_ClimateZoneNumber == InRange(1, 15)  Or   Iheated03_InsulationPlacement == OutsideEdge   result = BlankGrayedOut stored in NotApplicablemessage;
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="Iheated06a_InsulationInteriorRValue" type="env:InsulationInteriorRValue">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">R-value per Design</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input numeric value and "R-" is prepended to result</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Iheated06b_InsulationInteriorRValue" type="env:InsulationInteriorRValue">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">R-value per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If A03_ClimateZoneNumber == 16 And Iheated03_InsulationPlacement == BetweenSlab user input numeric value and "R-" is prepended to result; 
Else If A03_ClimateZoneNumber == InRange(1, 15)  Or   Iheated03_InsulationPlacement == OutsideEdge   result = BlankGrayedOut stored in NotApplicablemessage;
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Iheated06b_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">R-value per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If A03_ClimateZoneNumber == 16 And Iheated03_InsulationPlacement == BetweenSlab user input numeric value and "R-" is prepended to result; 
Else If A03_ClimateZoneNumber == InRange(1, 15)  Or   Iheated03_InsulationPlacement == OutsideEdge   result = BlankGrayedOut stored in NotApplicablemessage;
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="Iheated07Label" minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Insulation Materials &amp;amp; install</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Iheated07" minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Materials &amp;amp; install requirements meeting 
                                                      <d:t24ref h="110.8(g)"/> have been included in the construction documents.</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Iheated08_BuildingPlanName" type="comp:BuildingPlanName">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Location in Construction Documents</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input text</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="ICalc1UF" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>Area-Weighted Average U-factor Compliance Calculation for Raised Mass Floors</d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required If  Imass01_UseInAreaWeightedAveUfactor == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="ICalc1UF01_AreaWeightAvgType">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Floor Type</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result = RaisedMass display term Raised Mass</xsd:documentation>
                              </xsd:annotation>
                              <xsd:simpleType>
                                 <xsd:restriction base="comp:AreaWeightAvgType">
                                    <xsd:enumeration value="RaisedMass"/>
                                 </xsd:restriction>
                              </xsd:simpleType>
                           </xsd:element>
                           <xsd:element name="ICalc1UF02_FloorTypeTotalArea" type="comp:FloorTypeTotalArea">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Total Area of Floor Type (ft<d:sup>2</d:sup>)</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result = SUM ALL (Imass11_FloorArea);</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="ICalc1UF03to04Label" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Area-weighted U-factor for Floor Type</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="ICalc1UF03_AreaWeightAvgRequiredUfactor" type="comp:AreaWeightAvgRequiredUfactor">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Required</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result = SUM ALL (Imass11_FloorArea x Imass09_InsulationUFactorLimit) / ICalc1UF02_FloorTypeTotalArea;</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="ICalc1UF04_AreaWeightAvgUfactor" type="comp:AreaWeightAvgUfactor">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Designed</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result= SUM ALL (Imass11_FloorArea x Ufactor) / ICalc1UF02_FloorTypeTotalArea.  
 
Ufactor for row is the property that IsPresent: Imass10_UfactorJA4Other Or Imass10_UfactorJA446</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="ICalc1UF05_DoesAreaWeightAvgComplyUfactor" type="comp:DoesAreaWeightAvgComplyUfactor">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Compliance Results Using Area-Weighted Calculation Option</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: 
If ICalc1UF03_AreaWeightAvgRequiredUfactor GTE ICalc1UF04_AreaWeightAvgUfactor result = true display term COMPLIES; 
Else result = false display term DOES NOT COMPLY
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="ICalc2UF" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>Area-Weighted Average U-factor Compliance Calculation for  Framed/ SIPs Floor Types</d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required If Iframed01_UseInAreaWeightedAveUfactor,  ISIPS01_UseInAreaWeightedAveUfactor == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="TableICalc2UF">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="ICalc2UF01_AreaWeightAvgType">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Floor Type</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated field: If any of the following conditions are true, a row is added and the result loaded into this field:  
If Iframed01_UseInAreaWeightedAveUfactor == true  result Framed; 
End if
 
 If  ISIPS01_UseInAreaWeightedAveUfactor == true, result = SIPS; 
End If</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="comp:AreaWeightAvgType">
                                                      <xsd:enumeration value="Framed"/>
                                                      <xsd:enumeration value="SIPS"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:element name="ICalc2UF02_FloorTypeTotalArea" type="comp:FloorTypeTotalArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Total Area of Floor Type (ft<d:sup>2</d:sup>)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated field: For current row, 
Switch (ICalc2UF01_AreaWeightAvgType) 
case Framed: result = SUM ALL (Iframed12_FloorArea );  
case SIPS:  result = SUM ALL (ISIPS12_FloorArea);
End Switch</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="ICalc2UF03to04Label" minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Area-weighted U-factor for Floor Type</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="ICalc2UF03_AreaWeightAvgRequiredUfactor" type="comp:AreaWeightAvgRequiredUfactor">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Required</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated field: 
Switch (ICalc2UF01_AreaWeightAvgType) 
case Framed: result = (SUM ALL (Iframed12_FloorArea x Iframed10_InsulationUFactorLimit)) / ICalc2UF02_FloorTypeTotalArea; 
case SIPS: (SUM ALL (ISIPS12_FloorArea x ISIPS10_InsulationUFactorLimit)) / ICalc2UF02_FloorTypeTotalArea;
End Switch</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="ICalc2UF04Framed_AreaWeightAvgUfactor" type="comp:AreaWeightAvgUfactor">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Designed</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">Calculated field: 
If ICalc2UF01_AreaWeightAvgType ==Framed result= (SUM ALL (Iframed12_FloorArea x Ufactor) ) / ICalc2UF02_FloorTypeTotalArea;  
End If
 
Ufactor for row is the property that IsPresent: Iframed11_UfactorJA441_16OC, Iframed11_UfactorJA441_24OC, Iframed11_UfactorJA442_16OC, Iframed11_UfactorJA442_24OC, Iframed11_UfactorJA444_16OC, Iframed11_UfactorJA444_24OC, Iframed11_UfactorJA445_16OC, Iframed11_UfactorJA445_24OC Or Iframed11_UfactorJA4Other</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="ICalc2UF04SIPS_AreaWeightAvgUfactor" type="comp:AreaWeightAvgUfactor">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Designed</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">Calculated field: 
If ICalc2UF01_AreaWeightAvgType == SIPS result= (SUM ALL (ISIPS12_FloorArea x Ufactor) ) / ICalc2UF02_FloorTypeTotalArea.  
End If
Ufactor for row is the property that IsPresent: ISIPS11_UfactorJA4Other Or  ISIPS11_UfactorJA443Crawl Or ISIPS11_UfactorJA443NoCrawl</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                             </xsd:choice>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="ICalc2UF02to04TotalsLabel" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">
                                    <d:b>Total for all Floor Types:</d:b>
                                 </xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="ICalc2UF02Totals_AreaWeightAvgTotalArea" type="comp:AreaWeightAvgTotalArea">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result = SUM ALL (ICalc2UF02_FloorTypeTotalArea)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="ICalc2UF03Totals_AreaWeightAvgUfactorRequiredAll" type="comp:AreaWeightAvgUfactorRequiredAll">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result = (SUM ALL ( ICalc2UF02_FloorTypeTotalArea x ICalc2UF03_AreaWeightAvgUfactorRequired)) / ICalc2UF02Totals_AreaWeightAvgTotalArea</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="ICalc2UF04Totals_AreaWeightAvgUfactorAll" type="comp:AreaWeightAvgUfactorAll">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result = (SUM ALL (ICalc2UF02_FloorTypeTotalArea x AreaWeightAvgUfactor)) / ICalc2UF02Totals_AreaWeightAvgTotalArea where AreaWeightAvgUfactor for row is the property that IsPresent: ICalc2UF04Framed_AreaWeightAvgUfactor Or ICalc2UF04SIPS_AreaWeightAvgUfactor</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="ICalc2UF05_DoesAreaWeightAvgComplyUfactor" type="comp:DoesAreaWeightAvgComplyUfactor">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Compliance Results Using Area-Weighted Calculation Option</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: 
If ICalc2UF03Totals_AreaWeightAvgUfactorRequiredAll GTE ICalc2UF04Totals_AreaWeightAvgUfactorAll result = true display term COMPLIES; 
Else result = false display term DOES NOT COMPLY
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_J" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>J. EXTERIOR DOOR SCHEDULE</d:b>
               </xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required If B02AddDoor_ComponentApplies  Or B02NewDoor_ComponentApplies == true</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="J_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i> This table demonstrates compliance with prescriptive exterior door requirements in 
                              <d:t24ref h="140.3(a)7"/> for new construction or additions. Doors which are being replaced (alterations) do not need to be documented in this table because there are no Title 24, Part 6 requirements that apply.  Exterior doors separate conditioned space from unconditioned space or from ambient air.  Doors that are more than 25% glass in area are considered Glazed Doors and should be documented on Table K with fenestration per Table B.  </d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableJdoors">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="J01_DoorName" type="env:DoorName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Tag/Plan Detail ID</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User enters text</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="J02_DoorDescription" type="env:DoorDescription">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name/Description</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="J03_TopLevelOccupancy">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Occupancy Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list: Nonresidential, ResidentialNR,    RelocableSchoolBuildingAllZones</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="ResidentialNR">High-rise Res/Hotel/ Motel</dtyp:displayterm>
                                             <dtyp:displayterm value="Nonresidential">Nonresidential/ Relocatable 1 CZ</dtyp:displayterm>
                                             <dtyp:displayterm value="RelocableSchoolBuildingAllZones">Relocatable all CZ</dtyp:displayterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="com:TopLevelOccupancy">
                                             <xsd:enumeration value="Nonresidential"/>
                                             <xsd:enumeration value="RelocableSchoolBuildingAllZones"/>
                                             <xsd:enumeration value="ResidentialNR"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="J04_DoorOperation" type="env:DoorOperation">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Door Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list: Swinging, NonSwinging display term Non-swinging</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="J05_DoorConstructionType" type="env:DoorConstructionType">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Door Insulation</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list: 
UninsulatedSingleLayerMetal display term Uninsulated single-layer metal, 
UninsulatedDoubleLayerMetal display term Uninsulated double-layer metal, 
InsulatedSwingingMetal display term Insulated metal swing doors, 
WoodThick display term Wood 
                                             <d:gte/>1 3/4 ", 
WoodOther display term Any other wood door,  UninsulatedSingleLayerRollupMetal display term Uninsulated single-layer metal rollup door, 
InsulatedSingleLayerSectionalMetal display term Insulated single layer metal sectional doors with EPS</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="J06_UFactorLimit" type="com:UFactorLimit">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Maximum Allowed U-factor</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated field: 
If J03_TopLevelOccupancy ==   Nonresidential Or ResidentialNR
If J04_DoorOperation == Swinging result = 0.70; 
Else If A03_ClimateZoneNumber == 1 Or 16 result = 0.50; Else result = 1.45;
End If
Else If J03_TopLevelOccupancy ==  RelocableSchoolBuildingAllZones
If J04_DoorOperation == Swinging result = 0.70; 
Else result = 0.50
End If
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="J07_UfactorJA451" type="comp:UfactorJA451">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">U-factor per Design</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated field: 
Switch (J05_DoorConstructionType) 
case UninsulatedSingleLayerMetal: result = 1.45; 
case UninsulatedDoubleLayerMetal: result = 0.70; 
case InsulatedSwingingMetal: result = 0.50; 
case WoodThick: result = 0.50;  
case WoodOther: result = 0.60;  
case UninsulatedSingleLayerRollupMetal: result = 1.45;  
case InsulatedSingleLayerSectionalMetal: result = 0.179
End Switch</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_K" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>K. FENESTRATION AND GLAZED DOOR  SCHEDULE</d:b>
               </xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required If B02NewFen_ComponentApplies, B02AddFen_ComponentApplies Or B02AltFen_ComponentApplies == true</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="K_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This table demonstrates compliance with prescriptive fenestration requirements in 
                              <d:t24ref h="140.3(a)5"/> for new constructions or additions, or 
                              <d:t24ref h="140.1(b)2A"/> for alterations.  Exterior doors that are more than one-half glass in area are considered Glazed Doors and should be documented on this table with fenestration. </d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="K01Label" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Indicate fenestration types included in the project:<d:sup>1</d:sup>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="K01VerticalAlt_FenestrationTypeApplies" type="comp:FenestrationTypeApplies">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Vertical (alterations)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input: If checked result = true; Else result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="K01VerticalNew_FenestrationTypeApplies" type="comp:FenestrationTypeApplies">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Vertical (new)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input: If checked result = true; Else result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="K01Skylights_FenestrationTypeApplies" type="comp:FenestrationTypeApplies">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Skylights</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input: If checked result = true; Else result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="K01GlazedDoors_FenestrationTypeApplies" type="comp:FenestrationTypeApplies">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Glazed Doors (new only)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input: If checked result = true; Else result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="KFootnote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i><d:sup>1</d:sup> FOOTNOTES: Fenestration types indicated above as "(new only)" do not have Title 24, Part 6 requirements for alterations.  New construction and additions do have requirements and should be clicked above and compliance demonstrated within this table.  </d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Kvert" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>Vertical Fenestration and Glazed Doors- Total Building &amp;amp; West Facing Area (New Construction &amp;amp; Additions Only)</d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required If K01VerticalNew_FenestrationTypeApplies == true Or K01GlazedDoors_FenestrationTypeApplies == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="TableKvert">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Kvert01_ExteriorWallName" type="env:ExteriorWallName">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Elevation Item Tag/ Description</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input text</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Kvert02_CardinalDirection">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Orientation (Azimuth)<d:sup>1</d:sup>
                                                   </xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from list: NorthFacing display term North Facing, SouthFacing display term South Facing, EastFacing display term East Facing, WestFacing display term West Facing</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="com:CardinalDirection">
                                                      <xsd:enumeration value="EastFacing"/>
                                                      <xsd:enumeration value="NorthFacing"/>
                                                      <xsd:enumeration value="SouthFacing"/>
                                                      <xsd:enumeration value="WestFacing"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:element name="Kvert03_ExteriorSurfaceGrossArea" type="env:ExteriorSurfaceGrossArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Gross Exterior Wall Area<d:sup>2</d:sup>
                                                   </xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input area</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Kvert04_DisplayPerimeter" type="env:DisplayPerimeter">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Display Perimeter Length<d:sup>2</d:sup>
                                                   </xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input perimieter</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Kvert05_PartitionWindowDoorArea" type="com:PartitionWindowDoorArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Vertical Fenestration Area per Design<d:sup>3</d:sup>
                                                   </xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input area</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="Kvert06_GlazingAreaLimitAll" type="env:GlazingAreaLimitAll">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Maximum Allowed Vertical Fenestration (ft<d:sup>2</d:sup>)- All Orientations</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated Field: result = Largest ((SUM ALL (0.40 x Kvert03_ExteriorSurfaceGrossArea)), (SUM ALL (6 x Kvert04_DisplayPerimeter)))</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Kvert07_TotalGlazingAreaAllOrientations" type="env:TotalGlazingAreaAllOrientations">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Total Vertical Fenestration (ft<d:sup>2</d:sup>) per design- All Orientations</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated Field: result = SUM ALL (Kvert05_PartitionWindowDoorArea)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Kvert08_GlazingAreaLimitWest" type="env:GlazingAreaLimitWest">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Maximum Allowed Vertical Fenestration (ft<d:sup>2</d:sup>)- West Facing</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated Field:  For rows where Kvert02_CardinalDirection == WestFacing result = Largest ((SUM ALL  (0.40 x Kvert03_ExteriorSurfaceGrossArea)), (SUM ALL (6 x Kvert04_DisplayPerimeter)))</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Kvert09_TotalGlazingAreaWest" type="env:TotalGlazingAreaWest">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Total Vertical Fenestration (ft<d:sup>2</d:sup>) per design- West Facing</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated Field:  For rows where Kvert02_CardinalDirection == WestFacing result = SUM ALL (Kvert05_PartitionWindowDoorArea)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="KvertFootNote1" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                                 <xsd:documentation source="AdditionalRequirements">
                                    <d:i><d:sup>1</d:sup>FOOTNOTES: Orientation between 226 deg and 315 deg are considered "West Facing".  A diagram has been provided in the 
                                       <d:t24table/>Nonresidential Compliance Manual for visual reference.</d:i>
                                 </xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="KvertFootNote2" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                                 <xsd:documentation source="AdditionalRequirements">
                                    <d:i><d:sup>2</d:sup>Do not include demising walls per 
                                       <d:t24ref h="140.3(a)5"/> .</d:i>
                                 </xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="KvertFootNote3" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                                 <xsd:documentation source="AdditionalRequirements">
                                    <d:i><d:sup>3</d:sup> Includes glazed door fenestration area .</d:i>
                                 </xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Kvertperf" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>Vertical Fenestration And Glazed Doors- U-factor, Solar Heat Gain Coefficient (RSHGC/ SHGC), Visible Transmittance (VT)</d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required If K01VerticalAlt_FenestrationTypeApplies  Or K01VerticalNew_FenestrationTypeApplies   Or K01GlazedDoors_FenestrationTypeApplies == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Kvertperf01_UseInAreaWeightedAveUfactor" type="comp:UseInAreaWeightedAveUfactor">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Calculate Area-Weighted Average U-factor for Vertical Fenestration and Glazed Doors<d:sup>1</d:sup>
                                 </xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If checked result = true, Else result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Kvertperf02_UseInAreaWeightedAveSHGC" type="comp:UseInAreaWeightedAveSHGC">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Calculate Area-Weighted Average (R)SHGC for Vertical Fenestration and Glazed Doors<d:sup>1</d:sup>
                                 </xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If checked result = true, Else result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Kvertperf03_UseInAreaWeightedAveVT" type="comp:UseInAreaWeightedAveVT">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Calculate Area-Weighted Average VT for Vertical Fenestration and Glazed Doors<d:sup>1</d:sup>
                                 </xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If checked result = true, Else result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="TableKvertperf">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Kvertperf04_FenestrationName" type="env:FenestrationName">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Tag/Plan Detail ID</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Kvertperf05_FenestrationProductType">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Fenestration Type</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from dropdown FixedWindow display term Fixed window, OperableWindow display term Operable window, StoreFront display term Storefront, CurtainWall display term Curtain wall, ChromogenicFen display term Chromogenic Glazing, DemisingWallFen display term Fenestration in Demising Wall, GlazedDoor display term Glazed Door</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="env:FenestrationProductType">
                                                      <xsd:enumeration value="ChromogenicFen"/>
                                                      <xsd:enumeration value="CurtainWall"/>
                                                      <xsd:enumeration value="DemisingWallFen"/>
                                                      <xsd:enumeration value="FixedWindow"/>
                                                      <xsd:enumeration value="GlazedDoor"/>
                                                      <xsd:enumeration value="OperableWindow"/>
                                                      <xsd:enumeration value="StoreFront"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:element name="Kvertperf06Occup">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"/>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:choice>
                                                      <xsd:element name="Kvertperf06">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Occupancy &amp;amp; Status</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">If  Kvertperf05_FenestrationProductType == ChromogenicFen result is BlankGrayedOut stored in NotApplicableMessage;  
Else User selects from dropdown list 
End if
 
If Kvertperf05_FenestrationProductType == GlazedDoor
list only includes TopLevelOccupancy values Nonresidential, ResidentialNR and RelocableSchoolBuildingAllZones 
 
Else the list is generated from display terms for the two properties as follows: 
 
TopLevelOccupancy values Nonresidential, ResidentialNR and RelocableSchoolBuildingAllZones are listed 4 times, once for each of these
 
FenestrationModification values whose display terms are appended:
 AllNew, ReplaceGT150ft2, AddGT50ft2 and AddLTE50ft2;  
 
TopLevelOccupancy value OtherOccupancy is listed once for FenestrationModification value  ReplaceLTE150ft2 whose display terms are appended</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:complexType>
                                                            <xsd:sequence>
                                                               <xsd:element name="Kvertperf06_TopLevelOccupancy">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText"/>
                                                                     <xsd:documentation source="CalculationsAndRules">User selects occupancy type with the appropriate status value
 from list:  Nonresidential display term Nonresidential/ Relocatable 1 CZ,  ResidentialNR display term High-rise Res/Hotel/ Motel, RelocableSchoolBuildingAllZones display term Relocatable all CZ, OtherOccupancy display term Any Occupancy</xsd:documentation>
                                                                     <xsd:appinfo source="displayterm">
                                                                        <dtyp:displayterm value="ResidentialNR">High-rise Res/Hotel/ Motel</dtyp:displayterm>
                                                                        <dtyp:displayterm value="Nonresidential">Nonresidential/ Relocatable 1 CZ</dtyp:displayterm>
                                                                        <dtyp:displayterm value="RelocableSchoolBuildingAllZones">Relocatable all CZ</dtyp:displayterm>
                                                                        <dtyp:displayterm value="OtherOccupancy">Any Occupancy</dtyp:displayterm>
                                                                     </xsd:appinfo>
                                                                  </xsd:annotation>
                                                                  <xsd:simpleType>
                                                                     <xsd:restriction base="com:TopLevelOccupancy">
                                                                        <xsd:enumeration value="Nonresidential"/>
                                                                        <xsd:enumeration value="OtherOccupancy"/>
                                                                        <xsd:enumeration value="RelocableSchoolBuildingAllZones"/>
                                                                        <xsd:enumeration value="ResidentialNR"/>
                                                                     </xsd:restriction>
                                                                  </xsd:simpleType>
                                                               </xsd:element>
                                                               <xsd:choice maxOccurs="1">
                                                                  <xsd:element name="Kvertperf06_FenestrationModification" type="env:FenestrationModification">
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText"/>
                                                                        <xsd:documentation source="CalculationsAndRules">User selects status value with the appropriate occupancy type from list:  
AllNew display term New 
ReplaceGT150ft2 display term Alt. (Replacement 
                                                                           <d:gt/> 150ft<d:sup>2</d:sup>), 
ReplaceLTE150ft2 display term Alt. (Replacement 
                                                                           <d:lte/> 150ft<d:sup>2</d:sup>), 
AddGT50ft2 display term Alt. (Adding New 
                                                                           <d:gt/> 50ft<d:sup>2</d:sup>) 
AddLTE50ft2 display term Alt. (Adding New 
                                                                           <d:lte/> 50ft<d:sup>2</d:sup>);</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Kvertperf06mod_NotApplicableMessage">
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText"/>
                                                                        <xsd:documentation source="CalculationsAndRules">If Kvertperf05_FenestrationProductType == GlazedDoor
result = BlankGrayedOut
End If</xsd:documentation>
                                                                     </xsd:annotation>
                                                                     <xsd:simpleType>
                                                                        <xsd:restriction base="comp:NotApplicableMessage">
                                                                           <xsd:enumeration value="BlankGrayedOut"/>
                                                                        </xsd:restriction>
                                                                     </xsd:simpleType>
                                                                  </xsd:element>
                                                               </xsd:choice>
                                                            </xsd:sequence>
                                                         </xsd:complexType>
                                                      </xsd:element>
                                                      <xsd:element name="Kvertperf06_NotApplicableMessage">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Occupancy &amp;amp; Status</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">If  Kvertperf05_FenestrationProductType == ChromogenicFen result = BlankGrayedOut stored in NotApplicableMessage
End If</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="comp:NotApplicableMessage">
                                                               <xsd:enumeration value="BlankGrayedOut"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                   </xsd:choice>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Kvertperf07_ComplianceMethodSHGC_VT">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">(R)SHGC Compliance Method</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">User selects from list customized as follows: 
If Kvertperf06_FenestrationModification == AllNew  Or
Kvertperf05_FenestrationProductType == GlazedDoor  list: 
Table140_3BCD display term Table 140.3-B/C/D 
NoOverHang display term 1st flr display perimeter- no overhang; 
 
Else If Kvertperf06_FenestrationModification == AddGT50ft2 list: 
Table140_3BCD display term Table 140.3-B/C/D; 
 
Else If Kvertperf06_FenestrationModification == ReplaceGT150ft2 list 
Table141_0A display term Table141.0-A
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:ComplianceMethodSHGC_VT">
                                                         <xsd:enumeration value="NoOverHang"/>
                                                         <xsd:enumeration value="Table140_3BCD"/>
                                                         <xsd:enumeration value="Table141_0A"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                                <xsd:element name="Kvertperf07_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">(R)SHGC Compliance Method</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Kvertperf05_FenestrationProductType == ChromogenicFen Or DemisingWallFen Or If Kvertperf06_FenestrationModification == ReplaceLTE150ft2 Or AddLTE50ft2  result = BlankGrayedOut
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Kvertperf08_ComplianceMethodSHGC_VT">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">VT Compliance Method</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">User selects from list customized as follows: 
If Kvertperf06_FenestrationModification == AllNew  Or 
If Kvertperf06_TopLevelOccupancy == Nonresidential Or ResidentialNR And  
Kvertperf05_FenestrationProductType == GlazedDoor
 list: 
Table140_3BCD display term Table 140.3-B/C/D,  
Equation140_3B display term Equation 140.3-B, 
NA_SkylitOverlap display term NA: Skylit &amp;amp; Sidelit Daylit zones overlap;  
 
Else If Kvertperf06_FenestrationModification == AddGT50ft2 Or ReplaceGT150ft2  list: 
Table140_3BCD display term Table 140.3-B/C/D;
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:ComplianceMethodSHGC_VT">
                                                         <xsd:enumeration value="Equation140_3B"/>
                                                         <xsd:enumeration value="NA_SkylitOverlap"/>
                                                         <xsd:enumeration value="Table140_3BCD"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                                <xsd:element name="Kvertperf08_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">VT Compliance Method</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Kvertperf05_FenestrationProductType == ChromogenicFen Or DemisingWallFen Or 
 
If Kvertperf06_FenestrationModification == ReplaceLTE150ft2 Or AddLTE50ft2  Or 
 
If Kvertperf06_TopLevelOccupancy == RelocableSchoolBuildingAllZones And  Kvertperf06_FenestrationModification == AllNew Or AddGT50ft2  Or
 
If Kvertperf06_TopLevelOccupancy == RelocableSchoolBuildingAllZones And  
Kvertperf05_FenestrationProductType == GlazedDoor
 
result = BlankGrayedOut
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Kvertperf09_FenestrationComplianceCalc" type="comp:FenestrationComplianceCalc">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Calculation Method for Performance Values per Design<d:sup>2</d:sup>
                                                      </xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">User selects from list: 
NFRC_Certified display term NFRC Certified, 
NA6DefaultCOG display term NA6 Default (COG)<d:sup>2</d:sup>, 
Table110_6Defaults display term 
                                                         <d:t24ref h="110.6"/> Defaults</xsd:documentation>
                                                      <xsd:appinfo source="displayterm">
                                                         <dtyp:displayterm value="NA6DefaultCOG">NA6 Default (COG)<d:sup>2</d:sup>
                                                         </dtyp:displayterm>
                                                      </xsd:appinfo>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Kvertperf09_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Calculation Method for Performance Values per Design<d:sup>2</d:sup>
                                                      </xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Kvertperf05_FenestrationProductType == ChromogenicFen result = BlankGrayedOut
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Kvertperf09a_OverhangForRSHGC" type="comp:OverhangForRSHGC">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Overhang used for RSHGC</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">User input If checked result = true;  Else result = false;
 
If result == true
D03_ExceptionalConditionMessageNRCCENVE +=  VerticalFenestrationOverhangsInK
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Kvertperf09a_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Overhang used for RSHGC</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Kvertperf05_FenestrationProductType == ChromogenicFen Or DemisingWallFen
Or If Kvertperf06_FenestrationModification == ReplaceLTE150ft2 Or AddLTE50ft2   
 result = BlankGrayedOut
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="Kvertperf10Label" minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Product Performance Unit</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Kvertperf10aLabel" minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">U-factor (max)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Kvertperf10bLabel" minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">(R)SHGC (max)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Kvertperf10cLabel" minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">VT (min)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Kvertperf11a_UFactorLimit" type="com:UFactorLimit">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Product Performance</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Kvertperf05_FenestrationProductType == GlazedDoor
result = 0.45 
Else If Kvertperf06_FenestrationModification == AllNew, AddGT50ft2 Or AddLTE50ft2  
If Kvertperf06_TopLevelOccupancy == Nonresidential Or ResidentialNR 
Switch (Kvertperf05_FenestrationProductType) 
case FixedWindow:  result = 0.36; 
case OperableWindow:  result = 0.46;  
case StoreFront:  case CurtainWall: case DemisingWallFen: result = 0.41; 
End Switch
 
Else If Kvertperf06_TopLevelOccupancy == RelocableSchoolBuildingAllZones result = 0.47;  
End if
 
Else If Kvertperf06_FenestrationModification == ReplaceGT150ft2 Or ReplaceLTE150ft2 
If A03_ClimateZoneNumber == 1, 2, 4 Or InRange (6, 16) result = 0.47; 
Else If A03_ClimateZoneNumber == 3 Or 5 result = 0.58;
End If
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Kvertperf11a_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Product Performance</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Kvertperf05_FenestrationProductType == ChromogenicFen result = BlankGrayedOut
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Kvertperf11b_SHGC_Limit" type="com:SHGC_Limit">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Product Performance</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Kvertperf07_ComplianceMethodSHGC_VT == NoOverHang result = 0.56;
Else If Kvertperf05_FenestrationProductType == GlazedDoor
result = 0.23; 
Else If Kvertperf07_ComplianceMethodSHGC_VT == Table140_3BCD  
If Kvertperf06_TopLevelOccupancy == Nonresidential Or ResidentialNR 
Switch (Kvertperf05_FenestrationProductType) 
case FixedWindow:  result = 0.25; 
case OperableWindow:  result = 0.22;  
case StoreFront:  case CurtainWall: result = 0.26; 
End Switch
 
If Kvertperf06_TopLevelOccupancy == RelocableSchoolBuildingAllZones result = 0.26;  
End If
 
Else  If Kvertperf07_ComplianceMethodSHGC_VT == Table141_0A 
If A03_ClimateZoneNumber == 1, 3, 5 Or 16 result = 0.41; 
Else If A03_ClimateZoneNumber == 2, 4 Or InRange (6,15) result = 0.31;  
End If
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Kvertperf11b_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Product Performance</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Kvertperf05_FenestrationProductType == ChromogenicFen Or DemisingWallFen Or If Kvertperf06_FenestrationModification == ReplaceLTE150ft2 Or AddLTE50ft2  result = BlankGrayedOut
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Kvertperf11c_VT_Limit" type="com:VT_Limit">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Product Performance</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Kvertperf05_FenestrationProductType == GlazedDoor
 If Kvertperf06_TopLevelOccupancy == Nonresidential  Or ResidentialNR result = 0.17
End If
 
Else If Kvertperf08_ComplianceMethodSHGC_VT == Table140_3BCD 
 If Kvertperf06_TopLevelOccupancy == Nonresidential Or ResidentialNR 
Switch (Kvertperf05_FenestrationProductType) 
case FixedWindow:  result = 0.42; 
case OperableWindow:  result = 0.32;  
case StoreFront:  case CurtainWall: result = 0.46; 
End Switch
End If
 
Else  If Kvertperf08_ComplianceMethodSHGC_VT == Equation140_3B result = Smallest (0.11 / ( Kvert07_TotalGlazingAreaAllOrientations / SUM ALL (Kvert03_ExteriorSurfaceGrossArea)),   0.40);
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Kvertperf11c_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Product Performance</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Kvertperf05_FenestrationProductType == ChromogenicFen Or DemisingWallFen Or
 
If Kvertperf06_FenestrationModification == ReplaceLTE150ft2 Or AddLTE50ft2  Or
 
Or If Kvertperf06_TopLevelOccupancy == RelocableSchoolBuildingAllZones and  Kvertperf06_FenestrationModification == AllNew Or AddGT50ft2  Or
 
If Kvertperf06_TopLevelOccupancy == RelocableSchoolBuildingAllZones And  
Kvertperf05_FenestrationProductType == GlazedDoor  Or
If Kvertperf08_ComplianceMethodSHGC_VT == NA_SkylitOverlap
 
 result = BlankGrayedOutresult = BlankGrayedOut
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Kvertperf12a_UFactor" type="com:UFactor">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Product Performance per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If  Kvertperf09_FenestrationComplianceCalc == NFRC_Certified  user input; 
Else If Kvertperf09_FenestrationComplianceCalc == NA6DefaultCOG result =  Kvertperf19a_UfactorNA6;  
Else If Kvertperf09_FenestrationComplianceCalc == Table110_6Defaults  result =  Kvertperf25a_Ufactor110Default
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Kvertperf12a_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Product Performance per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Kvertperf05_FenestrationProductType == ChromogenicFen result = BlankGrayedOut
end If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Kvertperf12b_SHGC" type="com:SHGC">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Product Performance per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If  Kvertperf09_FenestrationComplianceCalc == NFRC_Certified  
If Kvertperf09a_OverhangeForRSHGC == false
user input; 
Else
result = Kvertperf32_ RSHGC; 
 
Else If Kvertperf09_FenestrationComplianceCalc == NA6DefaultCOG  
If Kvertperf09a_OverhangeForRSHGC == true result = Kvertperf32_ RSHGC; 
Else result = Kvertperf19b_ SHGC_NA6 
End If
 
Else If Kvertperf09_FenestrationComplianceCalc == Table110_6Defaults  
If Kvertperf09a_OverhangeForRSHGC == true result =  Kvertperf32_ RSHGC ; 
Else result =  Kvertperf25b_SHGC_110Default except under these conditions when user needs to provide value: 
Condition If Kvertperf24_GlazingTint == TintedGlazing And Kvertperf23_FenestrationPanes == GlassBlock</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Kvertperf12b_NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Product Performance per Design</xsd:documentation>
