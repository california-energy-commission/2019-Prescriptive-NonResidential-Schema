                                                            <xsd:documentation source="FieldText">per JA4</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:complexType>
                                                            <xsd:choice>
                                                               <xsd:element name="ISIPS11_UfactorJA443Crawl" type="comp:UfactorJA443Crawl">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText"/>
                                                                     <xsd:documentation source="CalculationsAndRules">If ISIPS05_HasCrawlSpace == true use this choice:  
If ISIPS06_SplineType == Single2x  
If ISIPS07_PanelThickness == 6_5In  
If ISIPS08_CoreInsulation == R22 
Switch (ISIPS09_ContinuousInsulationNR) 
case R0: result = 0.033; case R2: result = 0.030; case R4: result = 0.029; case R6: result = 0.027; case R7: result = 0.026;  case R8: result = 0.026; case R10: result = 0.024;  
End Switch
 
Else If ISIPS08_CoreInsulation == R33 
Switch (ISIPS09_ContinuousInsulationNR) 
case R0: result = 0.024; case R2: result = 0.023; case R4: result = 0.022; case R6: result = 0.021; case R7: result = 0.021;  case R8: result = 0.020; case R10: result = 0.019;   
End Switch
End If
 
Else If ISIPS07_PanelThickness == 8_25In 
If ISIPS08_CoreInsulation == R28 
Switch (ISIPS09_ContinuousInsulationNR) 
case R0: result = 0.027; case R2: result = 0.026; case R4: result = 0.024; case R6: result = 0.023; case R7: result = 0.023; case R8: result = 0.022;  case R10: result = 0.021; 
End Switch
End If
 
Else If ISIPS07_PanelThickness == 10_25In  
If ISIPS08_CoreInsulation == R36 
Switch (ISIPS09_ContinuousInsulationNR) 
case R0: result = 0.023; case R2: result = 0.022; case R4: result = 0.021; case R6: result = 0.020; case R7: result = 0.019; case R8: result = 0.019; case R10: result = 0.018;   
End Switch
End If
End If
 
Else If ISIPS06_SplineType == Double2x  
If ISIPS07_PanelThickness == 6_5In  
If ISIPS08_CoreInsulation == R22 
Switch (ISIPS09_ContinuousInsulationNR) 
case R0: result = 0.034; case R2: result = 0.031; case R4: result = 0.029; case R6: result = 0.028; case R7: result = 0.027;  case R8: result = 0.026; case R10: result = 0.025;  
End Switch
 
Else If ISIPS08_CoreInsulation == R33 
Switch (ISIPS09_ContinuousInsulationNR) 
case R0: result = 0.026; case R2: result = 0.024; case R4: result = 0.023; case R6: result = 0.022; case R7: result = 0.021;  case R8: result = 0.021; case R10: result = 0.020;   
End Switch
End If
 
Else If ISIPS07_PanelThickness == 8_25In 
If ISIPS08_CoreInsulation == R28 
Switch (ISIPS09_ContinuousInsulationNR) 
case R0: result = 0.028; case R2: result = 0.026; case R4: result = 0.025; case R6: result = 0.024; case R7: result = 0.023; case R8: result = 0.023;  case R10: result = 0.022; 
End Switch
 
Else If ISIPS07_PanelThickness == 10_25In  
If ISIPS08_CoreInsulation == R36 
Switch (ISIPS09_ContinuousInsulationNR) 
case R0: result = 0.024; case R2: result = 0.022; case R4: result = 0.021; case R6: result = 0.020; case R7: result = 0.020; case R8: result = 0.020; case R10: result = 0.019;  
End Switch
End If
End If
 
Else If ISIPS06_SplineType == I_Joist  
If ISIPS07_PanelThickness == 6_5In  
If ISIPS08_CoreInsulation == R22 
Switch (ISIPS09_ContinuousInsulationNR) 
case R0: result = 0.032; case R2: result = 0.030; case R4: result = 0.028; case R6: result = 0.027; case R7: result = 0.026  case R8: result = 0.025; case R10: result = 0.024;  
End Switch
 
Else If ISIPS08_CoreInsulation == R33 
Switch (ISIPS09_ContinuousInsulationNR) 
case R0: result = 0.024; case R2: result = 0.023; case R4: result = 0.022; case R6: result = 0.021; case R7: result = 0.020;  case R8: result = 0.020; case R10: result = 0.019;   
End Switch
 
Else If ISIPS07_PanelThickness == 8_25In 
If ISIPS08_CoreInsulation == R28 
Switch (ISIPS09_ContinuousInsulationNR) 
case R0: result = 0.027; case R2: result = 0.025; case R4: result = 0.024; case R6: result = 0.023; case R7: result = 0.022; case R8: result = 0.022;  case R10: result = 0.021; 
End Switch
 
Else If ISIPS07_PanelThickness == 10_25In  
If ISIPS08_CoreInsulation == R36 
Switch (ISIPS09_ContinuousInsulationNR) 
case R0: result = 0.022; case R2: result = 0.021; case R4: result = 0.020; case R6: result = 0.019; case R7: result = 0.019; case R8: result = 0.019; case R10: result = 0.018;
End Switch
End If
End If</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
