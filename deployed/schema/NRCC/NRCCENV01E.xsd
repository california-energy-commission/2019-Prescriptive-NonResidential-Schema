End Switch
 
Else If ISIPS07_PanelThickness == 10_25In  
If ISIPS08_CoreInsulation == R36 
Switch (ISIPS09_ContinuousInsulationNR) 
case R0: result = 0.028; case R2: result = 0.026; case R4: result = 0.025; case R6: result = 0.024; case R7: result = 0.023; case R8: result = 0.022; case R10: result = 0.021;  
End Switch
End If
 
Else If ISIPS06_SplineType == I_Joist  
If ISIPS07_PanelThickness == 6_5In  
If ISIPS08_CoreInsulation == R22 
Switch (ISIPS09_ContinuousInsulationNR) 
case R0: result = 0.040; case R2: result = 0.037; case R4: result = 0.034; case R6: result = 0.032; case R7: result = 0.031  case R8: result = 0.030; case R10: result = 0.028;  
End Switch
 
Else If ISIPS08_CoreInsulation == R33 
Switch (ISIPS09_ContinuousInsulationNR) 
