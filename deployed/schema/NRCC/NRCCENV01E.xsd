               <xsd:documentation source="FieldText">
                  <d:b>F. ROOF ASSEMBLY SCHEDULE</d:b>
               </xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required If B02NewRoof_ComponentApplies, B02AddRoof_ComponentApplies Or B02AltRoof_ComponentApplies == true</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="F_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This table demonstrates compliance for prescriptive roof assembly requirements in 
                              <d:t24ref h="140.3(a)1B"/> for new construction or additions, or  
                              <d:t24ref h="141.0(b)2Biii"/> for alterations,  </d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F01Label" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Indicate roof types included in the project:</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F01Framed_RoofTypeApplies" type="comp:RoofTypeApplies">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Framed</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input: If checked result = true; Else result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
