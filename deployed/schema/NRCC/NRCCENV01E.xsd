                  <xsd:element name="FCalc1UF" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>Area-Weighted Average U-factor Compliance Calculation for Framed/ SIPs/ Span Deck &amp;amp; Concrete/ Metal Panel Roofs</d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required If  Fframed01_UseInAreaWeightedAveUfactor,  FSIPS01_UseInAreaWeightedAveUfactor, Fspan01_UseInAreaWeightedAveUfactor Or Fmetalp01_UseInAreaWeightedAveUfactor  == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="TableFCalc1UF">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="FCalc1UF01_AreaWeightAvgType">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Roof Type</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated field: If any of the following conditions are true, a row is added and the result loaded into this field: 
 If Fframed01_UseInAreaWeightedAveUfactor == true  result = Framed;  
If  FSIPS01_UseInAreaWeightedAveUfactor == true, result = SIPS; 
If Fspan01_UseInAreaWeightedAveUfactor ==  true result = Concrete;  
If Fmetalp01_UseInAreaWeightedAveUfactor  == true; result = MetalPanel</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="comp:AreaWeightAvgType">
                                                      <xsd:enumeration value="Concrete"/>
                                                      <xsd:enumeration value="Framed"/>
                                                      <xsd:enumeration value="MetalPanel"/>
                                                      <xsd:enumeration value="SIPS"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:element name="FCalc1UF02_RoofTypeTotalArea" type="comp:RoofTypeTotalArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Total Area of Roof Type (ft<d:sup>2</d:sup>)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated field: For current row, 
Switch (FCalc1UF01_AreaWeightAvgRoofType) 
case Framed: result = SUM ALL (Fframed16_RoofNetArea);  
case SIPS:  result = SUM ALL (FSIPS16_RoofNetArea); 
case Concrete:  result = SUM ALL (Fspan15_RoofNetArea); 
case MetalPanel:  result = SUM ALL (Fmetalp13_RoofNetArea).
End Switch</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="FCalc1UF03to04Label" minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Area-weighted U-factor for Roof Type</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="FCalc1UF03_AreaWeightAvgRequiredUfactor" type="comp:AreaWeightAvgRequiredUfactor">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Required</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated field: 
Switch ( FCalc1UF01_AreaWeightAvgRoofType) 
case Framed: result = (SUM ALL (Fframed16_RoofNetArea x Fframed14_InsulationUFactorLimit)) / FCalc1UF02_RoofTypeTotalArea; 
 
case SIPS: (SUM ALL (FSIPS16_RoofNetArea x FSIPS14_InsulationUFactorLimit)) / FCalc1UF02_RoofTypeTotalArea;  
 
case Concrete: (SUM ALL (Fspan15_RoofNetArea x Fspan13_InsulationUFactorLimit)) / FCalc1UF02_RoofTypeTotalArea; 
 
case MetalPanel: (SUM ALL (Fmetalp13_RoofNetArea x Fmetalp11_InsulationUFactorLimit)) / FCalc1UF02_RoofTypeTotalArea;
End Switch</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="FCalc1UF04Framed_AreaWeightAvgUfactor" type="comp:AreaWeightAvgUfactor">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Designed</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">Calculated field: 
 
If FCalc1UF01_AreaWeightAvgRoofType == Framed result= (SUM ALL (Fframed16_RoofNetArea x Ufactor) ) / FCalc1UF02_RoofTypeTotalArea.  
End if
 
Ufactor for row is the property that IsPresent: 
Fframed15_UfactorJA4Other Or  
Fframed15_UfactorJA421_16OC Or  
Fframed15_UfactorJA421_24OC Or 
Fframed15_UfactorJA422_16OC Or 
Fframed15_UfactorJA422_24OC Or 
Fframed15_UfactorJA424_16OC or 
Fframed15_UfactorJA424_24OC Or 
Fframed15_UfactorJA425_16OC Or 
Fframed15_UfactorJA425_24OC</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="FCalc1UF04SIPS_AreaWeightAvgUfactor" type="comp:AreaWeightAvgUfactor">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Designed</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">Calculated field: 
 
If FCalc1UF01_AreaWeightAvgRoofType == SIPS result= (SUM ALL (FSIPS16_RoofNetArea x Ufactor) ) / FCalc1UF02_RoofTypeTotalArea.  
End if
 
Ufactor for row is the property that IsPresent: 
FSIPS15_UfactorJA4Other Or 
FSIPS15_UfactorJA423OSB Or 
FSIPS15_UfactorJA423Single or 
FSIPS15_UfactorJA423Double Or 
FSIPS15_UfactorJA423Ijoist Or 
FSIPS15_UfactorJA423Steel</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="FCalc1UF04Span_AreaWeightAvgUfactor" type="comp:AreaWeightAvgUfactor">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Designed</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">Calculated field: 
 
If FCalc1UF01_AreaWeightAvgRoofType == Concrete result= (SUM ALL (Fspan15_RoofNetArea x Ufactor) ) / FCalc1UF02_RoofTypeTotalArea.  
End If
Ufactor for row is the property that IsPresent: 
Fspan14_UfactorJA4Other Or 
Fspan14_UfactorJA426FireProof Or  
Fspan14_UfactorJA426NoFireProof</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="FCalc1UF04Metalp_AreaWeightAvgUfactor" type="comp:AreaWeightAvgUfactor">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Designed</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">Calculated field: 
 
If FCalc1UF01_AreaWeightAvgRoofType == MetalPanel result= (SUM ALL (Fmetalp13_RoofNetArea x Ufactor) ) / FCalc1UF02_RoofTypeTotalArea.  
End If
 
Ufactor for row is the property that IsPresent: 
Fmetalp12_ UfactorJA4Other Or 
Fmetalp12_UfactorJA429</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                             </xsd:choice>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="FCalc1UF02to04TotalsLabel" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">
                                    <d:b>Total for all Roof Types:</d:b>
                                 </xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FCalc1UF02Totals_AreaWeightAvgTotalArea" type="comp:AreaWeightAvgTotalArea">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result = SUM ALL (FCalc1UF02_RoofTypeTotalArea)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FCalc1UF03Totals_AreaWeightAvgUfactorRequiredAll" type="comp:AreaWeightAvgUfactorRequiredAll">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result = (SUM ALL ( FCalc1UF02_RoofTypeTotalArea x FCalc1UF03_AreaWeightAvgUfactorRequired)) / FCalc1UF02Totals_AreaWeightAvgTotalArea</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FCalc1UF04Totals_AreaWeightAvgUfactorAll" type="comp:AreaWeightAvgUfactorAll">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result = (SUM ALL ( FCalc1UF02_RoofTypeTotalArea x FCalc1UF04_AreaWeightAvgUfactor)) / FCalc1UF02Totals_AreaWeightAvgTotalArea</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FCalc1UF05_DoesAreaWeightAvgComplyUfactor" type="comp:DoesAreaWeightAvgComplyUfactor">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Compliance Results Using Area-Weighted Calculation Option</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: 
 
If FCalc1UF03Totals_AreaWeightAvgUfactorRequiredAll GTE FCalc1UF04Totals_AreaWeightAvgUfactorAll  result = true display term COMPLIES; 
 
Else result = false display term DOES NOT COMPLY
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="FCalc2UF" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>Area-Weighted Average U-factor Compliance Calculation for  Metal Building Roof</d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required If Fmetalb01_UseInAreaWeightedAveUfactor == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="FCalc2UF01_AreaWeightAvgType">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Roof Type</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result = MetalBuilding display term Metal Building Roof</xsd:documentation>
                              </xsd:annotation>
                              <xsd:simpleType>
                                 <xsd:restriction base="comp:AreaWeightAvgType">
                                    <xsd:enumeration value="MetalBuilding"/>
                                 </xsd:restriction>
                              </xsd:simpleType>
                           </xsd:element>
                           <xsd:element name="FCalc2UF02_RoofTypeTotalArea" type="comp:RoofTypeTotalArea">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Total Area of Roof Type (ft<d:sup>2</d:sup>)</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: SUM ALL (Fmetalb15_RoofNetArea)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FCalc2UF03to04Label" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Area-weighted U-factor for Roof Type</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FCalc2UF03_AreaWeightAvgRequiredUfactor" type="comp:AreaWeightAvgRequiredUfactor">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Required</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result = (SUM ALL (Fmetalb15_RoofNetArea x Fmetalb13_InsulationUFactorLimit)) / FCalc1UF02_RoofTypeTotalArea</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FCalc2UF04_AreaWeightAvgUfactor" type="comp:AreaWeightAvgUfactor">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Designed</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result= (SUM ALL (Fmetalb15_RoofNetArea x Ufactor) ) / FCalc1UF02_RoofTypeTotalArea.  
 
Ufactor for row is the property that IsPresent: 
Fmetalb14_UfactorJA4Other Or 
Fmetalb14_UfactorJA427Screw Or 
Fmetalb14_UfactorJA427SeamSingle Or 
Fmetalb14_UfactorJA427SeamDouble Or 
Fmetalb14_UfactorJA427Blocks</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FCalc2UF05_DoesAreaWeightAvgComplyUfactor" type="comp:DoesAreaWeightAvgComplyUfactor">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Compliance Results Using Area-Weighted Calculation Option</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: If FCalc2UF03_AreaWeightAvgRequiredUfactor GTE FCalc2UF04_AreaWeightAvgUfactor result = true display term COMPLIES; 
 
Else result = false display term DOES NOT COMPLY
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_G" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>G. RATED ROOFING MATERIAL (COOL ROOF)</d:b>
               </xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required If B02NewRoof_ComponentApplies, B02AddRoof_ComponentApplies,  Or B02AltRoofmat_ComponentApplies  == true</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="G_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This table demonstrates compliance with prescriptive roof material requirements in 
                              <d:t24ref h="140.3(a)1A"/> for new construction or additions, or 
                              <d:t24ref h="141.0(b)2B"/> for alterations.</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableGcoolroof">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="G01_RoofAssemblyName" type="env:RoofAssemblyName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Tag/Plan Detail ID</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G02_RoofDescription" type="env:RoofDescription">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name/Description</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G03_Status">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Status</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list: New display term New, ExistingAlteration display term Altered</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="com:Status">
                                             <xsd:enumeration value="ExistingAlteration"/>
                                             <xsd:enumeration value="New"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="G04_TopLevelOccupancy">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Occupancy Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list: Nonresidential, ResidentialNR,    RelocableSchoolBuildingAllZones</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="ResidentialNR">High-rise Res/Hotel/ Motel</dtyp:displayterm>
                                             <dtyp:displayterm value="Nonresidential">Nonresidential/ Relocatable 1 CZ</dtyp:displayterm>
                                             <dtyp:displayterm value="RelocableSchoolBuildingAllZones">Relocatable all CZ</dtyp:displayterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="com:TopLevelOccupancy">
                                             <xsd:enumeration value="Nonresidential"/>
                                             <xsd:enumeration value="RelocableSchoolBuildingAllZones"/>
                                             <xsd:enumeration value="ResidentialNR"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="G05_RoofSlope" type="env:RoofSlope">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Roof Slope</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="LowSlope">
                                                <d:lt/>2:12 (Low)</dtyp:displayterm>
                                             <dtyp:displayterm value="SteepSlope">
                                                <d:gte/> 2:12 (Steep)</dtyp:displayterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G06_RoofProductType" type="env:RoofProductType">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Roof Material</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list: 
ToBeDetermined display term To Be Determined, 
AsphaltShingles display term Asphalt shingles, 
StoneAggregateBallast display term Aggregate or Ballast, 
ModifiedBitumin display term Bitumen, 
FieldAppliedCoating display term Field applied coating, 
FluidAppliedMembrane display term Fluid applied membrane, 
MetalRoof display term Metal , 
MetalShakesShingles display term Metal shakes or shingles, 
RoofPavers display term Paver, 
PolymerCompositeSteepSlope display term Polymer or Composite , 
SinglePlyThermoplastic display term Single-Ply,  
SprayPolyurethaneFoam display term Spray polyurethane foam, 
ClayRoofTiles display term Tile or Slate, 
WoodShakesShingles display term Wood, 
MetalBdgRoof display term Metal Building</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="StoneAggregateBallast">Aggregate or Ballast</dtyp:displayterm>
                                             <dtyp:displayterm value="ModifiedBitumin">Bitumen</dtyp:displayterm>
                                             <dtyp:displayterm value="MetalRoof">Metal</dtyp:displayterm>
                                             <dtyp:displayterm value="RoofPavers">Paver</dtyp:displayterm>
                                             <dtyp:displayterm value="PolymerCompositeSteepSlope">Polymer or Composite</dtyp:displayterm>
                                             <dtyp:displayterm value="SinglePlyThermoplastic">Single-Ply</dtyp:displayterm>
                                             <dtyp:displayterm value="ClayRoofTiles">Tile or Slate</dtyp:displayterm>
                                             <dtyp:displayterm value="WoodShakesShingles">Wood</dtyp:displayterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="G07_CoolRoofComplianceMethod" type="comp:CoolRoofComplianceMethod">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Compliance Method</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User selects from list customized as follows: 
Always list: 
InitialSolarReflectanceThermaEmittance display term Solar Reflectance (Initial)/ Emittance, 
AgedSolarReflectanceThermaEmittance display term Solar Reflectance (Aged)/ Emittance, 
SolarReflectanceIndex display term Solar Reflectance Index (SRI), 
RoofSolarPV display term NA: Roof-integrated PV or Solar Thermal, and 
DemisingRoof display term NA: Demising Roof; 
 
Customize the rest of the list as follows: 
 
Switch ( G03_Status) 
case ExistingAlteration: also list 
MassRoofGTE25Lbs display term NA: Weight 
                                                <d:gte/> 25lb/ft<d:sup>2</d:sup> over membrane and
RoofAlteredLT2000Ft2 display term NA:
                                                <d:lte/> 50% roof area &amp;amp; 
                                                <d:lt/>2,000ft2 altered;   
 
In addition If G04_TopLevelOccupancy == Nonresidential also list: 
UfactorTradeOffAlterations display term U-factor Trade-off per Table 141.0-B;  
 
 
case New:
 If G05_RoofSlope == LowSlope also list  
MassRoofGTE25Lbs display term NA: Weight 
                                                <d:gte/> 25lb/ft<d:sup>2</d:sup> over membrane; 
 
If G04_TopLevelOccupancy == Nonresidential also list 
UfactorTradeOff display term U-factor Trade-off per Table 140.3; 
 
If A03_ClimateZoneNumber == 3 Or 5 also list 
WoodFrameLTEU034 display term NA: Wood Frame Roof U :
                                                <d:lte/> 0.034.</xsd:documentation>
                                             <xsd:appinfo source="displayterm">
                                                <dtyp:displayterm value="AgedSolarReflectanceThermaEmittance">Solar Reflectance (Aged)/ Emittance</dtyp:displayterm>
                                                <dtyp:displayterm value="InitialSolarReflectanceThermaEmittance">Solar Reflectance (Initial)/ Emittance</dtyp:displayterm>
                                                <dtyp:displayterm value="SolarReflectanceIndex">Solar Reflectance Index (SRI)</dtyp:displayterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="G07_NotApplicableMessage" type="comp:NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Compliance Method</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">For any of these conditions result = BlankGrayedOut; 
 
Condition: G05_RoofSlope == SteepSlope And G04_TopLevelOccupancy == ResidentialNR And G03_Status == New And A03_ClimateZoneNumber == 1 Or 16; 
 
Condition: G05_RoofSlope == SteepSlope And G04_TopLevelOccupancy == ResidentialNR And G03_Status == ExistingAlteration And A03_ClimateZoneNumber == 1 Or 16; 
 
 
Condition:  G05_RoofSlope == LowSlope And G04_TopLevelOccupancy == ResidentialNR And G03_Status == New And A03_ClimateZoneNumber == 12, 16 Or InRange(1,8); 
 
Condition: G05_RoofSlope == LowSlope And G04_TopLevelOccupancy == ResidentialNR And G03_Status == ExistingAlteration And A03_ClimateZoneNumber == 12, 16 Or InRange(1,9);</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="G08Label" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Required Minimum Material Performance</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="G08a_SolarReflectanceAgedLimit" type="env:SolarReflectanceAgedLimit">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Reflectance</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If G07_CoolRoofComplianceMethod == AgedSolarReflectanceThermaEmittance Or InitialSolarReflectanceThermaEmittance  
 
Switch ( G04_TopLevelOccupancy) 
case Nonresidential: 
If G05_RoofSlope == LowSlope And A03_ClimateZoneNumber InRange (1,16)  
result = 0.63; 
Else If G05_RoofSlope == SteepSlope  And A03_ClimateZoneNumber InRange (1,16) 
result = 0.20; 
End If
 
case ResidentialNR:  
If G05_RoofSlope == LowSlope 
If G03_Status == New And A03_ClimateZoneNumber == 9, 10, 11, 13, 14 Or 15 
result = 0.55; 
Else If G03_Status == ExistingAlteration And A03_ClimateZoneNumber == 10, 11, 13, 14 Or 15 
result = 0.55; 
End If
Else If G05_RoofSlope == SteepSlope And A03_ClimateZoneNumber InRange (2,15) 
result = 0.20; 
End If 
 
case RelocableSchoolBuildingAllZones:
 If G05_RoofSlope == LowSlope result = 0.63; 
Else If G05_RoofSlope == SteepSlope result = 0.20; 
End If
End Switch 
 
Else If G07_CoolRoofComplianceMethod == UfactorTradeOff Or  UfactorTradeOffAlterations 
 
If G10_UfactorTradeOffRange == LTE0_028 , LTE0_031, LTE0_037 result = 0.25; 
Else If  G10_UfactorTradeOffRange == 0_0281To0_029,  0_0311To0_033, Or 0_0371To0_039 result = 0.36;  
Else  If  G10_UfactorTradeOffRange == 0_0291To0_030, 0_0331To0_035, Or 0_0391To0_042  result = 0.46;  
Else If  G10_UfactorTradeOffRange == 0_0301To0_032, , 0_0351To0_038 Or 0_0421To0_045 result = 0.56; 
Else If  G10_UfactorTradeOffRange == LTE0_034  Or LTE0_042 result = 0.25; 
Else If  G10_UfactorTradeOffRange == 0_0341To0_035 Or 0_0421To0_044 result = 0.3; 
Else If  G10_UfactorTradeOffRange == 0_0351To0_037 Or 0_0441To0_047 result = 0.35; 
Else If  G10_UfactorTradeOffRange == 0_0371To0_039 Or 0_0471To0_051 result = 0.4; 
Else If  G10_UfactorTradeOffRange == 0_0391To0_041 Or 0_0511To0_055 result = 0.45; 
Else If  G10_UfactorTradeOffRange == 0_0411To0_044 Or 0_0551To0_060 result = 0.5;  
Else If  G10_UfactorTradeOffRange == 0_0441To0_048 Or 0_0601To0_066 result = 0.55; 
Else If  G10_UfactorTradeOffRange == 0_0481To0_052 Or 0_0661To0_075 result = 0.60
End If
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="G08a_NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Reflectance</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">For any of these conditions result = BlankGrayedOut; 
 
Condition: G07_CoolRoofComplianceMethod != AgedSolarReflectanceThermaEmittance Or InitialSolarReflectanceThermaEmittance  Or UfactorTradeOff Or  UfactorTradeOffAlterations;  
 
Condition: G05_RoofSlope == SteepSlope And G04_TopLevelOccupancy == ResidentialNR And G03_Status == New Or ExistingAlteration And A03_ClimateZoneNumber == 1 Or 16; 
 
Condition:  G05_RoofSlope == LowSlope And G04_TopLevelOccupancy == ResidentialNR And G03_Status == New And A03_ClimateZoneNumber == 12, 16 Or InRange(1,8); 
 
Condition: G05_RoofSlope == LowSlope And G04_TopLevelOccupancy == ResidentialNR And G03_Status == ExistingAlteration And A03_ClimateZoneNumber == 12, 16 Or InRange(1,9);</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="G08b_ThermalEmittanceLimit" type="com:ThermalEmittanceLimit">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Emittance</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Calculated field: 
Switch ( G04_TopLevelOccupancy) 
case Nonresidential: 
If G05_RoofSlope == LowSlope And A03_ClimateZoneNumber InRange (1,16)  
result = 0.75; 
Else If G05_RoofSlope == SteepSlope And A03_ClimateZoneNumber InRange (1,16) 
result = 0.75; 
End If
 
case ResidentialNR:  
If G05_RoofSlope == LowSlope 
If G03_Status == New And A03_ClimateZoneNumber == 9, 10, 11, 13, 14 Or 15 
result = 0.75; 
Else If G03_Status == ExistingAlteration And A03_ClimateZoneNumber == 10, 11, 13, 14 Or 15 result = 0.75; 
End If
Else If G05_RoofSlope == SteepSlope And A03_ClimateZoneNumber InRange (2,15) 
result = 0.75; 
End If
 
case RelocableSchoolBuildingAllZones: 
If G05_RoofSlope == LowSlope 
result = 0.75; 
Else If G05_RoofSlope == SteepSlope 
result = 0.75
End If
End Switch</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="G08b_NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Emittance</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">For any of these conditions result = BlankGrayedOut; 
 
Condition: G07_CoolRoofComplianceMethod != AgedSolarReflectanceThermaEmittance Or InitialSolarReflectanceThermaEmittance;
 
Condition: G05_RoofSlope == SteepSlope And G04_TopLevelOccupancy == ResidentialNR And G03_Status == New Or ExistingAlteration And A03_ClimateZoneNumber == 1 Or 16; 
 
Condition:  G05_RoofSlope == LowSlope And G04_TopLevelOccupancy == ResidentialNR And G03_Status == New And A03_ClimateZoneNumber == 12, 16 Or InRange(1,8); 
 
Condition: G05_RoofSlope == LowSlope And G04_TopLevelOccupancy == ResidentialNR And G03_Status == ExistingAlteration And A03_ClimateZoneNumber == 12, 16 Or InRange(1,9);</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="G08c_SolarReflectanceIndexLimit" type="env:SolarReflectanceIndexLimit">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">SRI</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Calculated field: 
 
If  G07_CoolRoofComplianceMethod == SolarReflectanceIndex Or If G04_TopLevelOccupancy != RelocableSchoolBuildingAllZones
 
Switch ( G04_TopLevelOccupancy) 
case Nonresidential: 
If G05_RoofSlope == LowSlope And A03_ClimateZoneNumber InRange (1,16)  
result = 75; 
Else If G05_RoofSlope == SteepSlope And A03_ClimateZoneNumber InRange (1,16) 
result = 16; 
End if
 
case ResidentialNR:  
If G05_RoofSlope == LowSlope 
If G03_Status == New And A03_ClimateZoneNumber == 9, 10, 11, 13, 14 Or 15
result = 64; 
Else If G03_Status == ExistingAlteration And A03_ClimateZoneNumber == 10, 11, 13, 14 Or 15 
result = 64;
End If
Else If G05_RoofSlope == SteepSlope And A03_ClimateZoneNumber InRange (2,15) 
result = 16; 
 
End If
End Switch
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="G08c_NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">SRI</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Calculated field:  Several conditions result in BlankGrayedOut:  
If  G07_CoolRoofComplianceMethod != SolarReflectanceIndex Or If G04_TopLevelOccupancy == RelocableSchoolBuildingAllZones
result = BlankGrayedOut stored in NotApplicableMessage
End If
Or 
If G04_TopLevelOccupancy ==  ResidentialNR 
If G05_RoofSlope == SteepSlope And G03_Status == New Or ExistingAlteration And A03_ClimateZoneNumber == 1 Or 16
result = BlankGrayedOut stored in NotApplicableMessage
 
Else If G04_TopLevelOccupancy == ResidentialNR:  
If G05_RoofSlope == LowSlope 
If G03_Status == New 
If A03_ClimateZoneNumber == 12, 16 Or InRange (1-8) 
result = BlankGrayedOut stored in NotApplicableMessage
End If
Else If G03_Status == ExistingAlteration
If A03_ClimateZoneNumber == 12, 16 Or InRange (1-9) 
result = BlankGrayedOut stored in NotApplicableMessage
End If
End If
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="G09Label" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Designed Material Performance</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="G09a_SolarReflectanceAged" type="com:SolarReflectanceAged">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Reflectance</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If G07_CoolRoofComplianceMethod == AgedSolarReflectanceThermaEmittance Or InitialSolarReflectanceThermaEmittance Or UfactorTradeOff Or  UfactorTradeOffAlterations   user input. 
 
 Else If G07_CoolRoofComplianceMethod == InitialSolarReflectanceThermaEmittance, value must be converted from initial reflectance value to aged using this formula: 
 
 result = 0.2 + Beta (result initial - 0.2) where Beta = 0.65 If  G06_RoofProductType == FieldAppliedCoating; Else Beta = 0.70.
End if</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="G09a_NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Reflectance</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If G07_CoolRoofComplianceMethod != AgedSolarReflectanceThermaEmittance And If G07_CoolRoofComplianceMethod != InitialSolarReflectanceThermaEmittance And If G07_CoolRoofComplianceMethod != UfactorTradeOff And If G07_CoolRoofComplianceMethod != UfactorTradeOffAlterations 
Or
If G08a_NotApplicableMessage == BlankGrayedOut
result = BlankGrayedOut
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="G09b_ThermalEmittance" type="com:ThermalEmittance">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Emittance</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If G07_CoolRoofComplianceMethod == AgedSolarReflectanceThermaEmittance Or InitialSolarReflectanceThermaEmittanceCondition Or UfactorTradeOff Or  UfactorTradeOffAlterations user input
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="G09b_NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Emittance</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If G07_CoolRoofComplianceMethod != AgedSolarReflectanceThermaEmittance And If G07_CoolRoofComplianceMethod != InitialSolarReflectanceThermaEmittance And If G07_CoolRoofComplianceMethod != UfactorTradeOff And If G07_CoolRoofComplianceMethod != UfactorTradeOffAlterations 
Or
If G08b_NotApplicableMessage == BlankGrayedOut
result = BlankGrayedOut
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="G09c_SolarReflectanceIndex" type="env:SolarReflectanceIndex">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">SRI</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If  G07_CoolRoofComplianceMethod == SolarReflectanceIndex And If G04_TopLevelOccupancy !=RelocableSchoolBuildingAllZones  user input
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="G09c_NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">SRI</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If  G07_CoolRoofComplianceMethod != SolarReflectanceIndex Or If G04_TopLevelOccupancy == RelocableSchoolBuildingAllZones Or
If G08c_NotApplicableMessage == BlankGrayedOut
 result = BlankGrayedOut stored in NotApplicableMessage
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="G10_UfactorTradeOffRange" type="env:UfactorTradeOffRange">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">U-factor of Assembly</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If G07_CoolRoofComplianceMethod == UfactorTradeOff Or  UfactorTradeOffAlterations Or WoodFrameLTEU034 User selects from list customized as follows:  
 
Switch (G03_Status) 
case New: 
If G06_RoofProductType == MetalBdgRoof list: 
LTE0_031 display term 
                                                <d:lte/>0.031, 
0_0311To0_033 display term 0.0311-0.033, 
0_0331To0_035 display term 0.0331-0.035, 
0_0351To0_038 display term 0.0351-0.038;
End If 
 
Else If G06_RoofProductType != MetalBdgRoof 
If A03_ClimateZoneNumber == 6 Or 7 list: 
LTE0_037 display term 
                                                <d:lte/>0.037, 
0_0371To0_039 display term 0.0371-0.039, 
0_0391To0_042 display term 0.0391-0.042, 
0_0421To0_045 display term 0.0421-0.045; 
End If
 
Else If A03_ClimateZoneNumber == InRange(1, 5) Or InRange( 8, 16) list: 
LTE0_028 display term 
                                                <d:lte/>0.028, 
0_0281To0_029 display term 0.0281-0.029, 
0_0291To0_030 display term 0.0291-0.030, 
0_0301To0_032 display term 0.0301-0.032;  
End If
 
 
case ExistingAlteration:  
If A03_ClimateZoneNumber == 1 Or InRange( 3, 9) list: 
LTE0_042 display term 
                                                <d:lte/>0.042, 
0_0421To0_044 display term 0.0421-0.044, 
0_0441To0_047 display term 0.0441-0.047, 
0_0471To0_051 display term 0.0471-0.051, 
0_0511To0_055 display term 0.0511-0.055, 
0_0551To0_060 display term 0.0551-0.060, 
0_0601To0_066 display term 0.0601-0.066, 
0_0661To0_075 display term 0.0661-0.075; 
End If
 
Else If A03_ClimateZoneNumber == InRange( 10, 16) list: 
LTE0_034 display term 
                                                <d:lte/>0.034, 
0_0341To0_035 display term 0.0341-0.035, 
0_0351To0_037 display term 0.0351-0.037, 
0_0371To0_039 display term 0.0371-0.039, 
0_0391To0_041 display term 0.0391-0.041, 
0_0411To0_044 display term 0.0411-0.044, 
0_0441To0_048 display term 0.0441-0.048, 
0_0481To0_052 display term 0.0481-0.052
End If
End Switch
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="G10_NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">U-factor of Assembly</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If any of these conditions are true
result = BlankGrayedOut
End If
 
Condition: G07_CoolRoofComplianceMethod != UfactorTradeOff And   UfactorTradeOffAlterations And WoodFrameLTEU034 
 
Condition: G05_RoofSlope == SteepSlope And G04_TopLevelOccupancy == ResidentialNR And G03_Status == New Or ExistingAlteration And A03_ClimateZoneNumber == 1 Or 16; 
 
Condition:  G05_RoofSlope == LowSlope And G04_TopLevelOccupancy == ResidentialNR And G03_Status == New And A03_ClimateZoneNumber == 12, 16 Or InRange(1,8); 
 
Condition: G05_RoofSlope == LowSlope And G04_TopLevelOccupancy == ResidentialNR And G03_Status == ExistingAlteration And A03_ClimateZoneNumber == 12, 16 Or InRange(1,9);</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_H" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>H. WALL ASSEMBLY SCHEDULE</d:b>
               </xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required If B02NewWall_ComponentApplies, B02AddWall_ComponentApplies Or B02AltWall_ComponentApplies == true</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="H_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This table demonstrates compliance with prescriptive wall assembly requirements in 
                              <d:t24ref h="140.3(a)2"/> and  
                              <d:t24ref h="140.3(a)3"/> for new constructions or additions, or mandatory wall assembly requirements in 
                              <d:t24ref h="141.0(b)1B"/> for alterations. </d:i>
                        </xsd:documentation>
                     </xsd:annotation>
