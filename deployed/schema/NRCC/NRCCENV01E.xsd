End Switch
End If
 
Else If ISIPS06_SplineType == Double2x  
If ISIPS07_PanelThickness == 6_5In  
If ISIPS08_CoreInsulation == R22 
Switch (ISIPS09_ContinuousInsulationNR) 
case R0: result = 0.043; case R2: result = 0.039; case R4: result = 0.036; case R6: result = 0.034; case R7: result = 0.032;  case R8: result = 0.031; case R10: result = 0.029;  
