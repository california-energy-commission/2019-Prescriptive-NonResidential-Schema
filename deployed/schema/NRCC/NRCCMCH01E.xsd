                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Equipment Type per Tables 
                                                <d:t24table>110.2</d:t24table> &amp;amp; 
                                                <d:t24table>Title 20</d:t24table>
                                             </xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If F02_HvacSystemType == SPV_ACHP, user selects from equipment values in SPV_ACHP choice</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="F03_UnitaryCondensersAC" type="hvac:UnitaryCondensersAC">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Equipment Type per Tables 
                                                <d:t24table>110.2</d:t24table> &amp;amp; 
                                                <d:t24table>Title 20</d:t24table>
                                             </xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If F02_HvacSystemType == UnitaryCondensersAC, user selects from equipment values in UnitaryCondensersAC choice</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="F03_UnitaryHeatPumps" type="hvac:UnitaryHeatPumps">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Equipment Type per Tables 
                                                <d:t24table>110.2</d:t24table> &amp;amp; 
                                                <d:t24table>Title 20</d:t24table>
                                             </xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If F02_HvacSystemType == UnitaryHeatPumps, user selects from equipment values in UnitaryHeatPumps choice</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
