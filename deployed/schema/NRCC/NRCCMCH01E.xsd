                                          <xsd:documentation source="FieldText">Name or Item Tag</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="F02_HvacSystemType" type="hvac:HvacSystemType">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Equipment Category per 
                                             <d:t24table>Tables 110.2</d:t24table>
                                          </xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from main equipment categories: ComputerRoomAC, FurnaceAC, FurnaceUnitHeater, HeatPumpAC, PTAC_PTHP, RoomAC_HP, SPV_ACHP, UnitaryCondensersAC, UnitaryHeatPumps, VariableRefrigerantFlow</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
