                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">GPM/HP</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="M06_TowerSkinTempMax" type="hvac:TowerSkinTempMax">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Maximum Skin Temp (
                                             <d:deg/>F)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="M07_TowerConductivity" type="hvac:TowerConductivity">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Conductivity</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="M08_MAlkalinity" type="hvac:MAlkalinity">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">M-Alkalinity</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="M09_CalciumHardness" type="hvac:CalciumHardness">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Calcium Hardness</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="M10_MagnesiumHardness" type="hvac:MagnesiumHardness">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Magnesium Hardness</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="M11_TowerTargetCycles" type="hvac:TowerTargetCycles">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Target Tower Cycles</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="M12_Tower_pH" type="hvac:Tower_pH">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Calculated pH @ Target Cycles</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated value:
 result = (LOG(M08_MAlkalinity * M11_TowerTargetCycles * 0.9 * 1.219) + 2.19) /0.55</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="M13_TowerSaturation_pH" type="hvac:TowerSaturation_pH">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">pH Saturation @ Target Cycles</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated value: 
result = (LOG(M07_TowerConductivity * M11_TowerTargetCycles * 0.8) * 0.1111) + 12.3 ) - (LOG( M09_CalciumHardness *  M11_TowerTargetCycles * M08_MAlkalinity *  M11_TowerTargetCycles) + (0.00915 * M06_TowerSkinTempMax))</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="M14_TowerLSI" type="hvac:TowerLSI">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Tower LSI Based on Calculated pH</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated value: 
result =  M12_Tower_pH - TowerSaturation_pH</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="M15_DoesCoolingTowerComply" type="comp:DoesCoolingTowerComply">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Complies</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated value: 
 If  M14_TowerLSI LTE 2.5 result = true display term Yes; 
Else result = false display term No
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
