<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.lmonte.com/besm/NRCCMCH01E" version="2019.0.001" xmlns="http://www.lmonte.com/besm/NRCCMCH01E" xmlns:altova="http://www.altova.com/xml-schema-extensions" xmlns:bld="http://www.lmonte.com/besm/bld" xmlns:com="http://www.lmonte.com/besm/com" xmlns:comp="http://www.lmonte.com/besm/comp" xmlns:d="http://www.lmonte.com/besm/d" xmlns:dtyp="http://www.lmonte.com/besm/dtyp" xmlns:env="http://www.lmonte.com/besm/env" xmlns:hvac="http://www.lmonte.com/besm/hvac" xmlns:lit="http://www.lmonte.com/besm/lit" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
   <xsd:import namespace="http://www.lmonte.com/besm/bld" schemaLocation="../base/NrPresBuilding.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/com" schemaLocation="../base/NrPresCommon.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/comp" schemaLocation="../base/NrPresCompliance.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/dtyp" schemaLocation="../base/DataTypes.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/env" schemaLocation="../base/NrPresEnvelope.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/hvac" schemaLocation="../base/NrPresHvac.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/lit" schemaLocation="../base/NrPresLighting.xsd"/>
   <xsd:element name="ComplianceDocumentPackage">
      <xsd:complexType>
         <xsd:sequence>
            <xsd:element name="DocID" minOccurs="0">
               <xsd:complexType>
                  <xsd:attribute name="doc" type="comp:ComplianceDocumentTagNR" fixed="NRCCMCHE"/>
                  <xsd:attribute name="docType" type="comp:ComplianceDocumentTypeNR" fixed="CERTIFICATE OF COMPLIANCE NR"/>
                  <xsd:attribute name="docTitle" type="comp:ComplianceDocumentTitleNR" fixed="Mechanical Systems"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Payload" type="comp:Payload"/>
            <xsd:element name="DocumentData">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="Header" type="comp:HeaderNRCC"/>
                     <xsd:element ref="nRCCMCH01E"/>
                     <xsd:element name="DeclarationsBlock" type="comp:DeclarationsNRCC" maxOccurs="unbounded"/>
                     <xsd:element name="Footer" type="comp:FooterNRCC"/>
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Report" type="xsd:base64Binary" minOccurs="0"/>
         </xsd:sequence>
         <xsd:attribute name="revision" use="required" type="xsd:string" fixed="rev 20190401"/>
         <xsd:attribute name="doc" use="required" type="comp:ComplianceDocumentTagNR" fixed="NRCCMCHE"/>
      </xsd:complexType>
   </xsd:element>
   <xsd:complexType name="NRCCMCH01E">
      <xsd:annotation>
         <xsd:documentation>Mechanical Systems</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Section_A" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>A. GENERAL INFORMATION</d:b>
               </xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="A_BeforeNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This document is used to demonstrate compliance for mechanical systems that are within the scope of the permit application and are demonstrating compliance using the prescriptive path outlined in 
                              <d:t24ref h="140.4"/>, or  
                              <d:t24ref h="141.0(b)2"/> for alterations .</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A01_ProjectCity" type="comp:ProjectCity">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Project Location (city)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A02_ClimateZoneNumber" type="com:ClimateZoneNumber">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Climate Zone</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input selected from 16 California climate zones</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03_Label" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Occupancy Types Within Project:</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03All_WholeBuildingOccupancy" type="com:WholeBuildingOccupancy" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This field accumulates all the occupany types in the project and is evaluated in B_Alert1 and B_Alert2 and PO2_RequiredNRCVMch24H</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03Office_OccupancyApplies" type="comp:OccupancyApplies">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Office (B)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input; If checked result = true; Else result = false.  
If result == true A03_WholeBuildingOccupancy +=   OfficeBuilding;
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03Guest_OccupancyApplies" type="comp:OccupancyApplies">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Hotel/ Motel Guest Rooms (R-1)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input; If checked result = true; Else result = false.  
If result == true A03_WholeBuildingOccupancy +=  HotelMotel
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03HRR_OccupancyApplies" type="comp:OccupancyApplies">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">High-Rise Residential (R-2/R-3)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input; If checked result = true; Else result = false.  
If result == true A03_WholeBuildingOccupancy += HighRiseResidential;  
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03Retail_OccupancyApplies" type="comp:OccupancyApplies">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Retail (M)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input; If checked result = true; Else result = false.
If result == true A03_WholeBuildingOccupancy += RetailBuilding;
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03School_OccupancyApplies" type="comp:OccupancyApplies">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">School (E)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input; If checked result = true; Else result = false. 
 If result == true A03_WholeBuildingOccupancy += SchoolBuilding;
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03Reloc_OccupancyApplies" type="comp:OccupancyApplies">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Relocatable Class Bldg (E)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input; If checked result = true; Else result = false.  
If result == true A03_WholeBuildingOccupancy += RelocatableSchoolBuilding;
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03Warehouse_OccupancyApplies" type="comp:OccupancyApplies">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Non-refrigerated Warehouse (S)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input; If checked result = true; Else result = false.  
If result == true A03_WholeBuildingOccupancy +=  Warehouse;
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03Health_OccupancyApplies" type="comp:OccupancyApplies">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Healthcare Facility (H)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input; If checked result = true; Else result = false.  
If result == true A03_WholeBuildingOccupancy += HospitalBuilding;
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03Other_OccupancyApplies" type="comp:OccupancyApplies">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Other (write in)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input; If checked result = true; Else result = false. 
 If result == true A03_WholeBuildingOccupancy += AllOthersBuildings;
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03Other1_TypeOtherDescription" type="com:TypeOtherDescription" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="CalculationsAndRules">This field is required If A03Other == true.  User input should describe the type of  occupancy.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A04_TotalConditionedFloorArea" type="com:TotalConditionedFloorArea">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Total Conditioned Floor Area</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A05_TotalUnconditionedFloorArea" type="com:TotalUnconditionedFloorArea">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Total Unconditioned Floor Area</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A06_AboveGradeStoryCount" type="env:AboveGradeStoryCount">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"># of Stories (Habitable  Above Grade)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_B" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>B. PROJECT SCOPE</d:b>
               </xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="B_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This table Includes mechanical systems or components that are within the scope of the permit application and are demonstrating compliance using the prescriptive path outlined in 
                              <d:t24ref h="140.4"/>, or  
                              <d:t24ref h="141.0(b)2"/> for alterations .</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B_Subheading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b> My project consists of (check all that apply)</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01_Label1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Air System(s)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01a_ProjectHasHeatingAirSystem" type="comp:ProjectHasHeatingAirSystem">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Heating Air System</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01b_ProjectHasCoolingAirSystem" type="comp:ProjectHasCoolingAirSystem">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Cooling Air System</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01_Label2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Mechanical Controls</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01c_ProjectHasControls" type="comp:ProjectHasControls">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Mechanical Controls</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B02_Label" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Wet System Components</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B02a_ProjectHasWaterEconomizer" type="comp:ProjectHasWaterEconomizer">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Water Economizer</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B02b_ProjectHasPumps" type="comp:ProjectHasPumps">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Pumps</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B02c_ProjectHasSystemPiping" type="comp:ProjectHasSystemPiping">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">System Piping</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B02d_ProjectHasCoolingTowers" type="comp:ProjectHasCoolingTowers">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Cooling Towers</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B02e_ProjectHasChillers" type="comp:ProjectHasChillers">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Chillers</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B02f_ProjectHasBoilers" type="comp:ProjectHasBoilers">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Boilers</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03_Label" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Dry System Components</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03a_ProjectHasAirEconomizer" type="comp:ProjectHasAirEconomizer">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Air Economizer</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03b_ProjectHasElectricHeat" type="comp:ProjectHasElectricHeat">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Electric Resistance Heat</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03c_ProjectHasFanSystems" type="comp:ProjectHasFanSystems">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Fan Systems</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03d_ProjectHasDuctwork" type="comp:ProjectHasDuctwork">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Ductwork</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03e_ProjectHasVentilation" type="comp:ProjectHasVentilation">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Ventilation</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03f_ProjectHasZonalSystems" type="comp:ProjectHasZonalSystems">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Zonal Systems/ Terminal Boxes</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B_Alert1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:b>
                              <d:i>ALERT! Healthcare facilities must be ventilated in accordance with Chapter 4 of the CMC as amended by OSHPD and do not need to show compliance with Title 24, Part 6 ventilation requirements in Table J.</d:i>
                           </d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Display this alert If A03_WholeBuildingOccupancy includes HospitalBuilding</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B_Alert2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:b>
                              <d:i>ALERT! Healthcare facilities are not required to comply with space-conditioning zone controls from 
                                 <d:t24ref h="140.4(d)"/> and do not need to show compliance with the  requirements in Table K.</d:i>
                           </d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Display this alert If A03_WholeBuildingOccupancy includes HospitalBuilding</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_C" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>C. COMPLIANCE RESULTS</d:b>
               </xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="C_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>Table C will indicate if the project data input into the compliance document is compliant with mechanical requirements.  This table is not editable by the user.   If this table says "DOES NOT COMPLY" or "COMPLIES with Exceptional Conditions" refer to Table D., or the table indicated as not compliant for guidance.</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C01ColumnLabel1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">System Summary 
                           <d:t24ref h="110.1"/>, 
                           <d:t24ref h="110.2"/>, 
                           <d:t24ref h="140.4"/>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C01ColumnLabel2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">(See Table F)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C01_CompliantSectionF" type="comp:CompliantSectionF">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"/>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: 
If All of the following conditions are true: 
Condition 1
If TableDrySizing IsPresent  Then for every row 
fields F01-F04 have data;  
 
Condition 2
If Table1DryEquipment IsPresent  Then for every row 
any field in range Fa01-Fa09 != BlankGrayedOut  has data And 
If Fa05 and Fa06 != BlankGrayedOut Then Fa05 LTE Fa06 And 
If Fa08 And Fa09 != BlankGrayedOut Then Fa08 LTE Fa09; 
 
Condition 3
If Table2DryEquipment IsPresent  Then for every row 
any field in range Fb01-Fb07 != BlankGrayedOut  has data And
 If Fb03 and Fb04  != BlankGrayedOut  Then Fb03 LTE Fb04 And 
If Fb06 And Fb07 != BlankGrayedOut Then Fb06 LTE Fb07; 
 
Condition 4
If TableBoiler IsPresent  Then  for every row 
any field in range Fc01 - Fc09 != BlankGrayedOut has data And 
If Fc05 and Fc06 != BlankGrayedOut Then Fc05 GTE Fc06; 
 
Condition 5
If TableChiller IsPresent  Then for every row  
any field in range Fd01 - Fd10 != BlankGrayedOut has data And 
If Fd06 And Fd07 != BlankGrayedOut Then Fd06 GTE Fd07;  
 
Condition 6
If TableHeatRejection IsPresent  Then for every row 
any field in range  Fe01 - Fe12 != BlankGrayedOut has data And 
If Fe05 and Fe06 != BlankGrayedOut Then Fe05 GTE Fe06; 
 
Condition 7
If TableElectricHeat IsPresent  Then for every row 
any field in range  Ff01 - Ff04 != BlankGrayedOut has data.  
 
Then result = true display term Yes;
Else  result = false display term No.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C01_NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"/>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: 
If Section_F NotPresent  result = BlankGrayedOut stored in NotApplicableMessage and field is grayed out  
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="C01AndLabel" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">AND</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C02ColumnLabel1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Pumps 
                           <d:t24ref h="140.4(k)"/>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C02ColumnLabel2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">(See Table G)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C02_CompliantSectionG" type="comp:CompliantSectionG">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"/>
                           <xsd:documentation source="CalculationsAndRules">Calculated field:  
If B02b_ProjectHasPumps == true Then for every row 
any field in range G01 - G08 != BlankGrayedOut has data 
Then result = true display term Yes; 
Else result = false display term No
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C02_NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"/>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: 
If B02b_ProjectHasPumps == false  result = BlankGrayedOut stored in NotApplicableMessage and field is grayed out
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="C02AndLabel" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">AND</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C03ColumnLabel1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Fans/ Economizers  
                           <d:t24ref h="140.4(c)"/>, 
                           <d:t24ref h="140.4(e)"/>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C03ColumnLabel2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">(See Table H)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C03_CompliantSectionH" type="comp:CompliantSectionH">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"/>
                           <xsd:documentation source="CalculationsAndRules">Calculated field:  
If B03c_ProjectHasFanSystems == true Or B03a_ProjectHasAirEconomizer== true Then for every row
any field in range H001-H06 != BlankGrayedOut has data 
And 
If TablePowerAdjustment IsPresent And for every row 
any field in range H08a-H08b != BlankGrayedOut has data And
for each fan system H06Total_FanSystemTotalHP LTE H08c_FanSystemMaxAllowedHP 
Then result = true display term Yes; 
Else result = false display term No.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C03_NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"/>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: 
If B03c_ProjectHasFanSystems == false And B03a_ProjectHasAirEconomizer== false  result =  BlankGrayedOut stored in NotApplicableMessage and field is grayed out  
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="C03AndLabel" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">AND</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C04ColumnLabel1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">System Controls 
                           <d:t24ref h="110.2"/>, 
                           <d:t24ref h="120.2"/>, 
                           <d:t24ref h="140.4(f)"/>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C04ColumnLabel2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">(See Table I)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C04_CompliantSectionI" type="comp:CompliantSectionI">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"/>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: 
 If B01a_ProjectHasHeatingAirSystem Or B01b_ProjectHasCoolingAirSystem Or B01c_ProjectHasControls == true  Then for every row
 
If fields I01-I09 have data And 
 
I05_ControlSystemShutOff And  I06_ControlSystemZone And I07_ControlSystemDR And  I08_ControlSystemTempReset != Other Or Exempt 
 
Or
 
I05_ControlSystemShutOff Or  I06_ControlSystemZone Or I07_ControlSystemDR Or  I08_ControlSystemTempReset == Other Or Exempt And I_Exception02_ ExceptionalConditionNote NotNull 
 
Then  result = true display term Yes; 
 
Else result = false display term No
 
End if
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C04_NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"/>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: 
If B01a_ProjectHasHeatingAirSystem And B01b_ProjectHasCoolingAirSystem And B01c_ProjectHasControls == false result = BlankGrayedOut stored in NotApplicableMessage and field is grayed out  
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="C04AndLabel" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">AND</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C05ColumnLabel1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Ventilation 
                           <d:t24ref h="120.1"/>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C05ColumnLabel2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">(See Table J)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C05_CompliantSectionJ" type="comp:CompliantSectionJ">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"/>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: 
If B03e_ProjectHasVentilation == true Then 
If J01_CalculationsAttached == true result = true display term Yes; 
 
Else for every row 
any field in range J04-J16 != BlankGrayedOut has data And 
for every system J18_DoesSystemVentialtionComply == true And 
J02_HiRiseDwellingUnitsincluded == false 
 
Or
 
any field in range J19-J25 And J27 has data And 
for every system J29_DoesVentMeetRequiredOA == true for all rows, 
 
result = true display term Yes; 
Else result = false display term No
End If
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C05_NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"/>
                           <xsd:documentation source="CalculationsAndRules">Calculated field:  
If B03e_ProjectHasVentilation == false result = BlankGrayedOut stored in NotApplicableMessage and field is grayed out  
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="C05AndLabel" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">AND</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C06ColumnLabel1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Terminal Box Controls 
                           <d:t24ref h="140.4(d)"/>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C06ColumnLabel2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">(See Table K)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C06_CompliantSectionK" type="comp:CompliantSectionK">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"/>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: 
If B03f_ProjectHasZonalSystems == true Then  for every row 
If any field in range K01-K11 != BlankGrayedOut has data And K12_ZonalControlComplies == true result = true display term Yes; 
Else result = false display term No
End If
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C06_NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"/>
                           <xsd:documentation source="CalculationsAndRules">Calculated field:  
If  B03f_ProjectHasZonalSystems == false result = BlankGrayedOut stored in NotApplicableMessage and field is grayed out 
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="C06AndLabel" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">AND</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C07ColumnLabel1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Distribution 
                           <d:t24ref h="120.3"/>, 
                           <d:t24ref h="140.4(l)"/>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C07ColumnLabel2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">(See Table L)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C07_CompliantSectionL" type="comp:CompliantSectionL">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"/>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: 
If B02c_ProjectHasSystemPiping == true  Then  for every row  
 
If L01_ PipeInsulationProtectionComplies == true And 
Either L07_PipeInsulationMinByDiameter LTE L09_PipeInsulationThickness Or 
L08_PipeInsulationMinimum LTE L09_PipeInsulationThickness Or L10_PipeInsulationException != NoException 
result = true; 
 
Else result = false;  
End If
 
If  B03d_ProjectHasDuctwork == true then 
If any field in range L11-L17 != BlankGrayedOut has data 
result = true display term Yes; 
Else result = false display term No.
End If
End If
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C07_NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"/>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: 
If  B02c_ProjectHasSystemPiping == false And B03d_ProjectHasDuctwork == false  result = BlankGrayedOut stored in NotApplicableMessage and field is grayed out
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="C07AndLabel" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">AND</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C08ColumnLabel1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Cooling Towers 
                           <d:t24ref h="110.2(e)2"/>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C08ColumnLabel2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">(See Table M)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C08_CompliantSectionM" type="comp:CompliantSectionM">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"/>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: 
If B02d_ProjectHasCoolingTowers == true Then 
If M01_CalculationasAttached == true 
result = true display term Yes; 
 
Else Ifor every row 
If fields M02-M14 have data And M15_DoesCoolingTowerComply == true 
result = true display term Yes; 
Else result = false display term No
End If
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C08_NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"/>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: 
If B02d_ProjectHasCoolingTowers == false  result = BlankGrayedOut stored in NotApplicableMessage and field is grayed out
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="C09_DoesFormComplyNRCC" type="comp:DoesFormComplyNRCC">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Compliance Results</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated field: 
Perform evaluation only for fields C01 - C08 != BlankGrayedOut:
 
If  C01_CompliantSectionF == false Or C02_CompliantSectionG ==  false Or  C03_CompliantSectionH ==  false Or C04_CompliantSectionI == false Or  C05_CompliantSectionJ ==  false Or  C06_CompliantSectionK ==  false Or  C07_CompliantSectionL == false Or  C08_CompliantSectionM == false
result = false  display term DOES NOT COMPLY;
 
Else If D03_  ExceptionalConditionMessageNRCCMCHE NotPresent 
result = Complies display term COMPLIES; 
 
Else If D03_  ExceptionalConditionMessageNRCCMCHE IsPresent result = CompliesExceptionalConditions result = true display term COMPLIES with Exceptional Conditions; 
 
Else result = DoesNotComply  display term DOES NOT COMPLY
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C10_MandatoryMeasuresComply" type="comp:MandatoryMeasuresComply">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b> Mandatory Measures Compliance (See Table Q for Details) </d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated field:  
If Q01_UsingMandMeasNoteBlock ==  true And Q02_ DocLocationMandMeasBlock NotNull; 
Or 
If Q01_UsingMandMeasNoteBlock == false And Q03a-m have data 
result = true display term COMPLIES;  
Else result = false display term DOES NOT COMPLY;
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_D" minOccurs="1">
