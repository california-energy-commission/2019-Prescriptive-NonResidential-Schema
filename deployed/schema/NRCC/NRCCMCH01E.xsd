                                       <xsd:element name="F03_ComputerRoomAC" type="hvac:ComputerRoomAC">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Equipment Type per Tables 
                                                <d:t24table>110.2</d:t24table> &amp;amp; 
                                                <d:t24table>Title 20</d:t24table>
                                             </xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If F02_HvacSystemType == ComputerRoomAC, user selects from equipment values in ComputerRoomAC choice</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="F03_FurnaceAC" type="hvac:FurnaceAC">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Equipment Type per Tables 
                                                <d:t24table>110.2</d:t24table> &amp;amp; 
                                                <d:t24table>Title 20</d:t24table>
                                             </xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If F02_HvacSystemType == FurnaceAC, user selects from equipment values in FurnaceAC  choice</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="F03_FurnaceUnitHeater" type="hvac:FurnaceUnitHeater">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Equipment Type per Tables 
                                                <d:t24table>110.2</d:t24table> &amp;amp; 
                                                <d:t24table>Title 20</d:t24table>
                                             </xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If F02_HvacSystemType == FurnaceUnitHeater, user selects from equipment values in FurnaceUnitHeater  choice</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="F03_HeatPumpAC" type="hvac:HeatPumpAC">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Equipment Type per Tables 
                                                <d:t24table>110.2</d:t24table> &amp;amp; 
                                                <d:t24table>Title 20</d:t24table>
                                             </xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If F02_HvacSystemType == HeatPumpAC, user selects from equipment values in HeatPumpAC choice</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="F03_PTAC_PTHP" type="hvac:PTAC_PTHP">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Equipment Type per Tables 
                                                <d:t24table>110.2</d:t24table> &amp;amp; 
                                                <d:t24table>Title 20</d:t24table>
                                             </xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If F02_HvacSystemType == PTAC_PTHP, user selects from equipment values in PTAC_PTHP choice</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="F03_RoomAC_HP" type="hvac:RoomAC_HP">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Equipment Type per Tables 
                                                <d:t24table>110.2</d:t24table> &amp;amp; 
                                                <d:t24table>Title 20</d:t24table>
                                             </xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If F02_HvacSystemType == RoomAC_HP, user selects from equipment values in RoomAC_HP choice</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
