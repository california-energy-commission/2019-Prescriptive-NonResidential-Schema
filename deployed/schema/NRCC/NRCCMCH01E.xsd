                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Equipment Type per Tables 
                                                <d:t24table>110.2</d:t24table> &amp;amp; 
                                                <d:t24table>Title 20</d:t24table>
                                             </xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If F02_HvacSystemType == VariableRefrigerantFlow, user selects from equipment values in VariableRefrigerantFlow choice</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
