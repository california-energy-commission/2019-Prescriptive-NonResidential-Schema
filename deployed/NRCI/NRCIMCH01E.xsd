                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Equipment Compliance</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If any user input field is blank
result = TableIncomplete  
Else If Pump04_PumpMotorHP  == Pump04cc_PumpMotorHP
result = ExactMatch
Else 
result = EqualBetter
End If
 
If Pump05_NotApplicableMessage NotPresent And  Pump05_PumpControlVF  != Pump05cc_PumpControlVF 
Or
Pump06_NotApplicableMessage NotPresent And Pump06_HeatPumpIsolation != Pump06cc_HeatPumpIsolation 
Or
Pump07_NotApplicableMessage NotPresent And Pump07_PumpControlVSD != Pump07cc_PumpControlVSD 
Or
Pump08_NotApplicableMessage NotPresent And Pump08_DifferentialPressureSensorRequired != Pump08cc_DifferentialPressureSensorRequired 
 
D03_ExceptionalConditionMessageNRCIMCHE += PumpsNoMatch
End If</xsd:documentation>
