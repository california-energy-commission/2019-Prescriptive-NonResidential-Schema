                                    <xsd:element name="Ventnr09_ComplianceResultNRCI" type="comp:ComplianceResultNRCI">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">System Compliance</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If any user input field is blank
result = TableIncomplete  
Else If Ventnr02_VentSystemOA LT Ventnr02cc_VentSystemOA 
Or
Ventnr03_NotApplicableMessage NotPresent And  Ventnr03_VentSystemTransferAir LT Ventnr03cc_VentSystemTransferAir 
Or
Ventnr06_NotApplicableMessage NotPresent And Ventnr06_ExhaustPerSpace LT Ventnr06cc_ExhaustPerSpace 
result = MayNotComply
 
Else If Ventnr02_VentSystemOA  == Ventnr02cc_VentSystemOA 
And
Ventnr03_NotApplicableMessage IsPresent Or  Ventnr03_VentSystemTransferAir   == Ventnr03cc_VentSystemTransferAir 
And
Ventnr4_NotApplicableMessage IsPresent Or  Ventnr04_AirFiltrationRequirements  == Ventnr04cc_AirFiltrationRequirements 
And
Ventnr06_NotApplicableMessage IsPresent Or Ventnr06_ExhaustPerSpace == Ventnr06cc_ExhaustPerSpace 
And 
Ventnr07_NotApplicableMessage IsPresent Or  Ventnr07_OccupantSensorVent  == Ventnr07cc_OccupantSensorVent 
And
Ventnr08_NotApplicableMessage IsPresent Or  Ventnr08_DemandControlVent   == Ventnr08cc_DemandControlVent 
result = ExactMatch
 
Else If Ventnr02_VentSystemOA GTE Ventnr02cc_VentSystemOA 
And
Ventnr03_NotApplicableMessage NotPresent And  Ventnr03_VentSystemTransferAir GTE Ventnr03cc_VentSystemTransferAir 
And
Ventnr06_NotApplicableMessage NotPresent And Ventnr06_ExhaustPerSpace GTE Ventnr06cc_ExhaustPerSpace 
 
result = EqualBetter
End If
 
If Ventnr07_NotApplicableMessage NotPresent And Ventnr07_OccupantSensorVent  != Ventnr07cc_OccupantSensorVent 
Or
Ventnr08_NotApplicableMessage NotPresent Or  Ventnr08_DemandControlVent   != Ventnr08cc_DemandControlVent 
    
D03_ExceptionalConditionMessageNRCIMCHE += VentNoMatch
End If</xsd:documentation>
