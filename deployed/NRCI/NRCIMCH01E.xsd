                                             <xsd:documentation source="FieldText">Supply Air CFM</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Ventres03cc_NotApplicableMessage IsPresent
result = Ventres03cc_NotApplicableMessage
End If</xsd:documentation>
                                          </xsd:annotation>
