                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="D01_ExceptionalConditionMessageNRCIMCHE" type="comp:ExceptionalConditionMessageNRCIMCHE" minOccurs="0" maxOccurs="unbounded">
