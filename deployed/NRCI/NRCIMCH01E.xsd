                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Equipment Compliance</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If any user input field is blank
result = TableIncomplete 
Else If Hrej02_HeatRejectionType != Hrej02cc_HeatRejectionType
Or
Hrej04_CoolingTowerCapacityRated IsPresent   LT Hrej04cc_CoolingTowerCapacityRated Or Hrej04_HeatRejectionCondenserCapacityRated IsPresent LT Hrej04cc_HeatRejectionCondenserCapacityRated  
result = MayNotComply
 
Else If Hrej02_HeatRejectionType ==  Hrej2cc_HeatRejectionType
And
Hrej03_NotApplicableMessage IsPresent Or Hrej03_HeatRejectionEquipmentCount has value ==  Hrej03cc_HeatRejectionEquipmentCount    
And 
Hrej06_NotApplicableMessage IsPresent Or Hrej06_HeatRejectionFanSpeedControl ==  Hrej06cc_HeatRejectionFanSpeedControl
And 
Hrej07_NotApplicableMessage IsPresent Or Hrej07_TowerFlowTurnDown ==  Hrej07cc_TowerFlowTurnDown 
And  
Hrej08_NotApplicableMessage IsPresent Or Hrej08_FanControlMultipleCells == Hrej08cc_FanControlMultipleCells
And
Hrej09_NotApplicableMessage IsPresent Or Hrej09_HasWaterEconomizerControls == Hrej09cc_HasWaterEconomizerControls
And
Hrej10_NotApplicableMessage IsPresent Or Hrej10_HasTempResetControls == Hrej10cc_HasTempResetControls
Then 
 
If Hrej04_NotApplicableMessage IsPresent Or Hrej04_CoolingTowerCapacityRated IsPresent  == Hrej04cc_CoolingTowerCapacityRated Or
Hrej04_HeatRejectionCondenserCapacityRated IsPresent == Hrej04cc_HeatRejectionCondenserCapacityRated
 
result = ExactMatch
 
Else  If Hrej04_CoolingTowerCapacityRated IsPresent  GT Hrej04cc_CoolingTowerCapacityRated Or Hrej04_HeatRejectionCondenserCapacityRated IsPresent GT  Hrej04cc_HeatRejectionCondenserCapacityRated
  
result = EqualBetter
End If
End If
 
 
If Hrej06_NotApplicableMessage NotPresent And Hrej06_HeatRejectionFanSpeedControl !=  Hrej06cc_HeatRejectionFanSpeedControl
Or
Hrej07_NotApplicableMessage NotPresent And Hrej07_TowerFlowTurnDown !=  Hrej07cc_TowerFlowTurnDown 
Or
Hrej08_NotApplicableMessage NotPresent And Hrej08_FanControlMultipleCells != Hrej08cc_FanControlMultipleCells
Or
Hrej09_NotApplicableMessage NotPresent And Hrej09_HasWaterEconomizerControls != Hrej09cc_HasWaterEconomizerControls
Or
Hrej10_NotApplicableMessage NotPresent And Hrej10_HasTempResetControls != Hrej10cc_HasTempResetControls
 
 D03_ExceptionalConditoinMessageNRCIMCHE += HeatRejNoMatch
End If</xsd:documentation>
