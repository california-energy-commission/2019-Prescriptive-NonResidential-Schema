<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
   targetNamespace="http://www.lmonte.com/besm/env"
   xmlns:d="http://www.lmonte.com/besm/d"
   xmlns:env="http://www.lmonte.com/besm/env"
   xmlns:com="http://www.lmonte.com/besm/com"
   xmlns:dtyp="http://www.lmonte.com/besm/dtyp"
   xmlns:xsd="http://www.w3.org/2001/XMLSchema"
   elementFormDefault="qualified" attributeFormDefault="unqualified" version="2019.0.002">
   <xsd:import namespace="http://www.lmonte.com/besm/com" schemaLocation="NrPresCommon.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/dtyp" schemaLocation="DataTypes.xsd"/>
   <xsd:simpleType name="AboveGradeStoryCount">
      <xsd:annotation>
         <xsd:documentation>An attribute of the Building object for recording the number of above grade stories in the building which is used in several rules.  Units: none</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Above Grade Story Count</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:IntegerNonnegative">
         <xsd:minInclusive value="1"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="AirInfiltrationRate">
      <xsd:annotation>
         <xsd:documentation>Uncontrolled inward air leakage from outside a building or unconditioned space, including leakage through cracks and interstices, around windows and doors, and through any other exterior or demising partition or pipe or duct penetration.  Units: ft3 per min ft2</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Air Infiltration Rate</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="CavityInsulationNR">
      <xsd:annotation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Cavity Insulation</dtyp:displayterm>
         </xsd:appinfo>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="R0">none</dtyp:displayterm>
            <dtyp:displayterm value="R10">R 10</dtyp:displayterm>
            <dtyp:displayterm value="R11">R 11</dtyp:displayterm>
            <dtyp:displayterm value="R13">R 13</dtyp:displayterm>
            <dtyp:displayterm value="R15">R 15</dtyp:displayterm>
            <dtyp:displayterm value="R16">R 16</dtyp:displayterm>
            <dtyp:displayterm value="R19">R 19</dtyp:displayterm>
            <dtyp:displayterm value="R20">R 20</dtyp:displayterm>
            <dtyp:displayterm value="R21">R 21</dtyp:displayterm>
            <dtyp:displayterm value="R22">R 22</dtyp:displayterm>
            <dtyp:displayterm value="R23">R 23</dtyp:displayterm>
            <dtyp:displayterm value="R24">R 24</dtyp:displayterm>
            <dtyp:displayterm value="R25">R 25</dtyp:displayterm>
            <dtyp:displayterm value="R26">R 26</dtyp:displayterm>
            <dtyp:displayterm value="R29">R 29</dtyp:displayterm>
            <dtyp:displayterm value="R30">R-30</dtyp:displayterm>
            <dtyp:displayterm value="R32">R 32</dtyp:displayterm>
            <dtyp:displayterm value="R35">R 35</dtyp:displayterm>
            <dtyp:displayterm value="R36">R 36</dtyp:displayterm>
            <dtyp:displayterm value="R38">R-38</dtyp:displayterm>
            <dtyp:displayterm value="R5">R 5</dtyp:displayterm>
            <dtyp:displayterm value="R6">R-6</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="R0"/>
         <xsd:enumeration value="R10"/>
         <xsd:enumeration value="R11"/>
         <xsd:enumeration value="R13"/>
         <xsd:enumeration value="R15"/>
         <xsd:enumeration value="R16"/>
         <xsd:enumeration value="R19"/>
         <xsd:enumeration value="R20"/>
         <xsd:enumeration value="R21"/>
         <xsd:enumeration value="R22"/>
         <xsd:enumeration value="R23"/>
         <xsd:enumeration value="R24"/>
         <xsd:enumeration value="R25"/>
         <xsd:enumeration value="R26"/>
         <xsd:enumeration value="R29"/>
         <xsd:enumeration value="R30"/>
         <xsd:enumeration value="R32"/>
         <xsd:enumeration value="R35"/>
         <xsd:enumeration value="R36"/>
         <xsd:enumeration value="R38"/>
         <xsd:enumeration value="R5"/>
         <xsd:enumeration value="R6"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="CeilingArea">
      <xsd:annotation>
         <xsd:documentation>  Units: ft2</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Ceiling Area</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="CeilingConstructionType">
      <xsd:annotation>
         <xsd:documentation>One of several general types of ceiling construction.</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Ceiling Construction Type</dtyp:displayterm>
         </xsd:appinfo>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="Suspended">Suspended</dtyp:displayterm>
            <dtyp:displayterm value="TBar">T-bar</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="Suspended"/>
         <xsd:enumeration value="TBar"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="CeilingInsulation">
      <xsd:annotation>
         <xsd:documentation>  Units: h ft2 F per Btu</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Proposed Ceiling Insulation</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="CeilingInsulationRequired">
      <xsd:annotation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Required Ceiling Insulation</dtyp:displayterm>
         </xsd:appinfo>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="R30">R-30</dtyp:displayterm>
            <dtyp:displayterm value="R38">R-38</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="R30"/>
         <xsd:enumeration value="R38"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="CeilingName">
      <xsd:annotation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:ObjectNamePermissive">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="ConditionedFloorAreaRangeNR">
      <xsd:annotation>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="10000To49999Ft2">10,000 - 49,999 ft[d:sup]2[/d:sup]</dtyp:displayterm>
            <dtyp:displayterm value="GT25000Ft2">[d:gt/] 25,000 ft[d:sup]2[/d:sup]</dtyp:displayterm>
            <dtyp:displayterm value="GTE50000Ft2">50,000 ft[d:sup]2[/d:sup] or greater</dtyp:displayterm>
            <dtyp:displayterm value="LT10000Ft2">[d:lt/] 10,000 ft[d:sup]2[/d:sup]</dtyp:displayterm>
            <dtyp:displayterm value="LTE25000Ft2">[d:lte/] 25,000 ft[d:sup]2[/d:sup]</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="10000To49999Ft2"/>
         <xsd:enumeration value="GT25000Ft2"/>
         <xsd:enumeration value="GTE50000Ft2"/>
         <xsd:enumeration value="LT10000Ft2"/>
         <xsd:enumeration value="LTE25000Ft2"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="ConstructedShadingType">
      <xsd:annotation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Shading Type</dtyp:displayterm>
         </xsd:appinfo>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="Overhang">Overhang</dtyp:displayterm>
            <dtyp:displayterm value="VerticalFin">Vertical fin</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="Overhang"/>
         <xsd:enumeration value="VerticalFin"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="ConstructionAssemblyRValue">
      <xsd:annotation>
         <xsd:documentation>  Units: h ft2 F per Btu</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="ConstructionAssemblyThickness">
      <xsd:annotation>
         <xsd:documentation>The thickness of a  construction assembly thickness measured in inches  Units: in</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="ConstructionAssemblyUFactor">
      <xsd:annotation>
         <xsd:documentation>The overall coefficient of thermal transmittance through a construction assembly. It includes air film resistance at both surfaces.  Units: Btu per h ft2 F</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="ConstructionSpecificationMethod">
      <xsd:annotation>
         <xsd:documentation>The method of describing a construction assembly. For California compliance, only the Layers approach is used. Other inputs facilitate specification of the final method used in simulation.</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Construction Specification Method</dtyp:displayterm>
         </xsd:appinfo>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="CFactor">C Factor</dtyp:displayterm>
            <dtyp:displayterm value="FFactor">F Factor</dtyp:displayterm>
            <dtyp:displayterm value="Layers">Layers</dtyp:displayterm>
            <dtyp:displayterm value="PredefinedConstructionAssembly">Predefined construction assembly</dtyp:displayterm>
            <dtyp:displayterm value="UFactor">U Factor</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="CFactor"/>
         <xsd:enumeration value="FFactor"/>
         <xsd:enumeration value="Layers"/>
         <xsd:enumeration value="PredefinedConstructionAssembly"/>
         <xsd:enumeration value="UFactor"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="CRRC_AgedEmittance">
      <xsd:annotation>
         <xsd:documentation>The CRRC rated 3-year aged thermal emittance of a roofing material.  Units: none</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
         <xsd:maxInclusive value="1"/>
         <xsd:minInclusive value="0"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="CRRC_AgedReflectance">
      <xsd:annotation>
         <xsd:documentation>The CRRC rated 3-year aged solar reflectance of a roofing material.  Units: none</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
         <xsd:maxInclusive value="1"/>
         <xsd:minInclusive value="0"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="CRRC_AgedSRI">
      <xsd:annotation>
         <xsd:documentation>The CRRC rated 3-year aged Solar Reflective Index (SRI) of a roofing material.The SRIis a measure defined by the Lawrence Berkeley National Laboratory as the roof's ability to reject solar heat, as shown by a small temperature rise.  Units: none</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:IntegerNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="CRRC_InitialEmittance">
      <xsd:annotation>
         <xsd:documentation>The CRRC rated initial thermal emittance of a roofing material.  Units: none</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
         <xsd:maxInclusive value="1"/>
         <xsd:minInclusive value="0"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="CRRC_InitialReflectance">
      <xsd:annotation>
         <xsd:documentation>The CRRC rated initial solar reflectance of a roofing material.  Units: none</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
         <xsd:maxInclusive value="1"/>
         <xsd:minInclusive value="0"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="CRRC_InitialSRI">
      <xsd:annotation>
         <xsd:documentation>The CRRC rated initial Solar Reflective Index (SRI) of a roofing material.The SRIis a measure defined by the Lawrence Berkeley National Laboratory as the roof's ability to reject solar heat, as shown by a small temperature rise.  Units: none</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:IntegerNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="CRRC_SRI">
      <xsd:annotation>
         <xsd:documentation>  Units: none</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="CRRCProductID">
      <xsd:annotation>
         <xsd:documentation>The CRRC product identification number for the roofing product</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="xsd:token">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="DisplayPerimeter">
      <xsd:annotation>
         <xsd:documentation>The length of an exterior wall in a building of CBC Occupancy Group B; F Div1, or M, that immediately abuts a public sidewalk, measured at the sidewalk level for each story that abuts a public sidewalk.  It can be derived using the CBC Occupancy type and the Width dimension of the Exterior Wall.  Units: ft</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Display Perimeter</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="DividerMinimumSpacing">
      <xsd:annotation>
         <xsd:documentation>The measurement of the width of the spacer between double panes in inches.  If it is less than 7/16 in, 0.05 is added to the UFactor.  Units: in</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Divider Minimum Spacing</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="DividerThroughPanes">
      <xsd:annotation>
         <xsd:documentation>A Boolean and if True, indicates that the divider goes through the fenestration pane and this causes the fenestration product U-Factor to be adjusted.</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Divider Through Panes</dtyp:displayterm>
         </xsd:appinfo>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="true">Yes</dtyp:displayterm>
            <dtyp:displayterm value="false">No</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="xsd:boolean">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="DoorArea">
      <xsd:annotation>
         <xsd:documentation>  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="DoorCertificationMethod">
      <xsd:annotation>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="CECDefaultPerformance">Tables 110.6-A and 110.6-B</dtyp:displayterm>
            <dtyp:displayterm value="NFRCRated">NFRC</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="CECDefaultPerformance"/>
         <xsd:enumeration value="NFRCRated"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="DoorConstructionName">
      <xsd:annotation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR name DoorConstructionReference
</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:ObjectNamePermissive">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="DoorLocation">
      <xsd:annotation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="xsd:string">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="DoorName">
      <xsd:annotation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:ObjectNamePermissive">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="DynamicGlazingType">
      <xsd:annotation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="ChromogenicGlazing">Chromogenic glazing</dtyp:displayterm>
            <dtyp:displayterm value="IntegratedShadingDevice">Integrated shading device</dtyp:displayterm>
            <dtyp:displayterm value="NoDynamicGlazing">None</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="ChromogenicGlazing"/>
         <xsd:enumeration value="IntegratedShadingDevice"/>
         <xsd:enumeration value="NoDynamicGlazing"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="EnvelopeName">
      <xsd:annotation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:ObjectNamePermissive">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="ExteriorFloorArea">
      <xsd:annotation>
         <xsd:documentation>  Units: ft2</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="ExteriorFloorType">
      <xsd:annotation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="Raised">Raised</dtyp:displayterm>
            <dtyp:displayterm value="SlabConcreteRaised">Concrete raised</dtyp:displayterm>
            <dtyp:displayterm value="SlabHeated">Heated Slab</dtyp:displayterm>
            <dtyp:displayterm value="SlabOnGrade">Slab on grade</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="Raised"/>
         <xsd:enumeration value="SlabConcreteRaised"/>
         <xsd:enumeration value="SlabHeated"/>
         <xsd:enumeration value="SlabOnGrade"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="ExteriorWallArea">
      <xsd:annotation>
         <xsd:documentation>  Units: ft2</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Exterior Wall Area</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="ExternalShadingObjectArea">
      <xsd:annotation>
         <xsd:documentation>  Units: ft2</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="ExternalShadingObjectName">
      <xsd:annotation>
         <xsd:documentation>The name of a shading control that is located on the outside of the building.</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:ObjectNamePermissive">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="FenestrationCertificationMethod">
      <xsd:annotation>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="CECDefaultPerformance">Tables 110.6-A and 110.6-B</dtyp:displayterm>
            <dtyp:displayterm value="CF1R_ENV_02">Area-weighted Average Worksheet (CF1R-ENV-02)</dtyp:displayterm>
            <dtyp:displayterm value="NA6Equations">Equations NA 6-1 and NA 6 -2</dtyp:displayterm>
            <dtyp:displayterm value="NFRCRated">NFRC</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="CECDefaultPerformance"/>
         <xsd:enumeration value="CF1R_ENV_02"/>
         <xsd:enumeration value="NA6Equations"/>
         <xsd:enumeration value="NFRCRated"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="FenestrationConstructionSkylightName">
      <xsd:annotation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:ObjectNamePermissive">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="FenestrationConstructionVerticalName">
      <xsd:annotation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:ObjectNamePermissive">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="FenestrationFloorRatio">
      <xsd:annotation>
         <xsd:documentation>  Units: percent</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:Decimal1PlaceNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="FenestrationFloorRatioLimitAll">
      <xsd:annotation>
         <xsd:documentation>For residential  new buildings the limit is 20% for all orientations.  The ration/percentage varies with the size of additions.  Units: none</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="FenestrationFloorRatioLimitWest">
      <xsd:annotation>
         <xsd:documentation>For residential buildings the limit is 5% for west facing fenestration.  Units: none</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="FenestrationOperability">
      <xsd:annotation>
         <xsd:documentation>The operation capability, if any, of a fenestration product.</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Fenestration Operability</dtyp:displayterm>
         </xsd:appinfo>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="Fixed">Fixed</dtyp:displayterm>
            <dtyp:displayterm value="Operable">Operable</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="Fixed"/>
         <xsd:enumeration value="Operable"/>
      </xsd:restriction>
   </xsd:simpleType>
