         <xsd:documentation>Thickness of a metal panel roof or ceiling.  Evaluated in order to assign UFactor.  Units: in</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Metal Panel Thickness</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="NonFramedAssembly">
      <xsd:annotation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Nonframed Assembly</dtyp:displayterm>
         </xsd:appinfo>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="ICF_Assembly">Insulating Concrete Form</dtyp:displayterm>
            <dtyp:displayterm value="LogWall">Log Home Walls</dtyp:displayterm>
            <dtyp:displayterm value="MetalPanel">Metal Panel Walls</dtyp:displayterm>
            <dtyp:displayterm value="SIP_IJoist">SIP I-Joist</dtyp:displayterm>
            <dtyp:displayterm value="SIP_OSB">SIP OSB</dtyp:displayterm>
            <dtyp:displayterm value="SIP2x">SIP 2x</dtyp:displayterm>
            <dtyp:displayterm value="SIP4x">SIP 4x</dtyp:displayterm>
            <dtyp:displayterm value="StrawBaleWall">Straw Bale Walls</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="ICF_Assembly"/>
         <xsd:enumeration value="LogWall"/>
         <xsd:enumeration value="MetalPanel"/>
         <xsd:enumeration value="SIP_IJoist"/>
         <xsd:enumeration value="SIP_OSB"/>
         <xsd:enumeration value="SIP2x"/>
         <xsd:enumeration value="SIP4x"/>
         <xsd:enumeration value="StrawBaleWall"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="OutsideBoundaryCondition">
      <xsd:annotation>
         <xsd:documentation>Defines the nature of the environment that the outward facing surface of a Partition is in contact with.</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Outside Boundary Condition</dtyp:displayterm>
         </xsd:appinfo>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="Adiabatic">Adiabatic</dtyp:displayterm>
            <dtyp:displayterm value="Ground">Ground</dtyp:displayterm>
            <dtyp:displayterm value="Outdoors">Outdoors</dtyp:displayterm>
            <dtyp:displayterm value="Surface">Surface</dtyp:displayterm>
            <dtyp:displayterm value="Zone">Zone</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="Adiabatic"/>
         <xsd:enumeration value="Ground"/>
         <xsd:enumeration value="Outdoors"/>
         <xsd:enumeration value="Surface"/>
         <xsd:enumeration value="Zone"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="PanesDivider">
      <xsd:annotation>
         <xsd:documentation>Indicates if a double paned fenestration product has a divider.  If so this adjusts the mandatory U-Factor.</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Panes Divider</dtyp:displayterm>
         </xsd:appinfo>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="Divider">Divider</dtyp:displayterm>
            <dtyp:displayterm value="NoDivider">No divider</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="Divider"/>
         <xsd:enumeration value="NoDivider"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="PanesPerLight">
      <xsd:annotation>
         <xsd:documentation>An attribute of Fenestration Construction that tracks how many panes are in the fenestration product.</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Panes Per Light</dtyp:displayterm>
         </xsd:appinfo>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="DoublePane">Double pane</dtyp:displayterm>
            <dtyp:displayterm value="GlassBlock">Glass block</dtyp:displayterm>
            <dtyp:displayterm value="SinglePane">Single Pane</dtyp:displayterm>
            <dtyp:displayterm value="TriplePane">Triple Pane</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="DoublePane"/>
         <xsd:enumeration value="GlassBlock"/>
         <xsd:enumeration value="SinglePane"/>
         <xsd:enumeration value="TriplePane"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="PartitionAboveGrade">
      <xsd:annotation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="true">Above Grade</dtyp:displayterm>
            <dtyp:displayterm value="false">Below Grade</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="xsd:boolean">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="PartitionFramingMaterial">
      <xsd:annotation>
         <xsd:documentation>The framing material used in a CodeCategory = Composite material.</xsd:documentation>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="Metal">Metal</dtyp:displayterm>
            <dtyp:displayterm value="WoodFraming">Wood</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="Metal"/>
         <xsd:enumeration value="WoodFraming"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="PartitionName">
      <xsd:annotation>
         <xsd:documentation>The name of any partition such as a wall or floor.  All partition names must be unique within a building description file.</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:ObjectNamePermissive">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="PartitionThickness">
      <xsd:annotation>
         <xsd:documentation>The thickness of a wall, floor, ceiling or roof partition measured in inches  Units: in</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="PartitionType">
      <xsd:annotation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="Ceiling">Ceiling</dtyp:displayterm>
            <dtyp:displayterm value="Floor">Floor</dtyp:displayterm>
            <dtyp:displayterm value="Roof">Roof</dtyp:displayterm>
            <dtyp:displayterm value="Wall">Wall</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="Ceiling"/>
         <xsd:enumeration value="Floor"/>
         <xsd:enumeration value="Roof"/>
         <xsd:enumeration value="Wall"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="RoofArea">
      <xsd:annotation>
         <xsd:documentation>The two dimensional extent of a roof surface.  Units: ft2</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Area</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="RoofPitch">
      <xsd:annotation>
         <xsd:documentation>Pitch is the slope calculated by dividing the rise by the run  Units: none</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="RoofProductType">
      <xsd:annotation>
         <xsd:documentation>An enumeration of types of roofing products that are evaluated when assigning a rule target in the Standards.</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Roof Product Type</dtyp:displayterm>
         </xsd:appinfo>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="AsphaltShingles">Asphalt shingles</dtyp:displayterm>
            <dtyp:displayterm value="BuiltUpRoofing">Built-up roofing</dtyp:displayterm>
            <dtyp:displayterm value="ClayRoofTiles">Clay roof tiles</dtyp:displayterm>
            <dtyp:displayterm value="FactoryAppliedCoating">Factory applied coating</dtyp:displayterm>
            <dtyp:displayterm value="FieldAppliedCoating">Field applied coating</dtyp:displayterm>
            <dtyp:displayterm value="FluidAppliedMembrane">Fluid applied membrane</dtyp:displayterm>
            <dtyp:displayterm value="MetalRoof">Metal roof</dtyp:displayterm>
            <dtyp:displayterm value="MetalShakesShingles">Metal shakes shingles</dtyp:displayterm>
            <dtyp:displayterm value="ModifiedBitumin">Modified bitumin</dtyp:displayterm>
            <dtyp:displayterm value="PolymerCompositeSteepSlope">Polymer composite steep slope</dtyp:displayterm>
            <dtyp:displayterm value="RoofPavers">Roof pavers</dtyp:displayterm>
            <dtyp:displayterm value="SinglePlyThermoplastic">Single ply thermoplastic</dtyp:displayterm>
            <dtyp:displayterm value="SinglePlyThermoset">Single ply thermoset</dtyp:displayterm>
            <dtyp:displayterm value="SprayPolyurethaneFoam">Spray polyurethane foam</dtyp:displayterm>
            <dtyp:displayterm value="StoneAggregateBallast">Stone aggregate ballast</dtyp:displayterm>
            <dtyp:displayterm value="WoodShakesShingles">Wood shakes shingles</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="AsphaltShingles"/>
         <xsd:enumeration value="BuiltUpRoofing"/>
         <xsd:enumeration value="ClayRoofTiles"/>
         <xsd:enumeration value="FactoryAppliedCoating"/>
         <xsd:enumeration value="FieldAppliedCoating"/>
         <xsd:enumeration value="FluidAppliedMembrane"/>
         <xsd:enumeration value="MetalRoof"/>
         <xsd:enumeration value="MetalShakesShingles"/>
         <xsd:enumeration value="ModifiedBitumin"/>
         <xsd:enumeration value="PolymerCompositeSteepSlope"/>
         <xsd:enumeration value="RoofPavers"/>
         <xsd:enumeration value="SinglePlyThermoplastic"/>
         <xsd:enumeration value="SinglePlyThermoset"/>
         <xsd:enumeration value="SprayPolyurethaneFoam"/>
         <xsd:enumeration value="StoneAggregateBallast"/>
         <xsd:enumeration value="WoodShakesShingles"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="RoofProductWeight">
      <xsd:annotation>
         <xsd:documentation>The weight of the roofing product covering 1 square foot.  Units: lb per ft2</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Roof Product Weight</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="RoofRise">
      <xsd:annotation>
         <xsd:documentation>This is the distance in inches that a roof rises vertically for the roof run of 12 inches to its ridge and is used in calculating the roof pitch or slope.  This is sometimes also referred to as pitch when the run is assumed to be 12 inches.  Units: in</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="RoofRiseToRun">
      <xsd:annotation>
         <xsd:documentation>This is the representation of roof rise to run as a regular expression that accepts #:#.</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:RoofRiseToRun">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="RoofRun">
      <xsd:annotation>
         <xsd:documentation>This is horizontal distance from the wall top plate to the roof's ridge line and is used in calculating the roof pitch or slope  Units: in</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="RoofSlope">
      <xsd:annotation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="LowSlope">Low slope</dtyp:displayterm>
            <dtyp:displayterm value="SteepSlope">Steep slope</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="LowSlope"/>
         <xsd:enumeration value="SteepSlope"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="SIPThickness">
      <xsd:annotation>
         <xsd:documentation>The measurement from one side of the SIP panel to the other side  Units: in</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>SIP Thickness</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="SkylightCurbType">
      <xsd:annotation>
         <xsd:documentation>Curb or No Curb.
For selection of NA-6 properties</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Skylight Curb Type</dtyp:displayterm>
         </xsd:appinfo>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="CurbMounted">Curb mounted</dtyp:displayterm>
            <dtyp:displayterm value="DeckMounted">Deck mounted</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="CurbMounted"/>
         <xsd:enumeration value="DeckMounted"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="SkylightGlazing">
      <xsd:annotation>
         <xsd:documentation>Used in selection of baseline properties</xsd:documentation>
         <xsd:appinfo source="displayterm">
            <dtyp:displayterm value="Glass">Glass</dtyp:displayterm>
            <dtyp:displayterm value="Plastic">Plastic</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:EnumerationConstant">
         <xsd:enumeration value="Glass"/>
         <xsd:enumeration value="Plastic"/>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="SkylightName">
      <xsd:annotation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:ObjectNamePermissive">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="SkylightRoofRatio">
      <xsd:annotation>
         <xsd:documentation>The ratio of the rough opening area of the skylights in a parent surface to the total parent surface area.  This can either be a local ratio (for a single roof section) or a global ratio (for the entire roof area in the building or project).  referred to as SRR.  Units: none</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Skylight Roof Ratio</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="SolarReflectance">
      <xsd:annotation>
         <xsd:documentation>From E+ Input/Output Ref:The diffuse solar reflectance of the unglazed part of the shading  surface (default = 0.2). This reflectance is assumed to be the same for   beam-to-diffuse and diffuse-to-diffuse reflection. Beam-to-diffuse reflection  is assumed to be independent of angle of incidence of beam radiation.  Diffuse-to-diffuse reflection is assumed to be independent of angular distribution of the incident of diffuse radiation. The outgoing diffuse radiation is assumed to be isotropic (hemispherically uniform). The sum of this reflectance and the shading surface transmittance should be less than or equal to 1.0.  Units: none</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="PartitionSpaceName">
      <xsd:annotation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:ObjectNamePermissive">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="TotalSkylightArea">
      <xsd:annotation>
         <xsd:documentation>The sum of all skylights in a building.  Total Skylight Area can be derived from the Fenestration Skylight object which contains all skylights in the building.  Units: ft2</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Total Skylight Area</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="UndergroundWallHeight">
      <xsd:annotation>
         <xsd:documentation>Describes the height of the underground wall, i.e. the depth to the ground surface.  Units: ft</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="VisibleReflectance">
      <xsd:annotation>
         <xsd:documentation>From E+ Input/Output Ref:The diffuse visible reflectance of the unglazed part of the shading surface (default = 0.2). This reflectance is assumed to be the same for beam-to-diffuse and diffuse-to-diffuse reflection. Beam-to-diffuse reflection is assumed to be independent of angle of incidence of beam radiation. Diffuse-to-diffuse reflection is assumed to be independent of angular distribution of the incident of diffuse radiation. The outgoing diffuse radiation is assumed to be isotropic (hemispherically uniform).  This value if used only for the daylighting calculation (ref: Daylighting:Controls).  The sum of this reflectance and the shading surface transmittance should be less than or equal to 1.0.  Units: none</xsd:documentation>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="WWR">
      <xsd:annotation>
         <xsd:documentation>The ratio of the rough opening area of the window in a parent Wall to the total Wall surface area.  This can either be a local ratio (for a single wall) or a global ratio (for all walls in the building or project).  Units: none</xsd:documentation>
         <xsd:documentation source="propertyrules">NR Presc include;  CBECC NR exclude</xsd:documentation>
         <xsd:appinfo source="displaytermType">
            <dtyp:displayterm>Window Wall Ratio</dtyp:displayterm>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="dtyp:DecimalNonnegative">
      </xsd:restriction>
   </xsd:simpleType>
</xsd:schema>
