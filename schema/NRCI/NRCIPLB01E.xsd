                                                <xsd:element name="DHW10_StandbyLoss" type="hvac:StandbyLoss" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Standby Loss</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If DHW10cc_StandbyLoss IsPresent And DHW06_WaterHeaterCapacityRated IsPresent 
If DHW04_WaterHeaterType == BoilerWaterHeater, InstantaneousCommercial, InstantaneousConsumer, StorageCommercial, StorageConsumer, StorageResDutyCommercial
Or  DHW04_TankCategory == BoilerCommercial, Boiler, StorageCommercial, Storage 
 
result = ( DHW06_WaterHeaterCapacityRated / 800) + (110 * DHW05_WaterHeaterTankVolume x 0.5)
End If 
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="DHW10_WaterHeaterStandbyLossFraction" type="hvac:WaterHeaterStandbyLossFraction" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Standby Loss</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If DHW10cc_WaterHeaterStandbyLossFraction IsPresent
If DHW04_WaterHeaterType == StorageCommercialElectric, StorageConsumerElectric
Or If DHW06_WaterHeaterElectricFiredRatedInput IsPresent And DHW04_TankCategory == StorageCommercial Or  Storage
 
result = 0.30 + (27/ DHW05_WaterHeaterTankVolume)
End If
End If</xsd:documentation>
                                                   </xsd:annotation>
