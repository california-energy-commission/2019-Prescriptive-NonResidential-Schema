                                             <xsd:element name="DHW11_ComplianceResultNRCI" type="comp:ComplianceResultNRCI" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">DHW Equipment Compliance</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">If any user input field is blank
result = TableIncomplete 
Else If   
DHW03_CentralSystem != DHW03cc_CentralSystem 
Or
(
DHW08_EfficiencyAFUE IsPresent And DHW08_EfficiencyAFUE LT DHW08cc_EfficiencyAFUE Or 
DHW08_EfficiencyCOP IsPresent And DHW08_EfficiencyCOP LT DHW08cc_EfficiencyCOP Or 
DHW08_ThermalEfficiency IsPresent And DHW08_ThermalEfficiency LT DHW08cc_ThermalEfficiency Or
DHW08_UniformEnergyFactor IsPresent And DHW08_UniformEnergyFactor LT DHW08cc_UniformEnergyFactor Or
DHW08_EnergyFactor IsPresent And DHW08_EnergyFactor LT DHW08cc_EnergyFactor
)
Or
(
DHW10_StandbyLoss IsPresent And DHW10_StandbyLoss GT DHW10cc_StandbyLoss Or
DHW10_WaterHeaterStandbyLossFraction IsPresent And DHW10_WaterHeaterStandbyLossFraction GT DHW10cc_WaterHeaterStandbyLossFraction
)
result = MayNotComply
Else If 
DHW03_CentralSystem == DHW03cc_CentralSystem
And 
(
DHW04_NotApplicableMessage IsPresent Or 
DHW04_WaterHeaterType IsPresent and DHW04_WaterHeaterType == DHW04cc_WaterHeaterType Or 
DHW04_TankCategory IsPreent And DHW04_TankCategory == DHW04cc_TankCategory
)
And
(
DHW05_NotApplicableMessage IsPresent Or
DHW05_WaterHeaterVolume IsPresent And DHW05_WaterHeaterVolume == DHW05cc_WaterHeaterVolume Or
DHW05_WaterHeaterTankVolume IsPresent And DHW05_WaterHeaterTankVolume == DHW05cc_WaterHeaterTankVolume
)
And 
(
DHW06_NotApplicableMessage IsPresent Or
DHW06_WaterHeaterCapacityRated IsPresent And DHW06_WaterHeaterCapacityRated == DHW06cc_WaterHeaterCapacityRated Or
DHW06_WaterHeaterElectricFiredRatedInput IsPresent And DHW06_WaterHeaterElectricFiredRatedInput == DHW06cc_WaterHeaterElectricFiredRatedInput
)
And 
DHW07_NotApplicableMessage IsPresent Or DHW07_WaterHeaterRatedInputUnits == DHW07cc_WaterHeaterRatedInputUnits
And 
(
DHW08_NotApplicableMessage IsPresent Or 
DHW08_EfficiencyAFUE IsPresent And DHW08_EfficiencyAFUE == DHW08cc_EfficiencyAFUE Or 
DHW08_EfficiencyCOP IsPresent And DHW08_EfficiencyCOP == DHW08cc_EfficiencyCOP Or 
DHW08_ThermalEfficiency IsPresent And DHW08_ThermalEfficiency == DHW08cc_ThermalEfficiency Or
DHW08_UniformEnergyFactor IsPresent And DHW08_UniformEnergyFactor == DHW08cc_UniformEnergyFactor Or
DHW08_EnergyFactor IsPresent And DHW08_EnergyFactor == DHW08cc_EnergyFactor
)
And 
(
DHW10_NotApplicableMessage IsPresent Or
DHW10_StandbyLoss IsPresent And DHW10_StandbyLoss == DHW10cc_StandbyLoss Or
DHW10_WaterHeaterStandbyLossFraction IsPresent And DHW10_WaterHeaterStandbyLossFraction == DHW10cc_WaterHeaterStandbyLossFraction
)
result = ExactMatch
Else If 
DHW08_EfficiencyAFUE IsPresent And DHW08_EfficiencyAFUE GTE DHW08cc_EfficiencyAFUE Or 
DHW08_EfficiencyCOP IsPresent And DHW08_EfficiencyCOP GTE DHW08cc_EfficiencyCOP Or 
DHW08_ThermalEfficiency IsPresent And DHW08_ThermalEfficiency GTE DHW08cc_ThermalEfficiency Or
DHW08_UniformEnergyFactor IsPresent And DHW08_UniformEnergyFactor GTE DHW08cc_UniformEnergyFactor Or
DHW08_EnergyFactor IsPresent And DHW08_EnergyFactor GTE DHW08cc_EnergyFactor
)
Or
(
DHW10_StandbyLoss IsPresent And DHW10_StandbyLoss LTE DHW10cc_StandbyLoss Or
DHW10_WaterHeaterStandbyLossFraction IsPresent And DHW10_WaterHeaterStandbyLossFraction LTE DHW10cc_WaterHeaterStandbyLossFraction
)
result = EqualBetter
End If</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
