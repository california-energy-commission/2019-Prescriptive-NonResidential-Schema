<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
   targetNamespace="http://www.lmonte.com/besm/NRCIPLB01E"
   xmlns="http://www.lmonte.com/besm/NRCIPLB01E"
   xmlns:d="http://www.lmonte.com/besm/d"
   xmlns:bld="http://www.lmonte.com/besm/bld"
   xmlns:env="http://www.lmonte.com/besm/env"
   xmlns:lit="http://www.lmonte.com/besm/lit"
   xmlns:hvac="http://www.lmonte.com/besm/hvac"
   xmlns:comp="http://www.lmonte.com/besm/comp"
   xmlns:com="http://www.lmonte.com/besm/com"
   xmlns:dtyp="http://www.lmonte.com/besm/dtyp"
   xmlns:xsd="http://www.w3.org/2001/XMLSchema"
   xmlns:altova="http://www.altova.com/xml-schema-extensions"
   elementFormDefault="qualified" attributeFormDefault="unqualified" version="2019.1.003">
   <xsd:import namespace="http://www.lmonte.com/besm/com" schemaLocation="../base/NrPresCommon.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/bld" schemaLocation="../base/NrPresBuilding.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/env" schemaLocation="../base/NrPresEnvelope.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/lit" schemaLocation="../base/NrPresLighting.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/hvac" schemaLocation="../base/NrPresHvac.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/comp" schemaLocation="../base/NrPresCompliance.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/dtyp" schemaLocation="../base/DataTypes.xsd"/>
   <xsd:element name="ComplianceDocumentPackage">
      <xsd:complexType>
         <xsd:sequence>
            <xsd:element name="DocID" minOccurs="0">
               <xsd:complexType>
                  <xsd:attribute name="doc" type="comp:ComplianceDocumentTagNR" fixed="NRCIPLB01E"/>
                  <xsd:attribute name="docType" type="comp:ComplianceDocumentTypeNR" fixed="CertificateInstallationNR"/>
                  <xsd:attribute name="docTitle" type="comp:ComplianceDocumentTitleNR" fixed="Domestic Water Heating System"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Payload" type="comp:Payload"/>
            <xsd:element name="DocumentData">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="Header" type="comp:HeaderNRCI" />
                     <xsd:element ref="nRCIPLB01E" />
                     <xsd:element name="DocAuthor" type="comp:DocumentAuthor" maxOccurs="unbounded" />
                     <xsd:element name="RespPerson" type="comp:ResponsiblePersonNRCI" maxOccurs="unbounded" />
                     <xsd:element name="Footer" type="comp:FooterNRCC" />
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Report" type="xsd:base64Binary" minOccurs="0"/>
         </xsd:sequence>
         <xsd:attribute name="revision" use="required" type="xsd:string" fixed="rev 20210701"/>
         <xsd:attribute name="doc" use="required" type="comp:ComplianceDocumentTagNR" fixed="NRCIPLB01E"/>
      </xsd:complexType>
   </xsd:element>
   <xsd:complexType name="NRCIPLB01E">
      <xsd:annotation>
         <xsd:documentation>Domestic Water Heating System</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Section_A" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]A. GENERAL INFORMATION[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="A_BeforeNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This  Certificate of Installation is used to document the installation of service water heating features, materials, components, and manufactured devices required to demonstrate compliance with Title 24, Part 6 per [d:t24refnolink/]110-103(a)3 for nonresidential, hotel/motel and high-rise residential occupancies.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A01_ProjectCity" type="comp:ProjectCity" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Project Location (city)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If NRCC-PLB parent
result = A01_ProjectCity
Else If NRCC-PRF parent
result = Info01_ProjectCity
End If</xsd:documentation>
                        <xsd:appinfo source="NRCIsrc">
                           <dtyp:srcSchema form="NRCCPLB01E"  usage="copy">A01_ProjectCity</dtyp:srcSchema>
                           <dtyp:srcSchema form="NRCCPRF01E"  usage="copy">Info01_ProjectCity</dtyp:srcSchema>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A02_ProjectZipcode" type="comp:ProjectZipcode" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Zip Code</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If NRCC-PLB parent
User input
Else If NRCC-PRF parent
result = Info02_ProjectZipcode
End If</xsd:documentation>
                        <xsd:appinfo source="NRCIsrc">
                           <dtyp:srcSchema form="NRCCPRF01E"  usage="copy">Info02_ProjectZipcode</dtyp:srcSchema>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03_PermitSetDate" type="comp:PermitSetDate" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Date of Permit Set used for construction</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A04_PermitSetName" type="comp:PermitSetName" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Name of Permit Set used for construction</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A05_AuthorityHavingJurisdiction" type="comp:AuthorityHavingJurisdiction" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Authority Having Jurisdiction</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A06_PermitNumber" type="comp:PermitNumber" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Building Permit #</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A07_AsBuiltSetDate" type="comp:AsBuiltSetDate" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Date of As-built Set</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A08_AsBuiltSetName" type="comp:AsBuiltSetName" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Name of As-built Set</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_B" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b] B. INSTALLER SCOPE[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="B_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table indicates construction systems and materials documented on this Nonresidential Certificate of Installation.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01_ProjectHasEquipment" type="comp:ProjectHasEquipment" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Equipment</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input;  This includes new and altered equipment</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B02_ProjectHasDistribution" type="comp:ProjectHasDistribution" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Distribution (Piping, valves, insulation, etc)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input; This includes new and altered distribution</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03_ProjectHasControls" type="comp:ProjectHasControls" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Controls</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input; This includes new and altered controls</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B_Alert1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b][d:i]ALERT! Solar water heating systems are documented on the NRCI-SRA compliance document.[/d:i][/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This is shown only in the UI</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_C" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]C. COMPLIANCE RESULTS[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="C_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table indicates whether the as-built conditions documented in this form are equal or better than what was documented on the permitted Certificate of Compliance.  If the installation is not equal or better, Section 10-103(a)2B requires the Certificate of Compliance form to be revised accordingly to demonstrate compliance.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C01_ComplianceResultNRCI" type="comp:ComplianceResultNRCI" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated result:  
If in any required tables or subsections in Section F  ComplianceResultNRCI ==  FormNotComplete
result = FormNotComplete
Else If in any required tables or subsections in Section F ComplianceResultNRCI == MayNotComply
result = MayNotComply
Else If All required tables or subsections in Section F  have  ComplianceResultNRCI ==  ExactMatch
result = ExactMatch
Else If any required tables or subsections in Section F have ComplianceResultNRCI ==  EqualBetter
result = EqualBetter
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C01Message_ComplianceResultMessageNRCI" type="comp:ComplianceResultMessageNRCI" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated result:  
If C01_ComplianceResultNRCI == ExactMatch Or EqualBetter
result = ExactEqualBetterText
Else If C01_ComplianceResultNRCI == MayNotComply
result =  MayNotComplyText
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_D" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]D. EXCEPTIONAL CONDITIONS[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="D_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table is auto-filled with uneditable comments because of field conditions noted by the installer that may impact requirements documented on the Certificate of Compliance.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="D01_ExceptionalConditionMessageNRCIPLBE" type="comp:ExceptionalConditionMessageNRCIPLBE"  minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Exceptional Messages</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Auto-filled with any system generated messages that explain exceptional conditions encountered in the the completed compliance document
If ExceptionalConditionMessageNRCIPLBE == ""
result = NoExceptionNote
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_E" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]E. INSTALLER NOTES[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required if there are  comments from the permit applicant to the Authority Having Jurisdiction</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="E_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table includes remarks made by the installer to the Authority Having Jurisdiction.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="E01_CommentsToPermitAuthority" type="comp:CommentsToPermitAuthority"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_F" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]F. INSTALLATION DETAILS [/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="F_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]The following tables indicate performance requirements as documented on the permitted Certificate of Compliance (NRCC) for all systems and components included in Table B. Installer Scope.  Also included are the as-built conditions documented by the installer/documentation author. [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F_RowLabelTopCell" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Per NRCC</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This is the text displayed at the left end of a row in the top cell for all tables/subsections in Section F except the last subsection Smart Thermostats and Additional Efficiency Measure</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F_RowLabelBottomCell" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">As-built Conditions</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This is the text displayed at the left end of a row in the bottom cell for all tables/subsections in Section F except the last subsection Smart Thermostats and Additional Efficiency Measure</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="DHW" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required If B01_ProjectHasEquipment == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="TableDHW" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">[d:b]Domestic Hot Water Equipment Efficiency[/d:b]</xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="DHW01cc_WaterHeaterName" type="hvac:WaterHeaterName" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Name or Item Tag</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">If NRCC-PLB parent
result = F01_WaterHeaterName Or F07_WaterHeaterName
Else If NRCC-PRF parent
result = Water01_WaterHeaterName 
End If</xsd:documentation>
                                                   <xsd:appinfo source="NRCIsrc">
                                                      <dtyp:srcSchema form="NRCCPLB01E"  usage="copy">F01_WaterHeaterName</dtyp:srcSchema>
                                                      <dtyp:srcSchema form="NRCCPLB01E"  usage="copy">F07_WaterHeaterName</dtyp:srcSchema>
                                                      <dtyp:srcSchema form="NRCCPRF01E"  usage="copy">Water01_WaterHeaterName</dtyp:srcSchema>
                                                   </xsd:appinfo>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="DHW01_InstalledSameAsCC" type="comp:InstalledSameAsCC"  minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects true if installed is the same as NRCC value  Or false if installed is different than NRCC value</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="DHW02cc_NotApplicableMessage" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Model #</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">The NRCC cell is always BlankGrayedOut</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="comp:NotApplicableMessage">
                                                      <xsd:enumeration value="BlankGrayedOut"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:element name="DHW02a_ModelNumber" type="hvac:ModelNumber" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Model #</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="DHW03cc_CentralSystem" type="hvac:CentralSystem" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Individual or Central System</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">If NRCC-PLB parent
If DHW01cc_WaterHeaterName assigned from F01_WaterHeaterName 
result = false
Else If DHW01cc_WaterHeaterName assigned from F07_WaterHeaterName
result = true
End If
Else If NRCC-PRF parent
If WaterMF03_CentralSystem IsPresent
If WaterMF03_CentralSystem == true
result = WaterMF03_CentralSystem
End If
End If
End If</xsd:documentation>
                                                   <xsd:appinfo source="displayterm">
                                                      <dtyp:displayterm value="true">Central</dtyp:displayterm>
                                                      <dtyp:displayterm value="false">Individual</dtyp:displayterm>
                                                   </xsd:appinfo>
                                                   
                                                   <xsd:appinfo source="NRCIsrc">
                                                      <dtyp:srcSchema form="NRCCPLB01E"  usage="reference">F01_WaterHeaterName</dtyp:srcSchema>
                                                      <dtyp:srcSchema form="NRCCPLB01E"  usage="reference">F07_WaterHeaterName</dtyp:srcSchema>
                                                      <dtyp:srcSchema form="NRCCPRF01E"  usage="copy">WaterMF03_CentralSystem</dtyp:srcSchema>
                                                   </xsd:appinfo>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="DHW03_CentralSystem" type="hvac:CentralSystem" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Individual or Central System</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">If DHW01_InstalledSameAsCC == true
result = DHW03cc_CentralSystem
Else
User input
End If</xsd:documentation>
                                                   <xsd:appinfo source="displayterm">
                                                      <dtyp:displayterm value="true">Central</dtyp:displayterm>
                                                      <dtyp:displayterm value="false">Individual</dtyp:displayterm>
                                                   </xsd:appinfo>
                                                   
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="DHW04cc_WaterHeaterType" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Equipment Type</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If NRCC-PLB parent
If DHW01cc_WaterHeaterName assigned from F01_WaterHeaterName 
result = F02_WaterHeaterType
Else If DHW01cc_WaterHeaterName assigned from F07_WaterHeaterName
result = F08_WaterHeaterType
End If
End If</xsd:documentation>
                                                      <xsd:appinfo source="displayterm">
                                                         <dtyp:displayterm value="StorageCommercial">Gas Storage Water Heater</dtyp:displayterm>
                                                         <dtyp:displayterm value="InstantaneousCommercial">Gas Instantaneous Water Heater</dtyp:displayterm>
                                                         <dtyp:displayterm value="StorageCommercialElectric">Electric Storage Water Heater</dtyp:displayterm>
                                                         <dtyp:displayterm value="BoilerCommercialGT300kBtu">Commercial Package Gas Hot Water Supply Boilers (300k-2.5MM Btu/h)
</dtyp:displayterm>
                                                      </xsd:appinfo>
                                                      
                                                      <xsd:appinfo source="NRCIsrc">
                                                         <dtyp:srcSchema form="NRCCPLB01E"  usage="copy">F02_WaterHeaterType</dtyp:srcSchema>
                                                         <dtyp:srcSchema form="NRCCPLB01E"  usage="copy">F08_WaterHeaterType</dtyp:srcSchema>
                                                      </xsd:appinfo>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="hvac:WaterHeaterType">
                                                         <xsd:enumeration value="BoilerCommercialGT300kBtu"/>
                                                         <xsd:enumeration value="BoilerResDutyCommercial"/>
                                                         <xsd:enumeration value="BoilerWaterHeater"/>
                                                         <xsd:enumeration value="HeatPumpWaterHeater"/>
                                                         <xsd:enumeration value="InstantaneousCommercial"/>
                                                         <xsd:enumeration value="InstantaneousConsumer"/>
                                                         <xsd:enumeration value="InstantaneousConsumerElectric"/>
                                                         <xsd:enumeration value="InstantaneousResDutyCommercial"/>
                                                         <xsd:enumeration value="StorageCommercial"/>
                                                         <xsd:enumeration value="StorageCommercialElectric"/>
                                                         <xsd:enumeration value="StorageConsumer"/>
                                                         <xsd:enumeration value="StorageConsumerElectric"/>
                                                         <xsd:enumeration value="StorageResDutyCommercial"/>
                                                         <xsd:enumeration value="TableTopElectric"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                                <xsd:element name="DHW04cc_TankCategory" type="hvac:TankCategory" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Equipment Type</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If NRCC-PRF parent
If Water03_TankCategory IsPresent
result = Water03_TankCategory
End If
End If</xsd:documentation>
                                                      <xsd:appinfo source="NRCIsrc">
                                                         <dtyp:srcSchema form="NRCCPRF01E"  usage="copy">Water03_TankCategory</dtyp:srcSchema>
                                                      </xsd:appinfo>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="DHW04cc_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Equipment Type</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If NRCC-PRFparent 
If Water03_NotApplicableMessage IsPresent
result = BlankGrayedOut
End If
End If</xsd:documentation>
                                                      <xsd:appinfo source="NRCIsrc">
                                                         <dtyp:srcSchema form="NRCCPRF01E"  usage="reference">Water03_NotApplicableMessage</dtyp:srcSchema>
                                                      </xsd:appinfo>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="DHW04_WaterHeaterType" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Equipment Type</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If DHW04cc_WaterHeaterType IsPresent
If DHW01_InstalledSameAsCC == true
result = DHW04cc_WaterHeaterType
Else 
User input
End If
End If</xsd:documentation>
                                                      <xsd:appinfo source="displayterm">
                                                         <dtyp:displayterm value="StorageCommercial">Gas Storage Water Heater</dtyp:displayterm>
