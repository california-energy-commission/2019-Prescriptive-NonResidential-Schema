                                                         <xsd:enumeration value="LibraryReadingArea"/>
                                                         <xsd:enumeration value="LibraryStacks"/>
                                                         <xsd:enumeration value="LobbyMainEntry"/>
                                                         <xsd:enumeration value="LockerDressingRoom"/>
                                                         <xsd:enumeration value="LoungeBreakRoom"/>
                                                         <xsd:enumeration value="LowVisionCorridorArea"/>
                                                         <xsd:enumeration value="LowVisionDiningArea"/>
                                                         <xsd:enumeration value="LowVisionLobbyMainEntry"/>
                                                         <xsd:enumeration value="LowVisionMultipurposeRoom"/>
                                                         <xsd:enumeration value="LowVisionReligiousWorshipArea"/>
                                                         <xsd:enumeration value="LowVisionRestroom"/>
                                                         <xsd:enumeration value="LowVisionStairwell"/>
                                                         <xsd:enumeration value="LowVisionWaitingArea"/>
                                                         <xsd:enumeration value="MuseumRestoration"/>
                                                         <xsd:enumeration value="Office250FtOrLess"/>
                                                         <xsd:enumeration value="OfficeGreaterThan250Ft"/>
