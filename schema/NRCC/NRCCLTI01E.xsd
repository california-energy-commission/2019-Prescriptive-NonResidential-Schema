                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="Iu03_AllowedLPD" type="comp:AllowedLPD" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Allowed Density (W/ft[d:sup]2[/d:sup])</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Auto-filled with allowed LPD for  building or space type value in Iu02. If Iu02_WholeBuilding NotNull  Switch ( Iu02_WholeBuildingOccupancy)   case AssemblyBuilding:  case LibraryBuilding: case ReligiousFacilityBuilding: case RestaurantBuilding: case MotionPictureBuilding:  result = 0.70; case FinancialInstitutionBuilding: case GymnasiumBuilding: case OfficeBuilding: case SchoolBuilding: result = 0.65;  case  ManufacturingFacilityBuilding: result = 0.60; case GroceryStoreBuilding: result =0.95; case ParkingGarageBuilding: result = 0.13; case HospitalBuilding: case RetailBuilding: result = 0.90; case SportsArenaBuilding: result =0.75; case PerformingArtsTheaterBuilding: result = 0.8; case AllOthersBuildings: result = 0.4;  Else If Iu02_SpaceFunction NotNull Switch (Iu02_SpaceFunction) case AuditoriumArea: case ClassroomLectureTrainingVocationalArea:  case Office250FtOrLess:  result = 0.70; case AutoRepairArea: case BarLoungeFineDiningArea: case HospitalMedicalSupply:  case HospitalPatientRoom:  result = 0.55;  case AudienceSeatingArea: case CommercialIndustrialStorageShipping: case CorridorArea: case ExhibitMuseumArea: case  GeneralCommercialIndustrialWorkAreaLowBay:  case OfficeOpenPlan: case RetailFittingRoom: case TheaterMotionPicture:  result = 0.6;  case case BeautySalonArea: case FinancialTransactionsArea: case LibraryReadingArea: case LowVisionStairwell: case LowVisionCorridorArea: case LowVisionDiningArea: case LowVisionRestroom:  result = 0.80;  case CivicMeetingPlaceArea:  case RetailSalesArea: case TheaterPerformance: case LaboratoryScientific: case HospitalImaging: case LowVisionReligiousWorshipArea:   result = 1.0;  case CommercialIndustrialWarehouse: case LockerDressingRoom: case Laundry: case TransportationFunctionTicketing:    result = 0.45;  case ConventionConferenceMultipurposeMeetingCenterArea:  case  HotelFunctionArea: case GeneralCommercialIndustrialWorkAreaPrecision: case LobbyMainEntry:  case HospitalPhysicalTherapy: case LowVisionLobbyMainEntry:  result = 0.85;   case CopyRoom: case FamilyDiningArea: case ExerciseCenterGymnasiumArea:  case ParkingGarageAreaDaylightAdaptationZone: case Stairwell: result = 0.50;   case FastfoodDining:  case ElectricalMechancialTelephoneRoom:  case TransportationFunctionConcourseBaggage:  case AllOthersSpace:  result = 0.40;     case MuseumRestoration:  case HospitalNurseStation:  case SportsArenaSeatsLimitedClassIV:  case LowVisionWaitingArea:  result = 0.75;    case GeneralCommercialIndustrialWorkAreaHighBay:  case LoungeBreakRoom:  case OfficeGreaterThan250Ft:  case Restroom:  result = 0.65;  case LibraryStacks:  case PharmacyArea:  case SportsArenaSeatsLT2000ClassIII:  result =1.10;  case SportsArenaSeatsLT5000ClassII:  result = 1.45;  case SportsArenaSeatsGTE5000ClassI:  result = 2.25;  case HospitalExamTreatment:  result = 1.15;  case HospitalOperatingRoom:  result = 1.90;   case RetailMallConcourse:  case HospitalRecovery: case  VideoConferencingStudio:    result = 0.90;  case KitchenCommercialFoodPreparation:  case HospitalNursery:  case ReligiousWorshipArea:  case LowVisionMultipurposeRoom: result = 0.95;   case GrocerySalesArea:   result = 1.05;    case ParkingGarageAreaDedicatedRamp: result = 0.25;   ParkingGarageBuildingParkingArea: result = 0.10;</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Iu04_SpaceFloorArea" type="com:SpaceFloorArea" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Area (ft[d:sup]2[/d:sup])</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input.</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Iu05_LightingPowerAllowance" type="comp:LightingPowerAllowance" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Allowed Wattage (Watts)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value:  result = PRODUCT (Iu03_AllowedLPD, Iu04_SpaceFloorArea)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Iu06a_AreaCategoryAdditionalAllowance" type="comp:AreaCategoryAdditionalAllowance" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Area Category</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input.  If selected, Section J is required.</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Iu06b_AdditionalAllowanceForPAF" type="comp:AdditionalAllowanceForPAF" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">PAF</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input.  If selected, Section P is required.</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="IuTotals_Heading" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">[d:b]TOTALS:[/d:b]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="IuTotals04Area_SpacesUnconditionedArea" type="com:SpacesUnconditionedArea" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated Value:  result = SUM All ( Iu04_SpaceFloorArea)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="IuTotals05AllowedWatt_LightingPowerAllowanceTotalUnconditioned" type="comp:LightingPowerAllowanceTotalUnconditioned" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated Value:  result = SUM All (Iu05_LightingPowerAllowance)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="IuErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage"  minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_J" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]J. ADDITIONAL ALLOWANCE: AREA CATEGORY METHOD QUALIFYING LIGHTING SYSTEM[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section applies if section I is required If section I IsPresent And for at least one row of data:  Ic06a_AreaCategoryAdditionalAllowance == true Or  Iu06a_AreaCategoryAdditionalAllowance== true.</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="J_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]All areas indicated in Table I as using an additional allowance using the Area Category Method have been included in this table to calculate the additional allowance per Table 140.6-C.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Jc_Conditioned" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Conditioned Spaces</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if in Section I at least one row of data has  Ic06a_AreaCategoryAdditionalAllowance== true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Jc_Label" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">[d:b]Conditioned Spaces[/d:b]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="JcTableSpaceFunction" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Jc01_SpaceDescription" type="com:SpaceDescription" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Area Description</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Auto-filled result = Ic01_SpaceDescription for row with Ic06a_AreaCategoryAdditionalAllowance == true</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Jc02_SpaceFunction" type="com:SpaceFunction" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Primary Function Area</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Auto-filled result = Ic02_SpaceFunction for space defined in Jc01_SpaceDescription</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="JcTableAddAllowance" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"></xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                                         <xsd:complexType>
                                                            <xsd:sequence>
                                                               <xsd:element name="Jc03_AreaCategoryAdditionalAllowanceType" type="comp:AreaCategoryAdditionalAllowanceType" >
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Applicable Qualifying Lighting System from Table 140.6-C</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User selects from dropdown list values dependent on Jc02_SpaceFunction as follows:   If Jc02_SpaceFunction == AuditoriumArea: list DisplayLighting and OrnamentalLighting; Else If Jc02_SpaceFunction == AutoRepairArea, ElectricalMechancialTelephoneRoom, MuseumRestoration,  GeneralCommercialIndustrialWorkAreaHighBay, Or  GeneralCommercialIndustrialWorkAreaLowBay:  list DetailedTaskLighting;   Else If Jc02_SpaceFunction == AudienceSeatingArea, CivicMeetingPlaceArea, HotelFunctionArea,  BarLoungeFineDiningArea, FastfoodDining, FamilyDiningArea,  FinancialTransactionsArea,  LibraryReadingArea, LobbyMainEntry, LoungeBreakRoom, ReligiousWorshipArea, RetailMallConcourse, TheaterMotionPicture, TheaterPerformance, LowVisionWaitingArea,  LowVisionMultipurposeRoom, LowVisionReligiousWorshipArea Or LowVisionDiningArea:   list OrnamentalLighting;  Else If Jc02_SpaceFunction ==  BeautySalonArea: list DetailedTaskLighting and OrnamentalLighting;  Else If Jc02_SpaceFunction == ClassroomLectureTrainingVocationalArea: list WhiteChalkBoardLighting;   Else If Jc02_SpaceFunction == ExhibitMuseumArea , TransportationFunctionTicketing Or LowVisionRestroom: list DisplayLighting;  Else If Jc02_SpaceFunction ==  GeneralCommercialIndustrialWorkAreaPrecision:  list PrecisionLightingIndustrial;   Else If Jc02_SpaceFunction ==  Office250FtOrLess, OfficeGreaterThan250Ft Or OfficeOpenPlan:  list PortableOfficeLighting;   Else If Jc02_SpaceFunction == ParkingGarageBuildingParkingArea: list ParkingGarageATM; Else If Jc02_SpaceFunction == PharmacyArea Or LaboratoryScientific:  list SpecializedTaskLighting;  Else If Jc02_SpaceFunction == GrocerySalesArea Or RetailSalesArea:  list DisplayLighting and DecorativeLighting;  Else If Jc02_SpaceFunction == RetailSalesFittingRoom: list MirrorLightingExternal and MirrorLightingInternal;  Else If Jc02_SpaceFunction == HospitalNursery, HospitalNurseStation, HospitalPhysicalTherapy  Or HospitalRecovery:  list TunableWhiteLuminaires;  Else If Jc02_SpaceFunction == HospitalPatientRoom:  list TunableWhiteLuminaires and DecorativeLighting;  Else If Jc02_SpaceFunction == Restroom Or Stairwell:  list DisplayLighting and DecorativeLighting;  Else If Jc02_SpaceFunction == VideoConferencingStudio: list VideoConferencing;  Else If Jc02_SpaceFunction ==  LowVisionLobbyMainEntry: list OrnamentalLighting and LowVisionMainEntryTransition;  Else If Jc02_SpaceFunction == LowVisionCorridorArea:  list DecorativeLighting.</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:choice maxOccurs="1">
                                                                  <xsd:element name="Jc04_AdditionalAllowedLPD" type="comp:AdditionalAllowedLPD" >
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Allowed Density (W/ft[d:sup]2[/d:sup] or W/lf or W/unit)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">Calculated field:  Assigned results are W/ft2 stored in AdditionalAllowedLPD  Switch (Jc03_ AreaCategoryAdditionalAllowanceType) case OrnamentalLighting: result = 0.30; case DisplayLighting: If Jc02_SpaceFunction == ExhibitMuseumArea result = 0.50; Else result = 0.20; case DetailedTaskLighting result = 0.20;    case PrecisionLightingIndustrial: result = 0.70;   case PortableOfficeLighting: result = 0.20;  case SpecializedTaskLighting result = 0.35; case DecorativeLighting: result = 0.15; case TunableWhiteLuminaires: result = 0.10; case VideoConferencing: result = 1.00; case LowVisionMainEntryTransition: result = 0.95;</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Jc04_AdditionalAllowedPowerPerFoot" type="comp:AdditionalAllowedPowerPerFoot" >
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Allowed Density (W/ft[d:sup]2[/d:sup] or W/lf or W/unit)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">Calculated field:  Assigned results are W/ft stored in AdditionalAllowedPowerPerFoot; Switch (Jc03_ AreaCategoryAdditionalAllowanceType) case WhiteChalkBoardLighting: result = 4.50;</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Jc04_AdditionalAllowedPowerPerUnit" type="comp:AdditionalAllowedPowerPerUnit" >
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Allowed Density (W/ft[d:sup]2[/d:sup] or W/lf or W/unit)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">Calculated field:  Assigned results are W/unit stored in  AdditionalAllowedPowerPerUnit; Switch (Jc03_ AreaCategoryAdditionalAllowanceType) case MirrorLightingExternal: result = 40;  case MirrorLightingInternal: result = 120;</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Jc04_NotApplicableMessage" >
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Allowed Density (W/ft[d:sup]2[/d:sup] or W/lf or W/unit)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">Calculated field:  If Jc03_ AreaCategoryAdditionalAllowanceType == ParkingGarageATM result = BlankGrayedOut stored in NotApplicableMessage;</xsd:documentation>
                                                                     </xsd:annotation>
                                                                     <xsd:simpleType>
                                                                        <xsd:restriction base="comp:NotApplicableMessage">
                                                                           <xsd:enumeration value="BlankGrayedOut"/>
                                                                        </xsd:restriction>
                                                                     </xsd:simpleType>
                                                                  </xsd:element>
                                                               </xsd:choice>
                                                               <xsd:choice maxOccurs="1">
                                                                  <xsd:element name="Jc05_SpaceFloorArea" type="com:SpaceFloorArea" >
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Ltg Area, Length or ATM/Mirror  (ft[d:sup]2[/d:sup], lf or #)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">If Jc04_AdditionalAllowedLPD NotNull User enters floor area;</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Jc05_BoardLightingLength" type="lit:BoardLightingLength" >
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Ltg Area, Length or ATM/Mirror (ft[d:sup]2[/d:sup], lf or #)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">If Jc04_AdditionalAllowedPowerPerFoot NotNull User enters the length of the white/chalk board.</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Jc05_MirrorsCount" type="lit:MirrorsCount" >
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Ltg Area, Length or ATM/Mirror (ft[d:sup]2[/d:sup], lf or #)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">If Jc03_ AreaCategoryAdditionalAllowanceType == MirrorLightingExternal Or MirrorLightingInternal User enters number of mirrors</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Jc05_BankingKioskCount" type="lit:BankingKioskCount" >
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Ltg Area, Length or ATM/Mirror  (ft[d:sup]2[/d:sup], lf or #)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">If Jc03_ AreaCategoryAdditionalAllowanceType == ParkingGarageATM:  User enters number of ATM's</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                               </xsd:choice>
                                                               <xsd:element name="Jc06_ExtraPowerCalculatedForArea" type="comp:ExtraPowerCalculatedForArea" >
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Extra Allowance (Watts)</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">Calculated value:  If Jc04_AdditionalAllowedLPD NotNull  result = 
PRODUCT (Jc04_AdditionalAllowedLPD, Jc05_SpaceFloorArea); Else If Jc04_AdditionalAllowedPowerPerFoot NotNull result = PRODUCT (Jc04_AdditionalAllowedPowerPerFoot, Jc05_BoardLightingLength); Else If  Jc03_ AreaCategoryAdditionalAllowanceType == MirrorLightingExternal Or   MirrorLightingInterna result = PRODUCT (Jc04_AdditionalAllowedPowerPerUnit, Jc05_MirrorsCount); Else If Jc03_ AreaCategoryAdditionalAllowanceType == ParkingGarageATM:  result = 100 + PRODUCT (Jc05_BankingKioskCount-1, 50)</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                            </xsd:sequence>
                                                         </xsd:complexType>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:element name="JcTableLuminaires" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"></xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                                         <xsd:complexType>
                                                            <xsd:sequence>
                                                               <xsd:element name="Jc07_LuminaireName" type="lit:LuminaireName" >
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Luminaire Name or Item Tag</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User input: Dropdown list auto-filled from values for Fc01_LuminaireName</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Jc08_LuminairePower" type="lit:LuminairePower" >
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Watts  per Luminaire</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">autofilled result = Fc04_LuminairePower for luminaire selected in Jc07__LuminaireName</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Jc09_LuminaireCountByType" type="lit:LuminaireCountByType" >
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Number of Luminaires</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Jc10_LuminairePowerByType" type="lit:LuminairePowerByType" >
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Total Design Watts</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">Calculated value:   result = PRODUCT (Jc08_LuminairePower, Jc09_LuminaireCountByType)</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                            </xsd:sequence>
                                                         </xsd:complexType>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:element name="Jc10a_AllLuminairePowerInArea" type="lit:AllLuminairePowerInArea" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Total Design Watts</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value: result = SUM All ( Jc10_LuminairePowerByType)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Jc10b_AllExtraPowerCalculatedForArea" type="comp:AllExtraPowerCalculatedForArea" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Calculated Allowance (Watts):</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value: result = SUM All (Jc06_ExtraPowerCalculatedForArea)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Jc10c_AllExtraPowerAllowedForArea" type="comp:AllExtraPowerAllowedForArea" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Total Additional Allowance for this area:</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value:  result = Smallest (Jc10a_AllLuminairePowerInArea,   Jc10b_AllExtraPowerCalculatedForArea)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="Jc11_AreaAdditionalLightingPowerAllowedConditioned" type="comp:AreaAdditionalLightingPowerAllowedConditioned" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">[d:b]Total Additional Allowance (Watts) CONDITIONED SPACES[/d:b]</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  result = SUM All ( Jc10c_AllExtraPowerAllowedForArea)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="JcErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Ju_Unconditioned" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Unconditioned Spaces</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if in Section I at least one row of data with   Iu06a_AreaCategoryAdditionalAllowance == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Ju_Label" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">[d:b]Unconditioned Spaces[/d:b]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="JuTableSpaceFunction" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Ju01_SpaceDescription" type="com:SpaceDescription" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Area Description</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Auto-filled from  Iu01_SpaceDescription for row with Iu06a_AreaCategoryAdditionalAllowance == true</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Ju02_SpaceFunction" type="com:SpaceFunction" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Primary Function Area</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Auto-filled from Iu02_SpaceFunction for space defined in Ju01_SpaceDescription</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="JuTableAddAllowance" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"></xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                                         <xsd:complexType>
                                                            <xsd:sequence>
                                                               <xsd:element name="Ju03_AreaCategoryAdditionalAllowanceType" type="comp:AreaCategoryAdditionalAllowanceType" >
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Applicable Qualifying Lighting System from Table 140.6-C</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User selects from dropdown list values dependent on Ju02_SpaceFunction as follows:   If Ju02_SpaceFunction == AuditoriumArea: list DisplayLighting and OrnamentalLighting; Else If Ju02_SpaceFunction == AutoRepairArea, ElectricalMechancialTelephoneRoom, MuseumRestoration,  GeneralCommercialIndustrialWorkAreaHighBay, Or  GeneralCommercialIndustrialWorkAreaLowBay:  list DetailedTaskLighting;   Else If Ju02_SpaceFunction == AudienceSeatingArea, CivicMeetingPlaceArea, HotelFunctionArea,  BarLoungeFineDiningArea, FastfoodDining, FamilyDiningArea,  FinancialTransactionsArea,  LibraryReadingArea, LobbyMainEntry, LoungeBreakRoom, ReligiousWorshipArea, RetailMallConcourse, TheaterMotionPicture, TheaterPerformance, LowVisionWaitingArea,  LowVisionMultipurposeRoom, LowVisionReligiousWorshipArea Or LowVisionDiningArea:   list OrnamentalLighting;  Else If Ju02_SpaceFunction ==  BeautySalonArea: list DetailedTaskLighting and OrnamentalLighting;  Else If Ju02_SpaceFunction == ClassroomLectureTrainingVocationalArea: list WhiteChalkBoardLighting;   Else If Ju02_SpaceFunction == ExhibitMuseumArea , TransportationFunctionTicketing Or LowVisionRestroom: list DisplayLighting;  Else If Ju02_SpaceFunction ==  GeneralCommercialIndustrialWorkAreaPrecision:  list PrecisionLightingIndustrial;   Else If Ju02_SpaceFunction ==  Office250FtOrLess, OfficeGreaterThan250Ft Or OfficeOpenPlan:  list PortableOfficeLighting;   Else If Ju02_SpaceFunction == ParkingGarageBuildingParkingArea: list ParkingGarageATM; Else If Ju02_SpaceFunction == PharmacyArea Or LaboratoryScientific:  list SpecializedTaskLighting;  Else If Ju02_SpaceFunction == GrocerySalesArea Or RetailSalesArea:  list DisplayLighting and DecorativeLighting;  Else If Ju02_SpaceFunction == RetailSalesFittingRoom: list MirrorLightingExternal and MirrorLightingInternal;  Else If Ju02_SpaceFunction == HospitalNursery, HospitalNurseStation, HospitalPhysicalTherapy  Or HospitalRecovery:  list TunableWhiteLuminaires;  Else If Ju02_SpaceFunction == HospitalPatientRoom:  list TunableWhiteLuminaires and DecorativeLighting;  Else If Ju02_SpaceFunction == Restroom Or Stairwell:  list DisplayLighting and DecorativeLighting;  Else If Ju02_SpaceFunction == VideoConferencingStudio: list VideoConferencing;  Else If Ju02_SpaceFunction ==  LowVisionLobbyMainEntry: list OrnamentalLighting and LowVisionMainEntryTransition;  Else If Ju02_SpaceFunction == LowVisionCorridorArea:  list DecorativeLighting.</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:choice maxOccurs="1">
                                                                  <xsd:element name="Ju04_AdditionalAllowedLPD" type="comp:AdditionalAllowedLPD" >
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Allowed Density (W/ft[d:sup]2[/d:sup] or W/lf or W/unit)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">Calculated field:  Assigned results are W/ft2 stored in AdditionalAllowedLPD  Switch (Ju03_ AreaCategoryAdditionalAllowanceType) case OrnamentalLighting: result = 0.30; case DisplayLighting: If Ju02_SpaceFunction == ExhibitMuseumArea result = 0.50; Else result = 0.20; case DetailedTaskLighting result = 0.20;    case PrecisionLightingIndustrial: result = 0.70;   case PortableOfficeLighting: result = 0.20;  case SpecializedTaskLighting result = 0.35; case DecorativeLighting: result = 0.15; case TunableWhiteLuminaires: result = 0.10;  case VideoConferencing: result = 1.00; case LowVisionMainEntryTransition: result = 0.95;</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Ju04_AdditionalAllowedPowerPerFoot" type="comp:AdditionalAllowedPowerPerFoot" >
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Allowed Density (W/ft[d:sup]2[/d:sup] or W/lf or W/unit)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">Calculated field:  Assigned results are W/ft stored in AdditionalAllowedPowerPerFoot; Switch (Ju03_ AreaCategoryAdditionalAllowanceType) case WhiteChalkBoardLighting: result = 4.50;</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Ju04_AdditionalAllowedPowerPerUnit" type="comp:AdditionalAllowedPowerPerUnit" >
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Allowed Density (W/ft[d:sup]2[/d:sup] or W/lf or W/unit)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">Calculated field:  Assigned results are W/unit stored in  AdditionalAllowedPowerPerUnit; Switch (Ju03_ AreaCategoryAdditionalAllowanceType) case MirrorLightingExternal: result = 40;  case MirrorLightingInternal: result = 120;</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Ju04_NotApplicableMessage" >
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Allowed Density (W/ft[d:sup]2[/d:sup] or W/lf or W/unit)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">Calculated field:  If Ju03_ AreaCategoryAdditionalAllowanceType == ParkingGarageATM result = BlankGrayedOut stored in NotApplicableMessage;</xsd:documentation>
                                                                     </xsd:annotation>
                                                                     <xsd:simpleType>
                                                                        <xsd:restriction base="comp:NotApplicableMessage">
                                                                           <xsd:enumeration value="BlankGrayedOut"/>
                                                                        </xsd:restriction>
                                                                     </xsd:simpleType>
                                                                  </xsd:element>
                                                               </xsd:choice>
                                                               <xsd:choice maxOccurs="1">
                                                                  <xsd:element name="Ju05_SpaceFloorArea" type="com:SpaceFloorArea" >
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Ltg Area, Length or ATMs  (ft[d:sup]2[/d:sup], lf or #)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">If Ju04_AdditionalAllowedLPD NotNull User enters floor area</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Ju05_BoardLightingLength" type="lit:BoardLightingLength" >
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Ltg Area, Length or ATMs  (ft[d:sup]2[/d:sup], lf or #)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">If Ju04_AdditionalAllowedPowerPerFoot NotNull User enters the length of the white/chalk board</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Ju05_MirrorsCount" type="lit:MirrorsCount" >
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Ltg Area, Length or ATM/Mirror (ft[d:sup]2[/d:sup], lf or #)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">If Ju03_ AreaCategoryAdditionalAllowanceType == MirrorLightingExternal Or MirrorLightingInternal User enters number of mirrors</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Ju05_BankingKioskCount" type="lit:BankingKioskCount" >
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Ltg Area, Length or ATMs  (ft[d:sup]2[/d:sup], lf or #)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">If Ju03_ AreaCategoryAdditionalAllowanceType == ParkingGarageATM:  User enters number of ATMs</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                               </xsd:choice>
                                                               <xsd:element name="Ju06_ExtraPowerCalculatedForArea" type="comp:ExtraPowerCalculatedForArea" >
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Extra Allowance (Watts)</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">Calculated value:  If Ju04_AdditionalAllowedLPD NotNull  result = PRODUCT (Ju04_AdditionalAllowedLPD, Ju05_SpaceFloorArea); Else If Ju04_AdditionalAllowedPowerPerFoot NotNull result = PRODUCT (Ju04_AdditionalAllowedPowerPerFoot, Ju05_BoardLightingLength); Else If  Ju03_ AreaCategoryAdditionalAllowanceType == MirrorLightingExternal Or   MirrorLightingInterna result = PRODUCT (Ju04_AdditionalAllowedPowerPerUnit, Ju05_MirrorsCount); Else If Ju03_ AreaCategoryAdditionalAllowanceType == ParkingGarageATM:  result = 100 + PRODUCT (Ju05_BankingKioskCount-1, 50);</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                            </xsd:sequence>
                                                         </xsd:complexType>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:element name="JuTableLuminaires" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"></xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                                         <xsd:complexType>
                                                            <xsd:sequence>
                                                               <xsd:element name="Ju07_LuminaireName" type="lit:LuminaireName" >
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Luminaire Name or Item Tag</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User input: Dropdown list auto-filled from values for Fu01_LuminaireName</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Ju08_LuminairePower" type="lit:LuminairePower" >
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Watts  per Luminaire</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">autofilled result = Fu04_LuminairePower for luminaire selected in Ju07__LuminaireName</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Ju09_LuminaireCountByType" type="lit:LuminaireCountByType" >
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Number of Luminaires</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Ju10_LuminairePowerByType" type="lit:LuminairePowerByType" >
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Total Design Watts</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">Calculated value:   result = PRODUCT (Ju08_LuminairePower, Ju09_LuminaireCountByType)</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                            </xsd:sequence>
                                                         </xsd:complexType>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:element name="Ju10a_AllLuminairePowerInArea" type="lit:AllLuminairePowerInArea" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Total Design Watts</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value: result = SUM All ( Ju10_LuminairePowerByType)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Ju10b_AllExtraPowerCalculatedForArea" type="comp:AllExtraPowerCalculatedForArea" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Calculated Allowance (Watts):</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value: result = SUM All ( Ju06_ExtraPowerCalculatedForArea</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Ju10c_AllExtraPowerAllowedForArea" type="comp:AllExtraPowerAllowedForArea" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Total Additional Allowance for this area:</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value: result = Smallest (Ju10a_AllLuminairePowerInArea,  ju10b_AllExtraPowerCalculatedForArea)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="Ju11_AreaAdditionalLightingPowerAllowedUnconditioned" type="comp:AreaAdditionalLightingPowerAllowedUnconditioned" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">[d:b]Total Additional Allowance (Watts) UNCONDITIONED SPACES[/d:b]</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value: result = SUM All ( Ju10c_AllExtraPowerAllowedForArea)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="JuErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_K" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]K. TAILORED METHOD GENERAL LIGHTING POWER ALLOWANCE[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section applies if in section B at least one value for Bnew02_LightingPrescriptiveMethodNR, Bnew04_LightingPrescriptiveMethodNR,   Baltered02_LightingPrescriptiveMethodNR Or Baltered04_LightingPrescriptiveMethodNR == Tailored Or AreaCategoryAndTailored</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="K_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table includes areas using the Tailored Method per [d:t24ref/]140.6(c)3.  If additional Tailored Method "use it or lose it" allowances are used it is also indicated.  Lighting that qualifies for one of the additional allowances shall not qualify for another additional allowance. [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="K_ColumnHeading00cTo00f" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Additional "Use it or lose it" Allowance (Watts)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="K_ColumnHeading02To09" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Calculated General Lighting Power Allowance</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Kc_Conditioned" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Conditioned Spaces</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if in section B at least one value for Bnew02_LightingPrescriptiveMethodNR Or Baltered02_LightingPrescriptiveMethodNR == Tailored Or AreaCategoryAndTailored And at least one value for Bnew03_NewLightingConditionedFloorArea  Or Baltered03_AlteredLightingConditionedFloorArea GT 0</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Kc_Label" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">[d:b]Conditioned Spaces[/d:b]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Kc00a_TailoredPowerAllowedGeneral" type="comp:TailoredPowerAllowedGeneral" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">General Lighting Power Allowance   Table K(below)</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Auto-filled result =  Kc10_TailoredPowerAllowedGeneral</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Kc00c1_TailoredWallDisplay" type="comp:TailoredWallDisplay" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Wall Display Lighting</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input; If user checks box, Section L is required.</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Kc00c2_PowerAllowedWallDisplay" type="comp:PowerAllowedWallDisplay" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Table L</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  If Kc00c1_TailoredWallDisplay == true then result =  Lc12_PowerAllowedWallDisplay; Else result = 0</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Kc00d1_TailoredFloorDisplayOrTask" type="comp:TailoredFloorDisplayOrTask" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Floor Display &amp; Task Lighting</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input; If user checks box, Section M is required.</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Kc00d2_PowerAllowedFloorDisplayOrTask" type="comp:PowerAllowedFloorDisplayOrTask" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Table M</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  If Kc00d1_TailoredFloorDisplayOrTask == true then result = Mc12_PowerAllowedFloorDisplayOrTask; Else result = 0</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Kc00e1_TailoredOrnamentalSpecialEffects" type="comp:TailoredOrnamentalSpecialEffects" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Ornamental &amp; Special Effects</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input; If user checks box, Section N is required.</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Kc00e2_PowerAllowedOrnamentalSpecialEffects" type="comp:PowerAllowedOrnamentalSpecialEffects" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Table N</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  If Kc00e1_TailoredOrnamentalSpecialEffects == true then result = Nc10_PowerAllowedOrnamentalSpecialEffects; Else result = 0</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Kc00f1_TailoredVeryValuableDisplay" type="comp:TailoredVeryValuableDisplay" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Very Valuable Merchandise</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input; If user checks box, Section O is required.</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Kc00f2_PowerAllowedVeryValuableDisplay" type="comp:PowerAllowedVeryValuableDisplay" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Table O</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  If Kc00f1_TailoredVeryValuableDisplay == true then result = Oc14_PowerAllowedVeryValuableDisplay; Else result = 0</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Kc01_TailoredTotalLightingPowerAllowedConditioned" type="comp:TailoredTotalLightingPowerAllowedConditioned" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Total Allowed Watts using Tailored Method</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  result = SUM ( Kc00a_TailoredPowerAllowedGeneral, Kc00c2_PowerAllowedWallDisplay,  Kc00d2_PowerAllowedFloorDisplayOrTask, Kc00e2_PowerAllowedOrnamentalSpecialEffects, Kc00f2_PowerAllowedVeryValuableDisplay)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="KcTableTailoredGeneral" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Kc02_SpaceDescription" type="com:SpaceDescription" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Area Description (per room)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input.</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Kc03_SpaceFunctionTailored" type="comp:SpaceFunctionTailored" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Primary Function Area per Table 140.6-D</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from dropdown list loaded with space functions allowed in tailored method: AuditoriumArea, ConferenceArea, ConventionArea, MeetingCenterArea, MultipurposeArea, DiningArea, ExhibitMuseumArea, HotelBallroomEventsArea, HotelLobby, MainEntryLobby, MotionPictureTheater, PerformanceTheater, ReligiousWorshipArea, GroceryStoreArea, RetailMerchandiseSales, ShowroomArea.</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Kc04_SpaceIlluminationLevel" type="lit:SpaceIlluminationLevel" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Illuminance Value (LUX)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value: If Kc03_SpaceFunctionTailored == AuditoriumArea, ConferenceArea, ConventionArea, MeetingCenterArea, MultipurposeArea, Or ReligiousWorshipArea  result = 300; Else if Kc03_SpaceFunctionTailored == DiningArea, HotelLobby, MainEntryLobby, MotionPictureTheater Or PerformanceTheater result = 200;  Else if  Kc03_SpaceFunctionTailored == ExhibitMuseumArea  then result = 150; Else If Kc03_SpaceFunctionTailored == GroceryStoreArea  then result = 600; Else If Kc03_SpaceFunctionTailored == HotelBallroomEventsArea  result = 400; Else If Kc03_SpaceFunctionTailored == RetailMerchandiseSales Or ShowroomArea result = 500;</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Kc05_IsSpaceRectangular" type="com:IsSpaceRectangular" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Room Configuration</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input.  Dropdown list displays Rectangular for true value and Non-Rectangular for false value.</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Kc05a_SpaceLength" type="com:SpaceLength" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Length of Room (ft)</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If IsSpaceRectangular == true user input value stored in Kc05a_SpaceLength; Else result = NotApplicable stored in the NotApplicableMessage.</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Kc05a_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Not Applicable</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If IsSpaceRectangular = true user input value stored in Kc05a_SpaceLength; Else result =  NotApplicable stored in the NotApplicableMessage.</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Kc05b_SpaceWidth" type="com:SpaceWidth" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Room Width (ft)</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If IsSpaceRectangular == true, user enters the width of the space stored in Kc05b_SpaceWidth; Else user enters the perimeter of the space stored in Kc05b_SpacePerimeter.</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Kc05b_SpacePerimeter" type="com:SpacePerimeter" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Perimeter (ft)</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If IsSpaceRectangular == true, user enters the width of the space stored in Kc05b_SpaceWidth; Else user enters the perimeter of the space stored in Kc05b_SpacePerimeter.</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="Kc05c_Height" type="dtyp:Height" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Height (ft)[d:sup]1[/d:sup]</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Kc05d_RoomCavityRatio" type="com:RoomCavityRatio" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Room Cavity Ratio (RCR)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value using equations in TABLE 140.6-F:  If kc05_IsSpaceRectanfular == true then result = (5 x Kc05c_Height x (Kc05a_SpaceLength + Kc05b_SpaceWidth)) / (Kc05a_SpaceLength x Kc05b_SpaceWidth); Else result = 2.5 x Kc05c_Height x Kc05b_SpaceWidth / Kc07_SpaceFloorArea.</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Kc06_TailoredGeneralLPD" type="comp:TailoredGeneralLPD" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Allowed Density (W/ft[d:sup]2[/d:sup])</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value based on values for Kc04_SpaceIlluminationLevel, referred to as lux in formula and Kc05d_RoomCavityRatio, referred to as rcr in formula. In formula LTE stands for less than or equal to and GT stands for greater than.   Formula:   Switch (Kc04_SpaceIlluminationLevel) case 150: If rcr LTE 2.0  result = 0.40; Else If rcr LTE 3.5 result = 0.45; Else If  rcr LTE 7.0  result = 0.60; Else If  rcr GT 7.0 result = 0.75;  case 200: If rcr LTE 2.0 result = 0.45; Else If  rcr LTE 3.5 result = 0.55; Else If  rcr LTE 7.0 result = 0.75;  Else If rcr GT 7.0 result = 1.00;  case 300: If rcr LTE 2.0 result = 0.65; Else If  rcr LTE 3.5 result = 0.80; Else If  rcr LTE 7.0 result = 1.00; Else If  rcr GT 7.0 result = 1.40;  case 400: If rcr LTE 2.0 result = 0.75; Else If  rcr LTE 3.5 result = 0.95; Else If  rcr LTE 7.0 result = 1.25; Else If  rcr GT 7.0 result = 1.50;  case 500: If rcr LTE 2.0 result = 0.90; Else If  rcr LTE 3.5 result = 1.05; Else If  rcr LTE 7.0 result = 1.45; Else If  rcr GT 7.0 result = 1.85; case 600: If rcr LTE 2.0 result = 1.08; Else If  rcr LTE 3.5 result = 1.24; Else If  rcr LTE 7.0 result = 1.64; Else If  rcr GT 7.0 result = 2.38;</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Kc07_SpaceFloorArea" type="com:SpaceFloorArea" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Area (ft[d:sup]2[/d:sup])</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Kc08_TailoredPowerAllowedForArea" type="comp:TailoredPowerAllowedForArea" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Extra Allowance (Watts)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value:  result = PRODUCT (Kc06_TailoredGeneralLPD, Kc07_SpaceFloorArea)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Kc09_AdditionalAllowanceForPAF" type="comp:AdditionalAllowanceForPAF" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Power Adjustment   (See Table P)   [d:line1/]  PAF</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input. Checked result = true and unchecked result = false.  If result == true, section P is required.</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="Kc10_TailoredPowerAllowedGeneral" type="comp:TailoredPowerAllowedGeneral" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">[d:b]Total General Power Allowance CONDITIONED SPACES:[/d:b]</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  result = SUM All (Kc08_TailoredPowerAllowedForArea)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="KcErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage"  minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Ku_Unconditioned" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Unconditioned Spaces</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if in section B at least one value for Bnew04_LightingPrescriptiveMethodNR Or Baltered04_LightingPrescriptiveMethodNR == Tailored Or AreaCategoryAndTailored And at least one value for Bnew05_NewLightingUnconditionedFloorArea Or Baltered05_AlteredLightingUnconditionedFloorArea GT 0</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
