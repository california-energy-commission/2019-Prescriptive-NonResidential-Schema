                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Table O</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  If Ku00f1_TailoredVeryValuableDisplay == true then result = Ou14_PowerAllowedVeryValuableDisplay; Else result = 0</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Ku01_TailoredTotalLightingPowerAllowedConditioned" type="comp:TailoredTotalLightingPowerAllowedConditioned" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Total Allowed Watts using Tailored Method</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  result = SUM ( Ku00a_TailoredPowerAllowedGeneral, Ku00c2_PowerAllowedWallDisplay,  Ku00d2_PowerAllowedFloorDisplayOrTask, Ku00e2_PowerAllowedOrnamentalSpecialEffects, Ku00f2_PowerAllowedVeryValuableDisplay);</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="KuTableTailoredGeneral" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Ku02_SpaceDescription" type="com:SpaceDescription" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Area Description (per room)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input.</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Ku03_SpaceFunctionTailored" type="comp:SpaceFunctionTailored" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Primary Function Area per Table 140.6-D</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from dropdown list loaded with space functions allowed in tailored method: AuditoriumArea, ConferenceArea, ConventionArea, MeetingCenterArea, MultipurposeArea, DiningArea, ExhibitMuseumArea, HotelBallroomEventsArea, HotelLobby, MainEntryLobby, MotionPictureTheater, PerformanceTheater, ReligiousWorshipArea, GroceryStoreArea, RetailMerchandiseSales, ShowroomArea.</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Ku04_SpaceIlluminationLevel" type="lit:SpaceIlluminationLevel" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Illuminance Value (LUX)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value: If Ku03_SpaceFunctionTailored == AuditoriumArea, ConferenceArea, ConventionArea, MeetingCenterArea, MultipurposeArea, Or ReligiousWorshipArea  result = 300; Else if Ku03_SpaceFunctionTailored == DiningArea, HotelLobby, MainEntryLobby, MotionPictureTheater Or PerformanceTheater result = 200;  Else if  Ku03_SpaceFunctionTailored == ExhibitMuseumArea  then result = 150; Else If Ku03_SpaceFunctionTailored == GroceryStoreArea  then result = 600; Else If Ku03_SpaceFunctionTailored == HotelBallroomEventsArea  result = 400; Else If Ku03_SpaceFunctionTailored == RetailMerchandiseSales Or ShowroomArea result = 500;</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Ku05_IsSpaceRectangular" type="com:IsSpaceRectangular" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Room Configuration</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input.  Dropdown list displays Rectangular for true value and Non-Rectangular for false value.</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Ku05a_SpaceLength" type="com:SpaceLength" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Length of Room (ft)</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If IsSpaceRectangular == true user input value stored in Ku05a_SpaceLength; Else result =  NotApplicable stored in the NotApplicableMessage.</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Ku05a_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Not Applicable</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If IsSpaceRectangular == true user input value stored in Ku05a_SpaceLength; Else result =  NotApplicable stored in the NotApplicableMessage.</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Ku05b_SpaceWidth" type="com:SpaceWidth" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Room Width (ft)</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If IsSpaceRectangular == true, user enters the width of the space stored in Ku05b_SpaceWidth; Else user enters the perimeter of the space stored in Ku05b_SpacePerimeter.</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Ku05b_SpacePerimeter" type="com:SpacePerimeter" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Perimeter (ft)</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If IsSpaceRectangular == true, user enters the width of the space stored in Ku05b_SpaceWidth; Else user enters the perimeter of the space stored in Ku05b_SpacePerimeter.</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="Ku05c_Height" type="dtyp:Height" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Height (ft)[d:sup]1[/d:sup]</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Ku05d_RoomCavityRatio" type="com:RoomCavityRatio" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Room Cavity Ratio (RCR)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value using equations in TABLE 140.6-F:  If Ku05_IsSpaceRectanfular == true then result = (5 x Ku05c_Height x (Ku05a_SpaceLength + Ku05b_SpaceWidth)) / (Ku05a_SpaceLength x Ku05b_SpaceWidth); Else result = 2.5 x Ku05c_Height x Ku05b_SpaceWidth / Ku07_SpaceFloorArea.</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Ku06_TailoredGeneralLPD" type="comp:TailoredGeneralLPD" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Allowed Density (W/ft[d:sup]2[/d:sup])</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value based on values for Ku04_SpaceIlluminationLevel, referred to as lux in formula and Ku05d_RoomCavityRatio, referred to as rcr in formula. In formula LTE stands for less than or equal to and GT stands for greater than.   Formula:   Switch (Ku04_SpaceIlluminationLevel) case 150: If rcr LTE 2.0  result = 0.40; Else If rcr LTE 3.5 result = 0.45; Else If  rcr LTE 7.0  result = 0.60; Else If  rcr GT 7.0 result = 0.75;  case 200: If rcr LTE 2.0 result = 0.45; Else If  rcr LTE 3.5 result = 0.55; Else If  rcr LTE 7.0 result = 0.75;  Else If rcr GT 7.0 result = 1.00;  case 300: If rcr LTE 2.0 result = 0.65; Else If  rcr LTE 3.5 result = 0.80; Else If  rcr LTE 7.0 result = 1.00; Else If  rcr GT 7.0 result = 1.40;  case 400: If rcr LTE 2.0 result = 0.75; Else If  rcr LTE 3.5 result = 0.95; Else If  rcr LTE 7.0 result = 1.25; Else If  rcr GT 7.0 result = 1.50;  case 500: If rcr LTE 2.0 result = 0.90; Else If  rcr LTE 3.5 result = 1.05; Else If  rcr LTE 7.0 result = 1.45; Else If  rcr GT 7.0 result = 1.85; case 600: If rcr LTE 2.0 result = 1.08; Else If  rcr LTE 3.5 result = 1.24; Else If  rcr LTE 7.0 result = 1.64; Else If  rcr GT 7.0 result = 2.38;</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Ku07_SpaceFloorArea" type="com:SpaceFloorArea" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Area (ft[d:sup]2[/d:sup])</xsd:documentation>
