                                             <xsd:enumeration value="EMCS"/>
                                             <xsd:enumeration value="ExemptShutOffControl"/>
                                             <xsd:enumeration value="OccupancySensor"/>
                                             <xsd:enumeration value="OtherShutOffControl"/>
                                             <xsd:enumeration value="PartialOffShutOffControl"/>
                                             <xsd:enumeration value="PartialOnShutOffControl"/>
                                             <xsd:enumeration value="VacancyShutOffControl"/>
                                          </xsd:restriction>
