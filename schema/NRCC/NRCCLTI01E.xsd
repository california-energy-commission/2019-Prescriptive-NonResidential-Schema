         <xsd:element name="Section_P" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]P.POWER ADJUSTMENT: LIGHTING CONTROL CREDIT  (POWER ADJUSTMENT FACTOR (PAF))[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section applies if section I is required and in section I at least one value in Ic06b_AdditionalAllowanceForPAF Or Iu06b_AdditionalAllowanceForPAF == true Or if section K is required and at least one value in Kc09_AdditionalAllowanceForPAF or Ku09_AdditionalAllowanceForPAF == true</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="P_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table includes all areas indicated in Table I or Table K as using a PAF credit described in [d:t24ref/]140.6(a)2.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="P_ColumnHeading02" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">PAF per [d:t24ref/]140.6(a)2[d:sup]1[/d:sup] [d:line1/] (*Can be used in conjunction with other PAF's)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="P_ColumnSubheading02aTo02d" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Pick up to one</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User can select one PAF credit type from this group 1, 2A, 2B, 2C</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="P_ColumnSubheading02eTo02g" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Pick up to one</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User can select one PAF credit type from this group 3A, 3B, 4</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="P_ColumnSubheading02hTo02j" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Pick up to one [d:sup]2[/d:sup]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User can select one PAF credit type from this group 5, 6, 7</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="P_ColumnHeading03To06" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Luminaires Controlled for PAF Credit</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Pc_Conditioned" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if at least one value in Ic06b_AdditionalAllowanceForPAF Or Kc09_AdditionalAllowanceForPAF == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Pc_ConditionedLabel" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">[d:b]Conditioned Spaces[/d:b]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="PcTablePAF" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Pc01_SpaceDescription" type="com:SpaceDescription" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Area Description</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Rows auto-filled with values from Ic01_SpaceDescription where Ic06b_AdditionalAllowanceForPAF == true And values from Kc02_SpaceDescription  where Kc09_AdditionalAllowanceForPAF == true.  If user adds another luminaire, this field value is repeated.</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pc02a_ApplyDaylightDimmingPlusOff" type="comp:ApplyDaylightDimmingPlusOff" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">1</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input: checked result = true, unchecked result = false.  If result == true add 0.10 to PAF value used in calculation for Pc07_PAF_AllowedInArea</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pc02b_ApplyOccupantSensingControlsUpTo125SF" type="comp:ApplyOccupantSensingControlsUpTo125SF" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">2A</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input: checked result = true, unchecked result = false.  If result == true add 0.40 to PAF value used in calculation for Pc07_PAF_AllowedInArea</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pc02c_ApplyOccupantSensingControlsFrom126To250SF" type="comp:ApplyOccupantSensingControlsFrom126To250SF" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">2B</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input: checked result = true, unchecked result = false.  If result == true add 0.30 to PAF value used in calculation for Pc07_PAF_AllowedInArea</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pc02d_ApplyOccupantSensingControlsFrom251To500SF" type="comp:ApplyOccupantSensingControlsFrom251To500SF" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">2C</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input: checked result = true, unchecked result = false.  If result == true add 0.20 to PAF value used in calculation for Pc07_PAF_AllowedInArea</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pc02e_ApplyInstitutionalTuningNonDaylit" type="comp:ApplyInstitutionalTuningNonDaylit" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">3A*</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input: checked result = true, unchecked result = false.  If result == true add 0.10 to PAF value used in calculation for Pc07_PAF_AllowedInArea</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pc02f_ApplyInstitutionalTuningDaylit" type="comp:ApplyInstitutionalTuningDaylit" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">3B*</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input: checked result = true, unchecked result = false.  If result == true add 0.05 to PAF value used in calculation for Pc07_PAF_AllowedInArea</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pc02g_ApplyDemandResponsiveControl" type="comp:ApplyDemandResponsiveControl" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">4*</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input: checked result = true, unchecked result = false.  If result == true add 0.05 to PAF value used in calculation for Pc07_PAF_AllowedInArea</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pc02h_ApplyClerestoryFenestrationPAF" type="comp:ApplyClerestoryFenestrationPAF" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">5*</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input: checked result = true, unchecked result = false.  If result == true add 0.05 to PAF value used in calculation for Pc07_PAF_AllowedInArea</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pc02i_ApplyHorizontalSlatsPAF" type="comp:ApplyHorizontalSlatsPAF" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">6*</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input: checked result = true, unchecked result = false.  If result == true add 0.05 to PAF value used in calculation for Pc07_PAF_AllowedInArea</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pc02j_ApplyLightShelvesPAF" type="comp:ApplyLightShelvesPAF" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">7*</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input: checked result = true, unchecked result = false.  If result == true add 0.10 to PAF value used in calculation for Pc07_PAF_AllowedInArea</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pc03_LuminaireName" type="lit:LuminaireName" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Luminaire Name or Item Tag</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input: selects from dropdown list filled with values from section F Fc02_LuminaireName</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pc04_LuminairePower" type="lit:LuminairePower" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Luminaire Design Watts</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Auto-filled result =  Fc04_LuminairePower in section F selected value in Pc03_LuminaireName</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pc05_LuminaireCountByType" type="lit:LuminaireCountByType" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Number of Luminaires</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pc06_LuminairePowerByType" type="lit:LuminairePowerByType" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Lighting Controlled (Watts)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value: result = PRODUCT (Pc04_ LuminairePower, Pc05_LuminaireCountByType)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pc07_PAF_AllowedInArea" type="comp:PAF_AllowedInArea" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Control Credit Power Adjustments (Watts)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value based on rules table 140.6-A for PAF credit(s) selected: First add all checked PAF values  and assign to local variable, PAF as follows:  If Pc02a_ApplyDaylightDimmingPlusOff == true  PAF += 0.10; If Pc02b_ApplyOccupantSensingControlsUpTo125SF == true, PAF += 0.40; If Pc02c_ApplyOccupantSensingControlsFrom126To250SF == true, PAF += 0.30; If Pc02d_ApplyOccupantSensingControlsFrom251To500SF == true,PAF += 0.20; If Pc02e_ApplyInstitutionalTuningNonDaylit == true, PAF += 0.10; If Pc02f_ApplyInstitutionalTuningDaylit == true, PAF += 0.05; If Pc02g_ApplyDemandResponsiveControl == true,PAF += 0.05; If Pc02h_ApplyClerestoryFenestrationPAF == true, PAF += 0.05;   If Pc02i_ApplyHorizontalSlatsPAF == true, PAF += 0.05;   If Pc02j_ApplyLightShelvesPAF == true, PAF += 0.10. Then get total Watts for area:  result = PRODUCT (PAF  x  Pc06_LuminairePowerByType)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:choice maxOccurs="1">
                              <xsd:element name="Pc08_DaylightRequirementsMetForPAF" type="comp:DaylightRequirementsMetForPAF" >
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">All spaces applying PAF 5, 6 or 7 include a daylight design meeting requirements in [d:t24ref/]140.3(d).  See Table S.</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">If all Pc02h_ApplyClerestoryFenestrationPAF And Pc02i_ApplyHorizontalSlatsPAF And Pc02j_ApplyLightShelvesPAF == false result = BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out; Else user input.  Checked result = true; unchecked result = false.</xsd:documentation>
                                 </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="Pc08_NotApplicableMessage" >
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">All spaces applying PAF 5, 6 or 7 includes a daylight design meeting requirements in [d:t24ref/]140.3(d)</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">If all Pc02h_ApplyClerestoryFenestrationPAF And Pc02i_ApplyHorizontalSlatsPAF And Pc02j_ApplyLightShelvesPAF == false result = BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out; Else user input.  Checked result = true; unchecked result = false.</xsd:documentation>
                                 </xsd:annotation>
                                 <xsd:simpleType>
                                    <xsd:restriction base="comp:NotApplicableMessage">
                                       <xsd:enumeration value="BlankGrayedOut"/>
                                    </xsd:restriction>
                                 </xsd:simpleType>
                              </xsd:element>
                           </xsd:choice>
                           <xsd:element name="Pc09_TotalPAF_AllowedAllAreas" type="comp:TotalPAF_AllowedAllAreas" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">[d:b]Total Power Adjustment (Watts) CONDITIONED SPACES:[/d:b]</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value: result = SUM All ( Pc07_PAF_AllowedInArea)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="PcErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage"  minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Pu_Unconditioned" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if at least one value in Iu06b_AdditionalAllowanceForPAF Or Ku09_AdditionalAllowanceForPAF == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Pu_ConditionedLabel" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">[d:b]Unconditioned Spaces[/d:b]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="PuTablePAF" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Pu01_SpaceDescription" type="com:SpaceDescription" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Area Description</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Rows auto-filled with values from Iu01_SpaceDescription where Iu06b_AdditionalAllowanceForPAF == true And values from Ku02_SpaceDescription  where Ku09_AdditionalAllowanceForPAF == true.  If user adds another luminaire, this field value is repeated</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pu02a_ApplyDaylightDimmingPlusOff" type="comp:ApplyDaylightDimmingPlusOff"  minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">1</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input; If checked add 0.10 to PAF value used in calculation for Pu07_PAF_AllowedInArea</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pu02b_ApplyOccupantSensingControlsUpTo125SF" type="comp:ApplyOccupantSensingControlsUpTo125SF"  minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">2A</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input; If checked add 0.40 to PAF value used in calculation for Pu07_PAF_AllowedInArea</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pu02c_ApplyOccupantSensingControlsFrom126To250SF" type="comp:ApplyOccupantSensingControlsFrom126To250SF"  minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">2B</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input; If checked add 0.30 to PAF value used in calculation for Pu07_PAF_AllowedInArea</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pu02d_ApplyOccupantSensingControlsFrom251To500SF" type="comp:ApplyOccupantSensingControlsFrom251To500SF"  minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">2C</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input; If checked add 0.20 to PAF value used in calculation for Pu07_PAF_AllowedInArea</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pu02e_ApplyInstitutionalTuningNonDaylit" type="comp:ApplyInstitutionalTuningNonDaylit"  minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">3A*</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input; If checked add 0.10 to PAF value used in calculation for Pu07_PAF_AllowedInArea</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pu02f_ApplyInstitutionalTuningDaylit" type="comp:ApplyInstitutionalTuningDaylit"  minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">3B*</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input; If checked add 0.05 to PAF value used in calculation for Pu07_PAF_AllowedInArea</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pu02g_ApplyDemandResponsiveControl" type="comp:ApplyDemandResponsiveControl"  minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">4*</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input; If checked add 0.05 to PAF value used in calculation for Pu07_PAF_AllowedInArea</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pu02h_ApplyClerestoryFenestrationPAF" type="comp:ApplyClerestoryFenestrationPAF" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">5*</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input: checked result = true, unchecked result = false.  If result == true add 0.05 to PAF value used in calculation for Pu07_PAF_AllowedInArea</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pu02i_ApplyHorizontalSlatsPAF" type="comp:ApplyHorizontalSlatsPAF" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">6*</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input: checked result = true, unchecked result = false.  If result == true add 0.05 to PAF value used in calculation for Pu07_PAF_AllowedInArea</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pu02j_ApplyLightShelvesPAF" type="comp:ApplyLightShelvesPAF" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">7*</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input: checked result = true, unchecked result = false.  If result == true add 0.10 to PAF value used in calculation for Pu07_PAF_AllowedInArea</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pu03_LuminaireName" type="lit:LuminaireName" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Luminaire Name or Item Tag</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input: selects from dropdown list filled with values from section F Fu02_LuminaireName</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pu04_LuminairePower" type="lit:LuminairePower" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Luminaire Design Watts</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Auto-filled with Fu04_LuminairePower in section F for selected value in Pu03_LuminaireName</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pu05_LuminaireCountByType" type="lit:LuminaireCountByType" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Number of Luminaires</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pu06_LuminairePowerByType" type="lit:LuminairePowerByType" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Lighting Controlled (Watts)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value: result = PRODUCT (Pu04_ LuminairePower,  Pu05_LuminaireCountByType)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Pu07_PAF_AllowedInArea" type="comp:PAF_AllowedInArea" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Control Credit Power Adjustments (Watts)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value based on rules table 140.6-A for PAF credit(s) selected: First add all checked PAF values  and assign to local variable, PAF as follows:  If Pu02a_ApplyDaylightDimmingPlusOff == true  PAF += 0.10; If Pu02b_ApplyOccupantSensingControlsUpTo125SF == true, PAF += 0.40; If Pu02c_ApplyOccupantSensingControlsFrom126To250SF == true, PAF += 0.30; If Pu02d_ApplyOccupantSensingControlsFrom251To500SF == true,PAF += 0.20; If Pu02e_ApplyInstitutionalTuningNonDaylit == true, PAF += 0.10; If Pu02f_ApplyInstitutionalTuningDaylit == true, PAF += 0.05; If Pu02g_ApplyDemandResponsiveControl == true,PAF += 0.05; If Pu02h_ApplyClerestoryFenestrationPAF == true, PAF += 0.05;   If Pu02i_ApplyHorizontalSlatsPAF == true, PAF += 0.05;   If Pu02j_ApplyLightShelvesPAF == true, PAF += 0.10. Then get total Watts for area:  result = PRODUCT (PAF  x  Pu06_LuminairePowerByType)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:choice maxOccurs="1">
                              <xsd:element name="Pu08_DaylightRequirementsMetForPAF" type="comp:DaylightRequirementsMetForPAF" >
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">All spaces applying PAF 5, 6 or 7 include a daylight design meeting requirements in [d:t24ref/]140.3(d).  See Table S.</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">If all Pu02h_ApplyClerestoryFenestrationPAF And Pu02i_ApplyHorizontalSlatsPAF And Pu02j_ApplyLightShelvesPAF == false result = BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out; Else user input.  Checked result = true; unchecked result = false.</xsd:documentation>
                                 </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="Pu08_NotApplicableMessage" >
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">All spaces applying PAF 5, 6 or 7 includes a daylight design meeting requirements in [d:t24ref/]140.3(d)</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">If all Pu02h_ApplyClerestoryFenestrationPAF And Pu02i_ApplyHorizontalSlatsPAF And Pu02j_ApplyLightShelvesPAF == false result = BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out; Else user input.  Checked result = true; unchecked result = false.</xsd:documentation>
                                 </xsd:annotation>
                                 <xsd:simpleType>
                                    <xsd:restriction base="comp:NotApplicableMessage">
                                       <xsd:enumeration value="BlankGrayedOut"/>
                                    </xsd:restriction>
                                 </xsd:simpleType>
                              </xsd:element>
                           </xsd:choice>
                           <xsd:element name="Pu09_TotalPAF_AllowedAllAreas" type="comp:TotalPAF_AllowedAllAreas" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">[d:b]Total Power Adjustment (Watts) UNCONDITIONED SPACES:[/d:b]</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value: result = SUM All ( Pu07_PAF_AllowedInArea)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="PuErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage"  minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="PEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]1[/d:sup] FOOTNOTES:  PAFs outlined in Table 140.6-A include 1) Daylight dimming plus OFF; 2A) Occupant sensors in offices [dlte/] 125 ft[d:sup]2[/d:sup];  2B) Occupant sensors in offices 126 - 250 ft[d:sup]2[/d:sup]; 2C) Occupant sensors in offices 251 - 500 ft[d:sup]2[/d:sup]; 3A) Institutional tuning, non-daylit areas and 3B) Institutional tuning, daylit areas; 4) Demand response; 5) Clerestory fenestration; 6) Horizontal slats; 7) Light shelves. [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="PEndNote2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]2[/d:sup] Luminaires that qualify for PAF 5, 6 or 7 can be used in conjunction with PAF 1. [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_Q" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]Q. RATED POWER REDUCTION COMPLIANCE FOR ALTERATIONS[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required if in section B there is at least one value in  Baltered02_LightingPrescriptiveMethodNR Or Baltered04_LightingPrescriptiveMethodNR  == RatedPowerReduction</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="Q_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]Indoor lighting alterations complying prescriptively with [d:t24ref/]141.0(b)2I(iii) are documented in this table. Any control option having a * will include a note in the Notes section of this table detailing how compliance is achieved, otherwise the compliance status in Table C will say "DOES NOT COMPLY". [/d:i].</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Q_ColumnHeading03To14" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]Fixture Schedule [/d:b] (Includes all luminaires being altered in the project).</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Q_ColumnSubheading03To08" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Pre-alteration Luminaire Information</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Q_ColumnSubheading09To14" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Post-alteration Luminaire Information</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Q_ColumnHeading15To19" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]Mandatory Controls[/d:b]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Q01_RatedPowerReductionScopeApplies" type="comp:RatedPowerReductionScopeApplies" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Alteration scope includes a one-for-one luminaire alteration within a building or tenant space of 5,000 ft[d:sup]2[/d:sup] or less per [d:t24ref/]141.0(b)2I(iii).</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Boolean If result == true, Q03 - 18 fields are displayed and enabled</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Q02_RatedPowerReductionException6AppliesToAll" type="comp:RatedPowerReductionException6AppliesToAll" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Exception 6 applies to some spaces</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">At least one floor or complete tenant space includes a one-for-one luminaire alteration of 50 or less luminaires, per annum.  These spaces do not need to comply with Part 6 requirements and therefore do not need to be included in tables below per Exception 6 to [d:t24ref/]141.0(b)2I.</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input: If checked result = false; Else result = true. If result == false, Q02a data is required</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Q02a_RatedPowerReductionException6Spaces" type="comp:RatedPowerReductionException6Spaces"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Applicable Spaces</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If Q02_RatedPowerReductionException6AppliesToAll == false, user input text listing all applicable spaces; Else no data is required in this field.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Q02b_RatedPowerReductionException6AppliesToAll" type="comp:RatedPowerReductionException6AppliesToAll" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Exception 6 applies to all spaces</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Exception 6 applies to all spaces within the permit application</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input: If checked result = true; Else result = false.  If result == true, all fields Q03 - Q19 are grayed out</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="ChoiceQTables" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:choice >
                           <xsd:element name="QTablesNA_NotApplicableMessage" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">If Q02b_RatedPowerReductionException6AppliesToAll == true result = BlankGrayedOut stored in NotApplicableMessage and data is not required fields Q03-Q19 and they are blank and grayed out; Else  Q03-Q19 fields are required.</xsd:documentation>
                              </xsd:annotation>
                              <xsd:simpleType>
                                 <xsd:restriction base="comp:NotApplicableMessage">
                                    <xsd:enumeration value="BlankGrayedOut"/>
                                 </xsd:restriction>
                              </xsd:simpleType>
                           </xsd:element>
                           <xsd:element name="QTablesRequired" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">If Q02b_RatedPowerReductionException6AppliesToAll == true result = BlankGrayedOut stored in NotApplicableMessage and data is not required fields Q03-Q19 and they are blank and grayed out; Else  Q03-Q19 fields are required.</xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="TableAllLuminairesAltered" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="Q03_LuminaireName" type="lit:LuminaireName" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Name or Item Tag</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Q04_LuminaireDescription" type="lit:LuminaireDescription" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Complete Luminaire Description</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Q05_LuminairePower" type="lit:LuminairePower" >
                                                         <xsd:annotation>
