<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
   targetNamespace="http://www.lmonte.com/besm/NRCCLTI01E"
   xmlns="http://www.lmonte.com/besm/NRCCLTI01E"
   xmlns:d="http://www.lmonte.com/besm/d"
   xmlns:bld="http://www.lmonte.com/besm/bld"
   xmlns:env="http://www.lmonte.com/besm/env"
   xmlns:lit="http://www.lmonte.com/besm/lit"
   xmlns:hvac="http://www.lmonte.com/besm/hvac"
   xmlns:comp="http://www.lmonte.com/besm/comp"
   xmlns:com="http://www.lmonte.com/besm/com"
   xmlns:dtyp="http://www.lmonte.com/besm/dtyp"
   xmlns:xsd="http://www.w3.org/2001/XMLSchema"
   xmlns:altova="http://www.altova.com/xml-schema-extensions"
   elementFormDefault="qualified" attributeFormDefault="unqualified" version="2019.0.006">
   <xsd:import namespace="http://www.lmonte.com/besm/com" schemaLocation="../base/NrPresCommon.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/bld" schemaLocation="../base/NrPresBuilding.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/env" schemaLocation="../base/NrPresEnvelope.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/lit" schemaLocation="../base/NrPresLighting.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/hvac" schemaLocation="../base/NrPresHvac.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/comp" schemaLocation="../base/NrPresCompliance.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/dtyp" schemaLocation="../base/DataTypes.xsd"/>
   <xsd:element name="ComplianceDocumentPackage">
      <xsd:complexType>
         <xsd:sequence>
            <xsd:element name="DocID" minOccurs="0">
               <xsd:complexType>
                  <xsd:attribute name="doc" type="comp:ComplianceDocumentTagNR" fixed="NRCCLTIE"/>
                  <xsd:attribute name="docType" type="comp:ComplianceDocumentTypeNR" fixed="CERTIFICATE OF COMPLIANCE NR"/>
                  <xsd:attribute name="docTitle" type="comp:ComplianceDocumentTitleNR" fixed="Indoor Lighting"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Payload" type="comp:Payload"/>
            <xsd:element name="DocumentData">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="Header" type="comp:HeaderNRCC" />
                     <xsd:element ref="nRCCLTI01E" />
                     <xsd:element name="DocAuthor" type="comp:DocumentAuthor" />
                     <xsd:element name="RespPerson" type="comp:ResponsiblePersonNRCC" />
                     <xsd:element name="Footer" type="comp:FooterNRCC" />
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Report" type="xsd:base64Binary" minOccurs="0"/>
         </xsd:sequence>
         <xsd:attribute name="revision" use="required" type="xsd:string" fixed="rev 20190401"/>
         <xsd:attribute name="doc" use="required" type="comp:ComplianceDocumentTagNR" fixed="NRCCLTIE"/>
      </xsd:complexType>
   </xsd:element>
   <xsd:complexType name="NRCCLTI01E">
      <xsd:annotation>
         <xsd:documentation>Indoor Lighting</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Section_A" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]A. GENERAL INFORMATION[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="A_BeforeNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This document is used to demonstrate compliance with requirements in [d:t24ref/]110.9, [d:t24ref/]110.12(c), [d:t24ref/]130.0, [d:t24ref/]130.1, [d:t24ref/]140.6 and [d:t24ref/]141.0(b)2 for indoor lighting scopes using the prescriptive path.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A01_ProjectCity" type="comp:ProjectCity" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Project Location (city)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A02_ClimateZone" type="com:ClimateZone" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Climate Zone</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03_WholeBuildingOccupancy"  minOccurs="1" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Occupancy Types Within Project (select all that apply):</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                        <xsd:appinfo source="displayterm">
                           <dtyp:displayterm value="RelocatableSchoolBuilding">Relocatable</dtyp:displayterm>
                           <dtyp:displayterm value="OfficeBuilding">Office</dtyp:displayterm>
                           <dtyp:displayterm value="RetailBuilding">Retail</dtyp:displayterm>
                           <dtyp:displayterm value="SchoolBuilding">School</dtyp:displayterm>
                           <dtyp:displayterm value="AllOthersBuildings">Other (Write in)</dtyp:displayterm>
                        </xsd:appinfo>
                        
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="com:WholeBuildingOccupancy">
                           <xsd:enumeration value="AllOthersBuildings"/>
                           <xsd:enumeration value="HighRiseResidential"/>
                           <xsd:enumeration value="HospitalBuilding"/>
                           <xsd:enumeration value="HotelMotel"/>
                           <xsd:enumeration value="OfficeBuilding"/>
                           <xsd:enumeration value="ParkingGarageBuilding"/>
                           <xsd:enumeration value="RelocatablePublicSchoolBuilding"/>
                           <xsd:enumeration value="RetailBuilding"/>
                           <xsd:enumeration value="SchoolBuilding"/>
                           <xsd:enumeration value="SupportArea"/>
                           <xsd:enumeration value="Warehouse"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="A03a_TypeOtherDescription" type="com:TypeOtherDescription"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Other (write in)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This field is required If A03_WholeBuildingOccupancy == AllOthersBuildings.  User input should describe the type of  occupancy.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A04_TotalConditionedFloorArea" type="com:TotalConditionedFloorArea"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Total Conditioned Floor Area (ft[d:sup]2[/d:sup])</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input  which can include 0;  If user doesn't enter anything the cell is blank</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A05_TotalUnconditionedFloorArea" type="com:TotalUnconditionedFloorArea"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Total Unconditioned Floor Area (ft[d:sup]2[/d:sup])</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input  which can include 0;  If user doesn't enter anything the cell is blank</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A06_AboveGradeStoryCount" type="env:AboveGradeStoryCount" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"># of Stories (Habitable Above Grade)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="AErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_B" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]B. PROJECT SCOPE[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="B_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table includes any lighting systems that are within the scope of the permit application and are demonstrating compliance using the prescriptive path outlined in [d:t24ref/]140.6 or [d:t24ref/]141.0(b)2 for alterations. [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B_ColumnHeading01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]Scope of Work[/d:b]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B_ColumnSubHeading01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">My Project Consists of (check all that apply):</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B_ColumnHeading02_03" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]Conditioned Spaces[/d:b]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B_ColumnFieldText02Cond" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Calculation Method</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B_ColumnFieldText03Cond" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Area (ft[d:sup]2[/d:sup]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B_ColumnHeading04_05" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]Unconditioned Spaces[/d:b]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B_ColumnFieldText02Uncond" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Calculation Method</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B_ColumnFieldText03Uncond" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Area (ft[d:sup]2[/d:sup]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Bnew01_LightingProjectScopeNR" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">New Lighting System</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If user checks New Lighting System result = NewLightingSystem  Else result = NotApplicableLightingScope</xsd:documentation>
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="comp:LightingProjectScopeNR">
                           <xsd:enumeration value="NewLightingSystem"/>
                           <xsd:enumeration value="NotApplicableLightingScope"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="NewLighting" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if LightingProjectScopeNR ==  NewLightingSystem</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Bnew02_LightingPrescriptiveMethodNR" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Calculation Method</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">If new lighting system does not include conditioned space, result = NotApplicableLightingMethod, Else user selects calculation method,</xsd:documentation>
                              </xsd:annotation>
                              <xsd:simpleType>
                                 <xsd:restriction base="comp:LightingPrescriptiveMethodNR">
                                    <xsd:enumeration value="AreaCategory"/>
                                    <xsd:enumeration value="AreaCategoryAndTailored"/>
                                    <xsd:enumeration value="CompleteBuilding"/>
                                    <xsd:enumeration value="NotApplicableLightingMethod"/>
                                    <xsd:enumeration value="Tailored"/>
                                 </xsd:restriction>
                              </xsd:simpleType>
                           </xsd:element>
                           <xsd:element name="Bnew03_NewLightingConditionedFloorArea" type="lit:NewLightingConditionedFloorArea"  minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Area (ft[d:sup]2[/d:sup])</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">If  Bnew02_LightingPrescriptiveMethodNR == NotApplicableLightingMethod  field is blank and not required; Else user inputs value.</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Bnew04_LightingPrescriptiveMethodNR" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Calculation Method</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">If new lighting system does not include unconditioned space, result = NotApplicableLightingMethod, Else If Bnew02_LightingPrescriptiveMethodNR  == CompleteBuilding, only value in list to select is CompleteBuilding, Else 
user selects calculation method from dropdown list,</xsd:documentation>
                              </xsd:annotation>
                              <xsd:simpleType>
                                 <xsd:restriction base="comp:LightingPrescriptiveMethodNR">
                                    <xsd:enumeration value="AreaCategory"/>
                                    <xsd:enumeration value="AreaCategoryAndTailored"/>
                                    <xsd:enumeration value="CompleteBuilding"/>
                                    <xsd:enumeration value="NotApplicableLightingMethod"/>
                                    <xsd:enumeration value="Tailored"/>
                                 </xsd:restriction>
                              </xsd:simpleType>
                           </xsd:element>
                           <xsd:element name="Bnew05_NewLightingUnconditionedFloorArea" type="lit:NewLightingUnconditionedFloorArea"  minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Area (ft[d:sup]2[/d:sup])</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">If  Bnew04_LightingPrescriptiveMethodNR == NotApplicableLightingMethod field is blank and not required; Else user inputs value.</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Bpark01_LightingProjectScopeNR" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">New Lighting System - Parking Garage</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If user checks New Lighting System - Parking Garage result = NewParkingGarageLighting  Else result = NotApplicableLightingScope</xsd:documentation>
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="comp:LightingProjectScopeNR">
                           <xsd:enumeration value="NewParkingGarageLighting"/>
                           <xsd:enumeration value="NotApplicableLightingScope"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="NewLightingGarage" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if LightingProjectScopeNR ==  NewParkingGarageLighting</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Bpark02_LightingPrescriptiveMethodNR" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Calculation Method</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">If new parking garage lighting system does not include conditioned space, result = NotApplicableLightingMethod, Else result = CompleteBuilding</xsd:documentation>
                              </xsd:annotation>
                              <xsd:simpleType>
                                 <xsd:restriction base="comp:LightingPrescriptiveMethodNR">
                                    <xsd:enumeration value="CompleteBuilding"/>
                                    <xsd:enumeration value="NotApplicableLightingMethod"/>
                                 </xsd:restriction>
                              </xsd:simpleType>
                           </xsd:element>
                           <xsd:element name="Bpark03_NewLightingConditionedFloorArea" type="lit:NewLightingConditionedFloorArea"  minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Area (ft[d:sup]2[/d:sup]</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">If  Bpark02_LightingPrescriptiveMethodNR == NotApplicableLightingMethod field is blank and not required; Else user inputs value.</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Bpark04_LightingPrescriptiveMethodNR" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Calculation Method</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">If new parking garage lighting system does not include unconditioned space, result = NotApplicableLightingMethod, Else result = CompleteBuilding</xsd:documentation>
                              </xsd:annotation>
                              <xsd:simpleType>
                                 <xsd:restriction base="comp:LightingPrescriptiveMethodNR">
                                    <xsd:enumeration value="CompleteBuilding"/>
                                    <xsd:enumeration value="NotApplicableLightingMethod"/>
                                 </xsd:restriction>
                              </xsd:simpleType>
                           </xsd:element>
                           <xsd:element name="Bpark05_NewLightingUnconditionedFloorArea" type="lit:NewLightingUnconditionedFloorArea"  minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Area (ft[d:sup]2[/d:sup]</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">If  Bpark04_LightingPrescriptiveMethodNR == NotApplicableLightingMethod  field is blank and not required; Else user inputs value.</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Baltered01_LightingProjectScopeNR" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Altered Lighting System</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If user checks Altered Lighting System result = AlteredLightingSystem  Else result = NotApplicableLightingScope</xsd:documentation>
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="comp:LightingProjectScopeNR">
                           <xsd:enumeration value="AlteredLightingSystem"/>
                           <xsd:enumeration value="NotApplicableLightingScope"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="TableAlteredLighting" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This table is required if LightingProjectScopeNR == AlteredLightingSystem</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Baltered02_LightingPrescriptiveMethodNR" type="comp:LightingPrescriptiveMethodNR" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Calculation Method</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If altered lighting system does not include conditioned space, result = NotApplicableLightingMethod, Else user selects calculation method,</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Baltered03_AlteredLightingConditionedFloorArea" type="lit:AlteredLightingConditionedFloorArea"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area (ft[d:sup]2[/d:sup]</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If Baltered02_LightingPrescriptiveMethodNR  == NotApplicableLightingMethod field is blank and not required; Else user inputs value.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Baltered04_LightingPrescriptiveMethodNR" type="comp:LightingPrescriptiveMethodNR" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Calculation Method</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If altered lighting system does not include unconditioned space, result = NotApplicableLightingMethod, Else If Baltered02_LightingPrescriptiveMethodNR == CompleteBuilding, only value in list to select is CompleteBuilding, Else 
user selects calculation method from dropdown list,</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Baltered05_AlteredLightingUnconditionedFloorArea" type="lit:AlteredLightingUnconditionedFloorArea"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area (ft[d:sup]2[/d:sup]</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If  Baltered04_LightingPrescriptiveMethodNR == NotApplicableLightingMethod field is blank and not required; Else user inputs value.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="BTotalsRowLabel" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Total Area of Work (ft[d:sup]2[/d:sup])[/d:b]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="BTotals03_LightingProjectTotalConditionedFloorArea" type="comp:LightingProjectTotalConditionedFloorArea"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated value: If Bnew03_NewLightingConditionedFloorArea IsNull And Bpark03_NewLightingConditionedFloorArea IsNull And  All Baltered03_AlteredLightingConditionedFloorArea IsNull result = field blank and not required;  Else for Bnew03, Bpark03 and all Baltered03  values NotNull result =  SUM  ( Bnew03_NewLightingConditionedFloorArea, Bpark03_NewLightingConditionedFloorArea, Baltered03_AlteredLightingConditionedFloorArea)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="BTotals05_LightingProjectTotalUnconditionedFloorArea" type="comp:LightingProjectTotalUnconditionedFloorArea"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated value: If Bnew05_NewLightingUnconditionedFloorArea IsNull And Bpark05_NewLightingUnconditionedFloorArea IsNull And  Baltered05_AlteredLightingUnconditionedFloorArea IsNull result = field blank and not required;  Else for Bnew05, Bpark05 and all Baltered05  values NotNull result = SUM ( Bnew05_NewLightingUnconditionedFloorArea, Bpark05_NewLightingUnconditionedFloorArea, Baltered05_AlteredLightingUnconditionedFloorArea)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="BalteredMessage03_05" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b][d:i]ALERT! The Rated Power Reduction Method for alterations may only be used within a building or tenant space of 5,000 ft[d:sup]2[/d:sup] or less per [d:t24ref/]141.0(b)2liii.[/d:i][/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This text is displayed to the user If Baltered02_LightingPrescriptiveMethodNR Or Baltered04_LightingPrescriptiveMethodNR == RatedPowerReduction And Baltered03_AlteredLightingConditionedFloorArea Or Baltered05_AlteredLightingUnconditionedFloorArea Or SUM (Baltered03_AlteredLightingConditionedFloorArea, Baltered05_AlteredLightingUnconditionedFloorArea) GT 5000</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_C" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]C. COMPLIANCE RESULTS[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="C_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i] If any cell on this table says  "DOES NOT COMPLY" or "COMPLIES with Exceptional Conditions" refer to Table D. for guidance.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C_BeginNote2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]Lighting in conditioned and unconditioned spaces must not be combined for compliance per [d:t24ref/]140.6(b)1[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C_ColumnHeading01To05" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]Allowed Lighting Power per [d:t24ref/]140.6(b) (Watts)[/d:b]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C_ColumnHeading06To08" minOccurs="0">
                     <xsd:annotation>
