                           <xsd:documentation source="CalculationsAndRules">If C06Row02_LightingPowerInstalledTotalUnconditioned NotNull Then If C08Row02_ControlsTotalLightingPowerAllowedUnconditioned NotNull result = C06Row02_LightingPowerInstalledTotalUnconditioned - C08Row02_ControlsTotalLightingPowerAllowedUnconditioned); Else result = C06Row02_LightingPowerInstalledTotalUnconditioned;  Else result = BlankGrayedOut stored in NotApplicableMessage and field is grayed out and blank</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C08Row02_NotApplicableMessage" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"></xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If C06Row02_LightingPowerInstalledTotalUnconditioned NotNull Then If C08Row02_ControlsTotalLightingPowerAllowedUnconditioned NotNull result = C06Row02_LightingPowerInstalledTotalUnconditioned - C08Row02_ControlsTotalLightingPowerAllowedUnconditioned); Else result = C06Row02_LightingPowerInstalledTotalUnconditioned;  Else result = BlankGrayedOut stored in NotApplicableMessage and field is grayed out and blank</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="C09_ColumnLabel" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]05 must be [d:gte/] 08 [d:line1/] [d:t24ref/]140.6[/d:b]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C09Row01_DoesLightingPowerConditionedComply" type="comp:DoesLightingPowerConditionedComply" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"></xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If C08Row1_LightingPowerInstalledTotalConditionedAdjusted IsNull  result = BlankGrayedOut stored in NotApplicableMessage and field is grayed out and blank; Else If C05Row01_LightingPowerAllowanceTotalConditioned GTE C08Row01_LightingPowerInstalledTotalConditionedAdjusted  result = true  display term COMPLIES, Else result = false display term DOES NOT COMPLY.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C09Row01_NotApplicableMessage" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"></xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If C08Row1_LightingPowerInstalledTotalConditionedAdjusted IsNull  result = BlankGrayedOut stored in NotApplicableMessage and field is grayed out and blank; Else If C05Row01_LightingPowerAllowanceTotalConditioned GTE C08Row01_LightingPowerInstalledTotalConditionedAdjusted  result = true  display term COMPLIES, Else result = false display term DOES NOT COMPLY.</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C09Row02_DoesLightingPowerUnconditionedComply" type="comp:DoesLightingPowerUnconditionedComply" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"></xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If C08Row2_LightingPowerInstalledTotalUnconditionedAdjusted IsNull  result = BlankGrayedOut stored in NotApplicableMessage and field is grayed out and blank; Else  If C05Row02_LightingPowerAllowanceTotalUnconditioned GTE C08Row02_LightingPowerInstalledTotalUnconditionedAdjusted  result = true  display term COMPLIES, Else result = false display term DOES NOT COMPLY.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C09Row02_NotApplicableMessage" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"></xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If C08Row2_LightingPowerInstalledTotalUnconditionedAdjusted IsNull, result = BlankGrayedOut stored in NotApplicableMessage and field is grayed out and blank; Else  If C05Row02_LightingPowerAllowanceTotalUnconditioned GTE C08Row02_LightingPowerInstalledTotalUnconditionedAdjusted  result = true  display term COMPLIES, Else result = false display term DOES NOT COMPLY.</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C10_DoLightingControlsComply" type="comp:DoLightingControlsComply" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">[d:b]Controls Compliance (See Table H for Details)[/d:b]</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If all values for Baltered02_LightingPrescriptiveMethodNR == Or Baltered04_LightingPrescriptiveMethodNR == RatedPowerReduction result = BlankGrayedOut stored in NotApplicableMessage and field is grayed out and blank; Else If H_Exception02_ExceptionalConditionNote IsPresent And NotNull, result = CompliesExceptionalConditions display term COMPLIES with Exceptional Conditions, Else If H_Exception02_ExceptionalConditionNote IsPresent And IsNull, result = DoesNotComply display term DOES NOT COMPLY; Else If H01, H02 and H04-H10 are completed result = Complies display term COMPLIES; Else result =  DoesNotComply display term DOES NOT COMPLY;</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C10_NotApplicableMessage" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">[d:b]Controls Compliance (See Table H for Details)[/d:b]</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If all values for Baltered02_LightingPrescriptiveMethodNR == Or Baltered04_LightingPrescriptiveMethodNR == RatedPowerReduction result = BlankGrayedOut stored in NotApplicableMessage and field is grayed out and blank;</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C11_DoesRatedPowerReductionComply" type="comp:DoesRatedPowerReductionComply" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">[d:b]Rated Power Reduction Compliance (See Table Q for Details)[/d:b]</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If all values for Baltered02_LightingPrescriptiveMethodNR != RatedPowerReduction  result = NotApplicable stored in C11_NotApplicableMessage display term NOT APPLICABLE; Else If  Q01 RatedPowerReductionScopeApplies == true If Q02_RatedPowerReductionException6AppliesToAll == true  result = NotApplicable stored in C11_NotApplicableMessage display term NOT APPLICABLE; Else If Q01, And Q03-Q19 have been completed or are not required And QPercentReduction_LightingPowerReductionPercent GTE .40 result = true display term COMPLIES Else result = false display term DOES NOT COMPLY</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C11_NotApplicableMessage" type="comp:NotApplicableMessage" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">[d:b]Rated Power Reduction Compliance (See Table Q for Details)[/d:b]</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If all values for Baltered02_LightingPrescriptiveMethodNR != RatedPowerReduction  result = NotApplicable stored in C11_NotApplicableMessage display term NOT APPLICABLE; Else If  Q01 RatedPowerReductionScopeApplies == true If Q02_RatedPowerReductionException6AppliesToAll == true  result = NotApplicable stored in C12_NotApplicableMessage display term NOT APPLICABLE;</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:choice>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_D" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]D. EXCEPTIONAL CONDITIONS[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="D_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table is auto-filled with uneditable comments because of selections made or data entered in tables throughout the form.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableAllExceptionalConditions" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This table is required if there are any exceptional conditions in sections H or S that required user explanation.</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="D01_SectionTitle" type="comp:SectionTitle" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto-filled with the section title in the FieldText  + "Permit Applicant Notes:". For section H result =  "H. Indoor Lighting Controls (Not including PAFs) Permit Applicant Notes"  For section Q result = "Q. Rated Power Reduction Compliance by Space Permit Applicant Notes"</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="D02_SectionExceptionalConditionNotes" type="comp:SectionExceptionalConditionNotes"  maxOccurs="unbounded">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">This combines all execeptional condition notes for the section defined in D01_SectionTitle.  For section H each entry = H_Exception01_SpaceDescription + : + H_Exception02_SectionExceptionalConditionNote.  For section Q each entry = Q_Exception01_SpaceDescription + : + Q_Exception02_SectionExceptionalConditionNote.    If the user failed to enter an explanation, "null" appears after the space description</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="D03_ExceptionalConditionMessageNRCCLTIE" type="comp:ExceptionalConditionMessageNRCCLTIE"  minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Exceptional Messages</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Auto-filled with any system generated messages that explain exceptional conditions encountered in the the completed compliance document</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_E" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]E. ADDITIONAL REMARKS[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="E_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table includes remarks made by the permit applicant to the Authority Having Jurisdiction.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="E01_CommentsToPermitAuthority" type="comp:CommentsToPermitAuthority"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_F" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]F. INDOOR LIGHTING FIXTURE SCHEDULE[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section applies If Bnew01_LightingProjectScope Or Bpark01_LightingProjectScope != NotApplicableLightingScope Or at least one instance of Baltered02_LightingPrescriptiveMethodNR Or Baltered04_LightingPrescriptiveMethodNR != RatedPowerReduction</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="F_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table includes all permanent designed lighting and all portable lighting in offices.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="DesignWattsConditioned" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required If BTotals03_LightingProjectTotalConditionedFloorArea GT 0</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Fc_Label" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">[d:b]Designed Wattage: Conditioned Spaces[/d:b]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FcTableLuminairesAll" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Fc01_LuminaireName" type="lit:LuminaireName" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Name or Item Tag</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input.</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Fc02_LuminaireDescription" type="lit:LuminaireDescription" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Complete Luminaire Description</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input.</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Fc03_IsLuminaireTrackLighting" type="lit:IsLuminaireTrackLighting" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Modular (Track) Fixture</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true; Else result = false</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Fc04_IsLuminaireSmallApertureColorChanging" type="lit:IsLuminaireSmallApertureColorChanging" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Small Aperture &amp; Color Change [d:sup]1[/d:sup]</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true; Else result = false</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Fc05_LuminairePower" type="lit:LuminairePower" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Watts per luminaire[d:sup]2[/d:sup]</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">If Fc03_IsLuminaireTrackLighting == true, this is auto-filled result =  G04_TrackLightingWattsCalculated  for this luminaire, Else user inputs value</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Fc06_LuminaireSpecSource" type="comp:LuminaireSpecSource" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">How is Wattage determined</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">If Fc03_IsLuminaireTrackLighting == true, result = SeeOtherSection display term See Table G; Else user selects from list: CEC_Default display term NA8 Default, ManufacturerSpec display term Mfr. Spec., Other and JA8Lamp display term JA8 Lamp</xsd:documentation>
                                                   <xsd:appinfo source="displayterm">
                                                      <dtyp:displayterm value="CEC_Default">NA8 Default</dtyp:displayterm>
                                                      <dtyp:displayterm value="ManufacturerSpec">Mfr. Spec[d:sup]1[/d:sup]</dtyp:displayterm>
                                                      <dtyp:displayterm value="SeeOtherSection">See Table G</dtyp:displayterm>
                                                   </xsd:appinfo>
                                                   
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Fc07_LuminaireCountByType" type="lit:LuminaireCountByType" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Total Number of Luminaires</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Fc08_LightingPowerExcluded" type="comp:LightingPowerExcluded" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Exempt per [d:t24ref/]140.6(a)3</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input; If checked result = true; Else result = false</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Fc09_LuminairePowerByType" type="lit:LuminairePowerByType" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Design Watts</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">Calculated value: If Fc08_LightingPowerExcluded == true, result = NotApplicable stored in  NotApplicableMessage with display term Exempt and the field is grayed out in that row, Else If Fc04_IsLuminaireSmallApertureColorChanging == true result = PRODUCT( PRODUCT (Fc05_LuminairePower,  Fc07LuminaireCountByType), 0.75); Else  result = PRODUCT ( Fc05_LuminairePower, Fc07LuminaireCountByType).</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fc09_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Design Watts</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">Calculated value: If Fc08_LightingPowerExcluded == true, result = NotApplicable stored in  NotApplicableMessage with display term Exempt and the field is grayed out in that row, Else If Fc04_IsLuminaireSmallApertureColorChanging == true result = PRODUCT( PRODUCT (Fc05_LuminairePower,  Fc07LuminaireCountByType), 0.75); Else  result = PRODUCT ( Fc05_LuminairePower, Fc07LuminaireCountByType).</xsd:documentation>
                                                      <xsd:appinfo source="displayterm">
                                                         <dtyp:displayterm value="NotApplicable">Exempt</dtyp:displayterm>
                                                      </xsd:appinfo>
                                                      
                                                   </xsd:annotation>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="Fc10_VerifedByInspector" type="comp:VerifedByInspector"  minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Field Inspector</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="Fc09Totals_LightingPowerInstalledTotalConditioned" type="comp:LightingPowerInstalledTotalConditioned" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">[d:b]Total Designed Watts: CONDITIONED SPACES[/d:b]</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  result = SUM All (Fc09_LuminairePowerByType)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FcErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage"  minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="DesignWattsUnconditioned" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if BTotals05_LightingProjectTotalUnconditionedFloorArea GT 0</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Fu_Label" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">[d:b]Designed Wattage: Unconditioned Spaces[/d:b]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FuTableLuminairesAll" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Fu01_LuminaireName" type="lit:LuminaireName" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Name or Item Tag</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input.</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Fu02_LuminaireDescription" type="lit:LuminaireDescription" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Complete Luminaire Description</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input.</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Fu03_IsLuminaireTrackLighting" type="lit:IsLuminaireTrackLighting" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Modular (Track) Fixture</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true; Else result = false</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Fu04_IsLuminaireSmallApertureColorChanging" type="lit:IsLuminaireSmallApertureColorChanging" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Small Aperture &amp; Color Change [d:sup]1[/d:sup]</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true; Else result = false</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Fu05_LuminairePower" type="lit:LuminairePower" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Watts per luminaire[d:sup]2[/d:sup]</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">If Fu03_IsLuminaireTrackLighting == true, this is auto-filled result = G05_TrackLightingWattsCalculated  for this luminaire, Else user inputs value</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Fu06_LuminaireSpecSource" type="comp:LuminaireSpecSource" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">How is Wattage determined</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">If Fu03_IsLuminaireTrackLighting == true, result = SeeOtherSection display term See Table G; Else user selects from list: CEC_Default display term NA8 Default, ManufacturerSpec display term Mfr. Spec., Other and JA8Lamp display term JA8 Lamp</xsd:documentation>
                                                   <xsd:appinfo source="displayterm">
                                                      <dtyp:displayterm value="CEC_Default">NA8 Default</dtyp:displayterm>
                                                      <dtyp:displayterm value="ManufacturerSpec">Mfr. Spec[d:sup]1[/d:sup]</dtyp:displayterm>
                                                      <dtyp:displayterm value="SeeOtherSection">See Table G</dtyp:displayterm>
                                                   </xsd:appinfo>
                                                   
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Fu07_LuminaireCountByType" type="lit:LuminaireCountByType" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Total Number of Luminaires</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Fu08_LightingPowerExcluded" type="comp:LightingPowerExcluded" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Excluded per [d:t24ref/]140.6(a)3</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input; If checked result = true; Else result = false</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Fu09_LuminairePowerByType" type="lit:LuminairePowerByType" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Design Watts</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">Calculated value: If Fu08_LightingPowerExcluded == true, result = NotApplicable stored in  NotApplicableMessage with display term Exempt and the field is grayed out in that row, Else If Fu04_IsLuminaireSmallApertureColorChanging == true result = PRODUCT( PRODUCT (Fu05_LuminairePower,  Fu07LuminaireCountByType), 0.75); Else  result = PRODUCT ( Fu05_LuminairePower, Fu07LuminaireCountByType).</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fu09_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Design Watts</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">Calculated value: If Fu08_LightingPowerExcluded == true, result = NotApplicable stored in  NotApplicableMessage with display term Exempt and the field is grayed out in that row, Else If Fu04_IsLuminaireSmallApertureColorChanging == true result = PRODUCT( PRODUCT (Fu05_LuminairePower,  Fu07LuminaireCountByType), 0.75); Else  result = PRODUCT ( Fu05_LuminairePower, Fu07LuminaireCountByType).</xsd:documentation>
                                                      <xsd:appinfo source="displayterm">
                                                         <dtyp:displayterm value="NotApplicable">Exempt</dtyp:displayterm>
                                                      </xsd:appinfo>
                                                      
                                                   </xsd:annotation>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="Fu10_VerifedByInspector" type="comp:VerifedByInspector"  minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Field Inspector</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="Fu09Totals_LightingPowerInstalledTotalUnconditioned" type="comp:LightingPowerInstalledTotalUnconditioned" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">[d:b]Total Designed Watts: UNCONDITIONED SPACES[/d:b]</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  result = SUM All (Fu09_LuminairePowerByType)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FuErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage"  minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="FEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]1[/d:sup]FOOTNOTE: Design Watts for small aperture and color changing luminaires which qualify per [d:t24ref/]140.6(a)4B is adjusted to be 75% of their rated wattage.  Table F automatically makes this adjustment, the permit applicant should enter full rated wattage in column 05. [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="FEndNote2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]2[/d:sup]Authority Having Jurisdiction may ask for Luminaire cut sheets to confirm wattage used for compliance per [d:t24ref/]130.0(c)  Wattage used must be the maximum rated for the luminaire, not the lamp.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_G" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]G. MODULAR LIGHTING SYSTEMS[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is applicable if in section F any row has Fc03_IsLuminaireTrackLighting or Fu03_IsLuminaireTrackLighting == true</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="G_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table calculates wattage for modular lighting systems/ track lighting fixtures indicated on Table F and transfers wattage to Table F.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableTrackLighting" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="G01_TrackLightingName" type="lit:TrackLightingName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name or Item Tag</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto-filled result = Fc01_LuminaireName where Fc03_IsLuminaireTrackLighting == true or Fu01_LuminaireName where Fu03_IsLuminaireTrackLighting == true</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G02_TrackLightingDescription" type="lit:TrackLightingDescription" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Complete Track Description</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto-filled result = Fc02_LuminaireDescription or Fu02_LuminaireDescription for G01_TrackLightingName</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G03ColumnHeading" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                          <xsd:documentation source="AdditionalRequirements">Calculation Method per [d:t24ref/]130.0(c)6</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G03i_IsTrackLightingMethodInstalledLuminaires" type="comp:IsTrackLightingMethodInstalledLuminaires" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">i Installed Luminaires vs Default 30 W/ft</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input; If checked result is true, else false</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G03ii_IsTrackLightingMethodCurrentLimiter" type="comp:IsTrackLightingMethodCurrentLimiter" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">ii Current Limiter</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input; If checked result is true, else false</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G03iii_IsTrackLightingMethodOvercurrentPanels" type="comp:IsTrackLightingMethodOvercurrentPanels" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">iii Overcurrent Protection Panel</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input; If checked result is true, else false</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G03iv_IsTrackLightingMethodPowerSource" type="comp:IsTrackLightingMethodPowerSource" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">iv  Power supplied by driver, power supply or transformer[d:sup]1[/d:sup]</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input; If checked result is true, else false</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TrackLightingCalculation" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:choice >
                                             <xsd:element name="G03iCalculationMethod" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"></xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:choice >
                                                      <xsd:element name="G03iTrackHeadPower" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText"></xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:complexType>
                                                            <xsd:sequence>
                                                               <xsd:element name="G03i01_LuminaireCountByType" type="lit:LuminaireCountByType" >
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Number of luminaires in system</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User input.</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="G03i01HeadsAfterLabel" minOccurs="0">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">X</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="G03i02_LuminairePower" type="lit:LuminairePower" >
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Rated Watts per luminaire</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User input.</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="G03i02HeadsAfterLabel" minOccurs="0">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">=</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="G03i03_LuminairePowerByType" type="lit:LuminairePowerByType" >
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Total Watts</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">Calculated Field:  result = PRODUCT ( G03i01_LuminaireCountByType, G03i01_LuminairePower)</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                            </xsd:sequence>
                                                         </xsd:complexType>
                                                      </xsd:element>
                                                      <xsd:element name="G03iTrackLengthPower" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText"></xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:complexType>
                                                            <xsd:sequence>
                                                               <xsd:element name="G03i01_TrackLightingLength" type="lit:TrackLightingLength" >
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Linear ft of track or busway</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="G03i01LengthAfterLabel" minOccurs="0">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">X</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="G03i02_WattsPerLinearFootDefault" type="lit:WattsPerLinearFootDefault"  fixed="30">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Default W/LF  [d:line1/] 30</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">Auto-filled result = 30 W/LF</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="G03i02LengthAfterLabel" minOccurs="0">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">=</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="G03i03_TrackLengthPowerCalculated" type="lit:TrackLengthPowerCalculated" >
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Total Watts</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">Calculated Field:  result = PRODUCT ( G03i01_TrackLightingLength, G03i02_WattsPerLinearFootDefault)</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                            </xsd:sequence>
                                                         </xsd:complexType>
                                                      </xsd:element>
                                                   </xsd:choice>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:element name="G03ii01_TrackLightingCurrentLimiterPower" type="lit:TrackLightingCurrentLimiterPower" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">VA of current limiter</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="G03iiiCalculationMethod" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"></xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">If G03iii_IsTrackLightingMethodOvercurrentPanels == true this subsection is displayed for the user to complete</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="G03iii01_TrackLightingBranchCircuitPower" type="lit:TrackLightingBranchCircuitPower" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Voltage of branch circuit</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="G03iii01AfterLabel" minOccurs="0">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">X</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="G03iii02_TrackOvercurrentPanelsTotalPower" type="lit:TrackOvercurrentPanelsTotalPower" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Sum of Ampere ratings for all devices on panel</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:element name="G03iv01_TrackLightingPowerSourceRatedWattage" type="lit:TrackLightingPowerSourceRatedWattage" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Maximum rated input wattage per manufacturer</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                          </xsd:choice>
                                       </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="G04_TrackLightingWattsCalculated" type="lit:TrackLightingWattsCalculated" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Track Wattage</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated value: If G03i_IsTrackLightingMethodInstalledLuminaire == true  then result = Largest (G03i03_LumnairePowerByType, G03i03_TrackLengthPowerCalculated);  Else If G03ii_IsTrackLightingMethodCurrentLimiter == true, result =G03ii01_TrackLightingCurrentLimiterPower;  Else If G03iii_IsTrackLightingMethodOvercurrentPanels == true,  result =  PRODUCT (G03iii01_TrackLightingBranchCircuitPower, G03iii02_TrackOvercurrentPanelsTotalPower); Else If G03iv_IsTrackLightingMethodPowerSource == true result = G03iv01_TrackLightingPowerSourceRatedWattage</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="GEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]1[/d:sup]FOOTNOTE: For power-over-Ethernet lighting systems, power provided to installed non-lighting devices may be subtracted from the total power rating of the power-over-Ethernet system..[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="GErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_H" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]H. INDOOR LIGHTING CONTROLS (Not including PAFs)[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section applies If Bnew01_LightingProjectScope Or Bpark01_LightingProjectScope != NotApplicableLightingScope Or at least one instance of Baltered02_LightingPrescriptiveMethodNR Or Baltered04_LightingPrescriptiveMethodNR != RatedPowerReduction</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="H_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table includes lighting controls for conditioned and unconditioned spaces. When an control having a  * is shown, the notes section of this table provides more detail on how compliance is achieved.  The lighting controls section of the Compliance Summary Table on the first page will show "DOES NOT COMPLY" if the notes are left blank. [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="H_ColumnHeading01to03" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Building Level Controls[/d:b]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="H_ColumnHeading04to12" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Area Level Controls[/d:b]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="H01_MandatoryLightingDemandResponseNR" type="comp:MandatoryLightingDemandResponseNR" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Mandatory Demand Response [d:t24ref/]110.12(c)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User selects from dropdown list: RequiredGT10000SF display term Required greater than 10,000 SF, NotRequiredLT10000SF display term Not Required less than 10,000 SF, NotRequiredInstallingAsPAF display term Not Required - Installing as PAF, NotRequiredLTE0_5WperSF display term  Not Required- Building less than or equal to 0.5W/SF, NA_LT80PctAllowedLP display term NA: less than or equal to 80 % LP (alt only)  If result == NA_LT80PctAllowedLP then If Bnew01 and Bpark01 LightingProjectScopeNR == NotApplicableLightingScope And Baltered01 LightingProjectScopeNR == AlteredLightingSystem And C08 GT PRODUCT (SUM (C01, C02), 0.8)  D03  ExceptionalConditionMessageNRCCLTIE = LP80PercentAlterationIssueInH</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="H02_ShutOffControlsBuilding" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Shut-off controls [d:t24ref/]130.1(c)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User selects from dropdown list: WholeBuildingAutoTimeSwitch display term Whole Building Timeswitch, OtherShutOffControl display term Whole Building Other*, AreaLevelControls display term See Area/Space Level Controls</xsd:documentation>
                        <xsd:appinfo source="displayterm">
                           <dtyp:displayterm value="OtherShutOffControl"> Whole Building Other*</dtyp:displayterm>
                        </xsd:appinfo>
                        
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="comp:ShutOffControlsBuilding">
                           <xsd:enumeration value="AreaLevelControls"/>
                           <xsd:enumeration value="OtherShutOffControl"/>
                           <xsd:enumeration value="WholeBuildingAutoTimeSwitch"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="H03_VerifedByInspector" type="comp:VerifedByInspector"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Field Inspector</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This is for the printed document where the person inspecting can record result.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableLightingControls" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="H04_SpaceDescription" type="com:SpaceDescription" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area Description</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="H05_WholeBuildingOccupancy" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Complete Building or Area Category Primary Function Area</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User selects from dropdown list which is loaded as follows:  If  a project has Bnew02_LightingPrescriptiveMethodNR Or Bpark02_LightingPrescriptiveMethodNR Or Baltered02_LightingPrescriptiveMethodNR ==  CompleteBuilding Then  WholeBuildingOccupancy values are loaded;  Else If  a project has Bnew02_LightingPrescriptiveMethodNR Or  Baltered02_LightingPrescriptiveMethodNR ==  AreaCategory Or  AreaCategoryTailored Then  SpaceFunction values are loaded.  If both conditions are true, then both  WholeBuildingOccupancy values and SpaceFunction values are loaded.</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="com:WholeBuildingOccupancy">
                                                <xsd:enumeration value="AllOthersBuildings"/>
                                                <xsd:enumeration value="AuditoriumBuilding"/>
                                                <xsd:enumeration value="ClassroomBuilding"/>
                                                <xsd:enumeration value="CommercialIndustrialBuilding"/>
                                                <xsd:enumeration value="CommercialIndustrialStorageBuilding"/>
                                                <xsd:enumeration value="ConventionCenterBuilding"/>
                                                <xsd:enumeration value="FinancialInstitutionBuilding"/>
                                                <xsd:enumeration value="GroceryStoreBuilding"/>
                                                <xsd:enumeration value="LibraryBuilding"/>
                                                <xsd:enumeration value="MedicalClinicBuilding"/>
                                                <xsd:enumeration value="OfficeBuilding"/>
                                                <xsd:enumeration value="ParkingGarageBuilding"/>
                                                <xsd:enumeration value="ReligiousFacilityBuilding"/>
                                                <xsd:enumeration value="RestaurantBuilding"/>
                                                <xsd:enumeration value="SchoolBuilding"/>
                                                <xsd:enumeration value="TheaterBuilding"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                       <xsd:element name="H05_SpaceFunction" type="com:SpaceFunction" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Complete Building or Area Category Primary Function Area</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User selects from dropdown list which is loaded as follows:  If  a project has Bnew02_LightingPrescriptiveMethodNR Or Bpark02_LightingPrescriptiveMethodNR Or Baltered02_LightingPrescriptiveMethodNR ==  CompleteBuilding Then  WholeBuildingOccupancy values are loaded;  If  a project has Bnew02_LightingPrescriptiveMethodNR Or  Baltered02_LightingPrescriptiveMethodNR ==  AreaCategory Or  AreaCategoryTailored Then  SpaceFunction values are loaded.  If both conditions are true, then both  WholeBuildingOccupancy values and SpaceFunction values are loaded.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="H05_NotApplicableMessage" type="comp:NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Complete Building or Area Category Primary Function Area</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If  values for  Bnew02_LightingPrescriptiveMethodNR Or Bnew04_LightingPrescriptiveMethodNR == Tailored And  If  All values for  Baltered02_LightingPrescriptiveMethodNR  Or Baltered04_LightingPrescriptiveMethodNR ==  Tailored     result = Not Applicable stored in the NotApplicableMessage</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="H06_AreaControlsCompliance" type="comp:AreaControlsCompliance" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area Controls [d:t24ref/]130.1(a)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from dropdown list.  If selection has an asterisk, the user must provide a note in H_Exception02_ExceptionalConditionNote for this area explaining how compliance is achieved with this selection.</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="AnnunciatedAreaControl">Annunciated*</dtyp:displayterm>
                                             <dtyp:displayterm value="AuthorizedPersonnelAreaControl">Auth. Personnel </dtyp:displayterm>
                                             <dtyp:displayterm value="AreaControlsExempt">Exempt*</dtyp:displayterm>
                                             <dtyp:displayterm value="AreaControlsOther">Other*</dtyp:displayterm>
                                          </xsd:appinfo>
                                          
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="H07_MultilevelControls" type="comp:MultilevelControls" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Multi-Level Controls [d:t24ref/]130.1(b)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from dropdown list.  If selection has an asterisk, the user must provide a note in H_Exception02_ExceptionalConditionNote for this area explaining how compliance is achieved with this selection.  If result == NA_LT80PctAllowedLP then If Bnew01 and Bpark01 LightingProjectScopeNR == NotApplicableLightingScope And Baltered01 LightingProjectScopeNR == AlteredLightingSystem And C08 GT PRODUCT (SUM (C01, C02), 0.8)  D03  ExceptionalConditionMessageNRCCLTIE = LP80PercentAlterationIssueInH</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="MultilevelControlsExempt">Exempt*</dtyp:displayterm>
                                             <dtyp:displayterm value="MultilevelControlsOther">Other*</dtyp:displayterm>
                                          </xsd:appinfo>
                                          
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="H08_ShutOffControlsArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Shut-Off Controls [d:t24ref/]130.1(c)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from dropdown list.  If selection has an asterisk, the user must provide a note in H_Exception02_ExceptionalConditionNote for this area explaining how compliance is achieved with this selection.</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="PartialOffShutOffControl">Partial Off*</dtyp:displayterm>
                                             <dtyp:displayterm value="PartialOnShutOffControl">Partial On*</dtyp:displayterm>
                                             <dtyp:displayterm value="ExemptShutOffControl">Exempt*</dtyp:displayterm>
                                          </xsd:appinfo>
                                          
                                       </xsd:annotation>
