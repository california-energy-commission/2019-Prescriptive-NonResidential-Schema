                                                         <xsd:enumeration value="CivicMeetingPlaceArea"/>
                                                         <xsd:enumeration value="ClassroomLectureTrainingVocationalArea"/>
                                                         <xsd:enumeration value="CommercialIndustrialStorageShipping"/>
                                                         <xsd:enumeration value="CommercialIndustrialWarehouse"/>
                                                         <xsd:enumeration value="ConventionConferenceMultipurposeMeetingCenterArea"/>
                                                         <xsd:enumeration value="CopyRoom"/>
                                                         <xsd:enumeration value="CorridorArea"/>
                                                         <xsd:enumeration value="ElectricalMechancialTelephoneRoom"/>
                                                         <xsd:enumeration value="ExerciseCenterGymnasiumArea"/>
                                                         <xsd:enumeration value="ExhibitMuseumArea"/>
                                                         <xsd:enumeration value="FamilyDiningArea"/>
                                                         <xsd:enumeration value="FastfoodDining"/>
                                                         <xsd:enumeration value="FinancialTransactionsArea"/>
                                                         <xsd:enumeration value="GeneralCommercialIndustrialWorkAreaHighBay"/>
                                                         <xsd:enumeration value="GeneralCommercialIndustrialWorkAreaLowBay"/>
                                                         <xsd:enumeration value="GeneralCommercialIndustrialWorkAreaPrecision"/>
                                                         <xsd:enumeration value="GrocerySalesArea"/>
                                                         <xsd:enumeration value="HospitalExamTreatment"/>
                                                         <xsd:enumeration value="HospitalImaging"/>
                                                         <xsd:enumeration value="HospitalMedicalSupply"/>
                                                         <xsd:enumeration value="HospitalNursery"/>
                                                         <xsd:enumeration value="HospitalNurseStation"/>
                                                         <xsd:enumeration value="HospitalOperatingRoom"/>
                                                         <xsd:enumeration value="HospitalPatientRoom"/>
                                                         <xsd:enumeration value="HospitalPhysicalTherapy"/>
                                                         <xsd:enumeration value="HospitalRecovery"/>
                                                         <xsd:enumeration value="HotelFunctionArea"/>
                                                         <xsd:enumeration value="KitchenCommercialFoodPreparation"/>
                                                         <xsd:enumeration value="LaboratoryScientific"/>
                                                         <xsd:enumeration value="Laundry"/>
                                                         <xsd:enumeration value="LibraryReadingArea"/>
                                                         <xsd:enumeration value="LibraryStacks"/>
                                                         <xsd:enumeration value="LobbyMainEntry"/>
                                                         <xsd:enumeration value="LockerDressingRoom"/>
                                                         <xsd:enumeration value="LoungeBreakRoom"/>
                                                         <xsd:enumeration value="LowVisionCorridorArea"/>
                                                         <xsd:enumeration value="LowVisionDiningArea"/>
                                                         <xsd:enumeration value="LowVisionLobbyMainEntry"/>
                                                         <xsd:enumeration value="LowVisionMultipurposeRoom"/>
                                                         <xsd:enumeration value="LowVisionReligiousWorshipArea"/>
                                                         <xsd:enumeration value="LowVisionRestroom"/>
                                                         <xsd:enumeration value="LowVisionStairwell"/>
                                                         <xsd:enumeration value="LowVisionWaitingArea"/>
                                                         <xsd:enumeration value="MuseumRestoration"/>
                                                         <xsd:enumeration value="Office250FtOrLess"/>
                                                         <xsd:enumeration value="OfficeGreaterThan250Ft"/>
                                                         <xsd:enumeration value="OfficeOpenPlan"/>
                                                         <xsd:enumeration value="ParkingGarageAreaDaylightAdaptationZone"/>
                                                         <xsd:enumeration value="ParkingGarageAreaDedicatedRamp"/>
                                                         <xsd:enumeration value="ParkingGarageBuildingParkingArea"/>
                                                         <xsd:enumeration value="PharmacyArea"/>
                                                         <xsd:enumeration value="ReligiousWorshipArea"/>
                                                         <xsd:enumeration value="Restroom"/>
                                                         <xsd:enumeration value="RetailFittingRoom"/>
                                                         <xsd:enumeration value="RetailMallConcourse"/>
                                                         <xsd:enumeration value="RetailSalesArea"/>
                                                         <xsd:enumeration value="SportsArenaSeatsGTE5000ClassI"/>
                                                         <xsd:enumeration value="SportsArenaSeatsLimitedClassIV"/>
                                                         <xsd:enumeration value="SportsArenaSeatsLT2000ClassIII"/>
                                                         <xsd:enumeration value="SportsArenaSeatsLT5000ClassII"/>
                                                         <xsd:enumeration value="Stairwell"/>
                                                         <xsd:enumeration value="TheaterMotionPicture"/>
                                                         <xsd:enumeration value="TheaterPerformance"/>
                                                         <xsd:enumeration value="TransportationFunctionConcourseBaggage"/>
                                                         <xsd:enumeration value="TransportationFunctionTicketing"/>
                                                         <xsd:enumeration value="VideoConferencingStudio"/>
