                                                <xsd:enumeration value="SportsArenaSeatsLT5000ClassII"/>
                                                <xsd:enumeration value="Stairwell"/>
                                                <xsd:enumeration value="TheaterMotionPicture"/>
                                                <xsd:enumeration value="TheaterPerformance"/>
                                                <xsd:enumeration value="TransportationFunctionConcourseBaggage"/>
                                                <xsd:enumeration value="TransportationFunctionTicketing"/>
                                                <xsd:enumeration value="VideoConferencingStudio"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="R03_SpaceFloorArea" type="com:SpaceFloorArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area (ft[d:sup]2[/d:sup])</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User Input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="R04_AllowedLPD" type="comp:AllowedLPD" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Allowed Density (W/ft[d:sup]2[/d:sup])</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto-filled with allowed LPD for  building or space type value in R02. If R02_WholeBuilding NotNull  Switch ( R02_WholeBuildingOccupancy)   case AssemblyBuilding:  case LibraryBuilding: case ReligiousFacilityBuilding: case RestaurantBuilding: case MotionPictureBuilding:  result = 0.70; case FinancialInstitutionBuilding: case GymnasiumBuilding: case OfficeBuilding: case SchoolBuilding: result = 0.65;  case  ManufacturingFacilityBuilding: result = 0.60; case GroceryStoreBuilding: result =0.95; case ParkingGarageBuilding: result = 0.13; case HospitalBuilding: case RetailBuilding: result = 0.90; case SportsArenaBuilding: result =0.75; case PerformingArtsTheaterBuilding: result = 0.8; case AllOthersBuildings: result = 0.4;  Else If R02_SpaceFunction NotNull Switch (R02_SpaceFunction) case AuditoriumArea: case ClassroomLectureTrainingVocationalArea:  case Office250FtOrLess:  result = 0.70; case AutoRepairArea: case BarLoungeFineDiningArea: case HospitalMedicalSupply:  case HospitalPatientRoom:  result = 0.55;  case AudienceSeatingArea: case CommercialIndustrialStorageShipping: case CorridorArea: case ExhibitMuseumArea: case  GeneralCommercialIndustrialWorkAreaLowBay:  case OfficeOpenPlan: case RetailFittingRoom: case TheaterMotionPicture:  result = 0.6;  case case BeautySalonArea: case FinancialTransactionsArea: case LibraryReadingArea: case LowVisionStairwell: case LowVisionCorridorArea: case LowVisionDiningArea: case LowVisionRestroom:  result = 0.80;  case CivicMeetingPlaceArea:  case RetailSalesArea: case TheaterPerformance: case LaboratoryScientific: case HospitalImaging: case LowVisionReligiousWorshipArea:   result = 1.0;  case CommercialIndustrialWarehouse: case LockerDressingRoom: case Laundry: case TransportationFunctionTicketing:    result = 0.45;  case ConventionConferenceMultipurposeMeetingCenterArea:  case  HotelFunctionArea: case GeneralCommercialIndustrialWorkAreaPrecision: case LobbyMainEntry:  case HospitalPhysicalTherapy: case LowVisionLobbyMainEntry:  result = 0.85;   case CopyRoom: case FamilyDiningArea: case ExerciseCenterGymnasiumArea:  case ParkingGarageAreaDaylightAdaptationZone: case Stairwell: result = 0.50;   case FastfoodDining:  case ElectricalMechancialTelephoneRoom:  case TransportationFunctionConcourseBaggage:  case AllOthersSpace:  result = 0.40;     case MuseumRestoration:  case HospitalNurseStation:  case SportsArenaSeatsLimitedClassIV:  case LowVisionWaitingArea:  result = 0.75;    case GeneralCommercialIndustrialWorkAreaHighBay:  case LoungeBreakRoom:  case OfficeGreaterThan250Ft:  case Restroom:  result = 0.65;  case LibraryStacks:  case PharmacyArea:  case SportsArenaSeatsLT2000ClassIII:  result =1.10;  case SportsArenaSeatsLT5000ClassII:  result = 1.45;  case SportsArenaSeatsGTE5000ClassI:  result = 2.25;  case HospitalExamTreatment:  result = 1.15;  case HospitalOperatingRoom:  result = 1.90;   case RetailMallConcourse:  case HospitalRecovery: case  VideoConferencingStudio:    result = 0.90;  case KitchenCommercialFoodPreparation:  case HospitalNursery:  case ReligiousWorshipArea:  case LowVisionMultipurposeRoom: result = 0.95;   case GrocerySalesArea:   result = 1.05;    case ParkingGarageAreaDedicatedRamp: result = 0.25;   ParkingGarageBuildingParkingArea: result = 0.10;</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="R05_LightingPowerAllowance" type="comp:LightingPowerAllowance" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Allowance (Watts)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated value:  result = PRODUCT (R03_ SpaceFloorArea, R04_AllowedLPD)</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="RTableLuminaires" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="R06_LuminaireName" type="lit:LuminaireName" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Luminaire Name or Item Tag</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input: Dropdown list auto-filled from values for Fc01_LuminaireName  and Fu01_LuminaireName</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="R07_LuminairePower" type="lit:LuminairePower" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Watts per Luminaire</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">autofilled result = Fc05_LuminairePower Or F05_LuminairePower for luminaire selected in R06_LuminaireName</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="R08_LuminaireCountByType" type="lit:LuminaireCountByType" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText"># of Luminaires</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="R09_LuminairePowerByType" type="lit:LuminairePowerByType" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Total Design Watts</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Calculated value:   result = PRODUCT (R07_LuminairePower, R08_LuminaireCountByType)</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="RTotals05_LightingPowerAllowedTotal" type="comp:LightingPowerAllowedTotal" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Total Allowance (Watts) for all Areas:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated value:   result = SUM ALL (R05_LightingPowerAllowance)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="RTotals09_LightingPowerInstalledTotal" type="comp:LightingPowerInstalledTotal" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Total Design Watts for all Areas:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated value:   result = SUM ALL (R09_LuminairePowerByType)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="R09bPercentAllowance_LightingPowerInstalledPowerAllowedRatio" type="comp:LightingPowerInstalledPowerAllowedRatio" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Percent of Indoor Lighting Power Allowance:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated value:   result = RTotals09_LightingPowerInstalledTotal / RTotals05_LightingPowerAllowedTotal. Display of percent = result x 100.  If result GT .80  add LP80PercentAlterationExceededInR to  D03_  ExceptionalConditionMessageNRCCLTIE</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_S" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]DAYLIGHT DESIGN POWER ADJUSTMENT FACTOR (PAF)[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required If in section P Pc08 Or Pu08 == true</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, display only the section FieldText and the statement 'This section does not apply to this project.'</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="S_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table documents clerestories, horizontal slats or light shelves meet the requirements in [d:t24ref/]140.3(d) if a Power Adjustment Factor was claimed on Table P.  These features must be documented on the architectural plans or where appropriate within the construction documents.  This PAF also must be verified in the field with an acceptance test per Table U. [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="S01_PAFCreditType" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Compliance Strategy</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Use selects from dropdown list: Clerestories, Horizontal Slats, Light Shelves</xsd:documentation>
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="comp:PAFCreditType">
                           <xsd:enumeration value="Clerestories"/>
                           <xsd:enumeration value="HorizontalSlats"/>
                           <xsd:enumeration value="LightShelves"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="Clerestories" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Clerestory Fenestration[/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if S01_PAFCreditType == Clerestories</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="S02_ClerestoryLocation" type="comp:ClerestoryLocation" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Installed on East, West or South facade</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="S03_ClerestoryHeadHeight" type="comp:ClerestoryHeadHeight" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Head height [d:gte/]10ft above finished floor</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="S04_ClerestoryGlazingHeight" type="comp:ClerestoryGlazingHeight" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Glazing height [d:gte/]0.10 x head height</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="S05_ClerestoryShading" type="comp:ClerestoryShading" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Operable shading on clerestory is controlled separately from other shading</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes is checked result = true Else If Not Applicable is checked result = false display term Not Applicable</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="HorizontalSlats" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Interior and Exterior Horizontal Slats[/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if S01_PAFCreditType == HorizontalSlats</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="S06_HorizontalSlatsLocation" type="comp:HorizontalSlatsLocation" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Installed on East or West facade with 20-30% WWR and extend the entire height of vertical fenestration</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="S07_HorizontalSlatsMounting" type="comp:HorizontalSlatsMounting" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Exterior slats level or sloped downward from fenestration.  Interior slats level or sloped upward from fenestration.  Slats are permanently mounted and not adjustable</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="S08_HorizontalSlatsProjection" type="comp:HorizontalSlatsProjection" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Slats have a Projection Factor per Table 140.3-D Daylighting Devices and extend beyond each side of the window jamb by a distance [d:gte/] their horizontal projection.  Slats do not need to extend beyond the jamb if they are located entirely within the fenestration rough opening or a fin is located at the jamb and extends vertically the entire height of the jamb and horizontally the entire depth of the projection.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="S09_HorizontalSlatsDistanceFactor" type="comp:HorizontalSlatsDistanceFactor" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Slats have a minimum Distance Factor of 0.3 calculated per [t24ref/]140.3(d)</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes is checked result = true Else If Not Applicable is checked result = false display term Not Applicable</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="S10_HorizontalSlatsVisibleReflectance" type="comp:HorizontalSlatsVisibleReflectance" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Slats have a minimum Visible Reflectance of 0.50 tested in accordance with ASTM E903, and are either opaque or have a maximum Visible Transmittance of 0.03 tested in accordance with ASTM E1175</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="S11_BuildingPlanName" type="comp:BuildingPlanName" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Location of slat design in construction documents:</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">User input string</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="LightShelves" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Interior and Exterior Light Shelves[/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if S01_PAFCreditType == LightShelves</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="S12_LightShelvesInteriorExterior" type="comp:LightShelvesInteriorExterior" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Where there is vertical fenestration area below the light shelf, both interior and exterior light shelves shall be installed.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes is checked result = true Else If Not Applicable is checked result = false display term Not Applicable</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="S13_LightShelvesLocation" type="comp:LightShelvesLocation" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Light shelves installed adjacent to clerestory fenestration on south facade with [d:gt/] 30% WWR and with a head height [d:lte/] 1ft below the finished ceiling.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="S14_LightShelvesMounting" type="comp:LightShelvesMounting" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Exterior shelves level or sloped downward from fenestration.  Interior shelves level or sloped upward from fenestration.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="S15_LightShelvesProjection" type="comp:LightShelvesProjection" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Shelves have a Projection Factor per Table 140.3-D Daylighting Devices and extend beyond each side of the window jamb by a distance [d:gte/] their horizontal projection.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="S16_LightShelvesDistanceFactor" type="comp:LightShelvesDistanceFactor" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Shelves  have a minimum Distance Factor of 0.3 calculated per [t24ref/]140.3(d)</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="S17_LightShelvesVisibleReflectance" type="comp:LightShelvesVisibleReflectance" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Shelves have a minimum Visible Reflectance of 0.50 tested in accordance with ASTM E903, or an exterior light shelf installed [d:gt/] 2ft below the clerestory sill.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="S18_BuildingPlanName" type="comp:BuildingPlanName" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Location of light shelve design in construction documents:</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">User input string</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_T" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]T.  DECLARATION OF REQUIRED CERTIFICATES OF INSTALLATION[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="T_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i] Selections have been made based on information provided in this document. If any selection have been changed by permit applicant, an explanation should be included in Table E. Additional Remarks.  These documents must be provided to the building inspector during construction and can be found online at https://www.energy.ca.gov/title24/2019standards/2019_compliance_documents/Nonresidential_Documents/NRCI/ [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="T01_RequireNRCILti01E" type="comp:RequireNRCILti01E" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Form/Title</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">NRCI-LTI-01-E - Must be submitted for all buildings</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated Boolean value based on user input in previous sections of this form. In all cases result = true display term Yes represented as a selected radio button.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="T01a_VerifedByInspector" type="comp:VerifedByInspector"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Field Inspector</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="T02_RequireNRCILti02E" type="comp:RequireNRCILti02E" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Form/Title</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">NRCI-LTI-02-E- Must be submitted for a lighting control system, or for an Energy Management Control System (EMCS), to be recognized for compliance.</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated Boolean value based on user input in previous sections of this form. By default result = false represented as a selected No radio button. In Section H If any H08_ShutOffControlsArea == EMCS  result = true represented as a selected Yes radio button.   If user changes this value, an explanation is required in Section E Additional Remarks.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="T02a_VerifedByInspector" type="comp:VerifedByInspector"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Field Inspector</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="T03_RequireNRCILti04E" type="comp:RequireNRCILti04E" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Form/Title</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">NRCI-LTI-04-E - Must be submitted for two interlocked systems serving an auditorium, a convention center, a conference room, a multipurpose room or a theater to be recognized for compliance.</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated Boolean value based on user input in previous sections of this form. Before any input from the user the default result = false represented as a selected No radio button. If H11_InterlockedLightingSystems == true result = true represented by a selected Yes radio button;  If user changes the value an explanation is required in Section E Additional Remarks.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="T03a_VerifedByInspector" type="comp:VerifedByInspector"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Field Inspector</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="T04_RequireNRCILti05E" type="comp:RequireNRCILti05E" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Form/Title</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">NRCI-LTI-05-E- Must be submitted for a Power Adjustment Factor (PAF) to be recognized for compliance.</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated Boolean value based on user input in previous sections of this form. By default result = false represented as a selected No radio button. If user changes the value an explanation is required in Section E Additional Remarks.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="T04a_VerifedByInspector" type="comp:VerifedByInspector"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Field Inspector</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="T05_RequireNRCILti06E" type="comp:RequireNRCILti06E" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Form/Title</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">NRCI-LTI-06-E- Must be submitted for additional wattage installed in a video conferencing studio to be recognized for compliance.</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated Boolean value based on user input in previous sections of this form. By default result = false represented as a selected No radio button. If user changes the value an explanation is required in Section E Additional Remarks.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="T05a_VerifedByInspector" type="comp:VerifedByInspector"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Field Inspector</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_U" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]U.  DECLARATION OF REQUIRED CERTIFICATES OF ACCEPTANCE[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="U_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]Selections have been made based on information provided in this document.  If any selection have been changed by the permit applicant, an explanation should be included in Table E. Additional Remarks. These documents must be provided to the building inspector during construction and any with "-A" in the form name must be completed through an Acceptance Test Technician Certification Provider (ATTCP). For more information visit: http://www.energy.ca.gov/title24/attcp/providers.html[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="U01_RequireNRCALti02A" type="comp:RequireNRCALti02A" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Form/Title</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">NRCA-LTI-02-A - Must be submitted for occupancy sensors and automatic time switch controls.</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated Boolean value based on user input in previous sections of this form. By default result = false represented as a selected No radio button.  If H02_ShutOffControlsBuilding == AreaLevelControls  result = false; Else If Q18_ ShutOffControlsBuilding == WholeBuildingAutoTimeSwitch, WholeBuildingEMCS Or WholeBuildingOccupancySensors Or Q18_ ShutOffControlsArea == AutomaticTimerSwitch, EMCS Or OccupancySensor  result = true represented as a selected Yes radio button.   If user changes this value to false, an explanation is required in Section E Additional Remarks.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="U01a_VerifedByInspector" type="comp:VerifedByInspector"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Field Inspector</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="U02_RequireNRCALti03A" type="comp:RequireNRCALti03A" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Form/Title</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">NRCA-LTI-03-A - Must be submitted for automatic daylight controls.</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated Boolean value based on user input in previous sections of this form. By default result = false represented as a selected No radio button. If all values for H09_PrimarySkylitDaylightControlsInclude Or H10_SecondaryDaylightControlsIncluded == Exempt  result = false; Else result = true represented as a selected Yes radio button.  If user changes this value an explanation is required in Section E Additional Remarks.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="U02a_VerifedByInspector" type="comp:VerifedByInspector"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Field Inspector</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="U03_RequireNRCALti04A" type="comp:RequireNRCALti04A" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Form/Title</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">NRCA-LTI-04-A - Must be submitted for demand responsive lighting controls.</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated Boolean value based on user input in previous sections of this form. By default result = false represented as a selected No radio button.. If H01_MandatoryLightingDemandResponseNR  == RequiredGT10000SF  Or H11_ InterlockedLightingSystems == true result = true represented as a selected Yes radio button.  If user changes the value an explanation is required in Section E Additional Remarks.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="U03a_VerifedByInspector" type="comp:VerifedByInspector"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Field Inspector</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="U04_RequireNRCALti05A" type="comp:RequireNRCALti05A" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Form/Title</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">NRCA-LTI-05-A. - Must be submitted for institutional tuning power adjustment factor (PAF)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated Boolean value based on user input in previous sections of this form. By default result = false represented as a selected No radio button.. If Pc02e_ApplyInstitutionalTuningNonDaylit Or Pc02f_ApplyInstitutionalTuningDaylit == true   Or Pu02e_ApplyInstitutionalTuningNonDaylit Or Pu02f_ApplyInstitutionalTuningDaylit == true  result = true represented as a selected Yes radio button. If user changes the value an explanation is required in Section E Additional Remarks.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="U04a_VerifedByInspector" type="comp:VerifedByInspector"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Field Inspector</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="U05_RequireNRCAEnv03F" type="comp:RequireNRCAEnv03F" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Form/Title</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">NRCA-ENV-03-F. - Must be submitted for daylighting design power adjustment factors (PAF)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated Boolean value based on user input in previous sections of this form. By default result = false represented as a selected No radio button. . If Pc02h_ApplyClerestoryFenestrationPAF, Pc02i_ApplyHorizontalSlatsPAF Or Pc02j_ApplyLightShelvesPAF == true Or If Pu02h_ApplyClerestoryFenestrationPAF, Pu02i_ApplyHorizontalSlatsPAF Or Pu02j_ApplyLightShelvesPAF == true result = true represented as a selected Yes radio button.   If user changes the value an explanation is required in Section E Additional Remarks.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="U05a_VerifedByInspector" type="comp:VerifedByInspector"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Field Inspector</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:element name="nRCCLTI01E" type="NRCCLTI01E">
      <xsd:annotation>
         <xsd:documentation>This element contains all of the data and text required to generate the NRCCLTI01E compliance report.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
</xsd:schema>
